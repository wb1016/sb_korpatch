[{"op": "replace", "path": "/scriptConfig/descriptions/findClues", "value": "^green;\ubb3c\uccb4\ub97c \uc870\uc0ac^reset;\ud558\uc5ec ^orange;\ud558\uc774\ub85c\ud2c0 \ub2e8\uc11c^reset;\ub97c \ucc3e\uc73c\uc138\uc694."}, {"op": "replace", "path": "/text", "value": "\uc774\uc81c ^orange;\ud558\uc774\ub85c\ud2c0 \uc720\ubb3c^reset;\uc744 \ucc3e\ub294 \uc791\uc5c5\uc744 \uc2dc\uc791\ud574\uc57c\uaca0\uad6c\ub098. ^orange;\uace0\uc628 \ud56d\uc131^reset; \uc8fc\ubcc0\uc744 \ucc3e\uc544\ubcf4\uac70\ub77c, \uadf8\ub9ac\uace0 \ud30c\ub3c4\uac00 \uce58\ub294 \ubc29\ub300\ud55c ^orange;\ubc14\ub2e4^reset; \ubc11 \ub610\ub294 \uc704\uc5d0\uc11c \ud558\uc774\ub85c\ud2c0 ^orange;\ub2e8\uc11c^white;\ub97c \ucc3e\uc544\ubcf4\ub834.^green; \ub2e8\uc11c\ub97c \ubaa8\ub450 \ucc3e\uc73c\uba74 \uc5ec\uae30\uc5d0 \ub3cc\uc544\uc640^reset; \ubab8\uc744 \ubcf4\uc0b4\ud53c\ub834,"}, {"op": "replace", "path": "/scriptConfig/descriptions/turnIn", "value": "^orange;\ubc29\uc8fc^reset;\uc5d0 \uc788\ub294 ^orange;\uc5d0\uc2a4\ub354^reset;\uc5d0\uac8c\ub85c \ub3cc\uc544\uac00\uc138\uc694."}, {"op": "replace", "path": "/scriptConfig/radioMessages/startAlreadyComplete/text", "value": "\ub108\ub294 \ud558\uc774\ub85c\ud2c0 \uace0\ub300 \uc720\ubb3c\ub4e4\uc758 \uc218\uc9d1\uac00\ub85c\uad6c\ub098! \ubcf4\uc544\ud558\ub2c8 \ub124\uac00 \uc774\ubbf8 \uc720\ubb3c\uc758 \uc704\uce58\ub97c \ucc3e\uc744 \ub9cc\ud55c \ucda9\ubd84\ud55c \ub2e8\uc11c\ub4e4\uc744 \ubaa8\uc558\ub2e8\ub2e4!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/complete/text", "value": "\ud765\ubbf8\ub86d\uad70. \ub300 \uc0ac\ucc30 \ub3c4\uc11c\uad00\uc774\ub77c\ub294 \uacf3\uc740 \ub4e4\uc5b4\ubcf8\uc801\uc774 \uc5c6\uc5b4. \ub354 \uc774\uc0c1\uc758 \uc815\ubcf4\ub294 \ud544\uc694\uc5c6\ub2e8\ub2e4 - ^#71f2f6;\uc900\ube44\uac00 \ub418\uba74 \ubc29\uc8fc\ub85c \uc624\uac70\ub77c^white;, \uc598\uc57c."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/9/text", "value": "\uc774 \ubaa8\ub4e0\uac74 \uc