[{"op": "replace", "path": "/text", "value": "\ub2e4\uc74c\uc740 ^orange;\uc544\ube44\uc548^reset;\uc73c\ub85c \ud558\ub294\uac8c \uc88b\uc744 \uac83 \uac19\ub2e8\ub2e4! \uc544\ub9c8\ub3c4 ^orange;\uc815\uae00^green;\uc774 \ud48d\ubd80\ud55c \ud589\uc131^white;\uc73c\ub85c \uac00\ub294\uac8c \uc88b\uc744\uac70\uc57c - \uc544\ube44\uc548\ub4e4\uc740 \uadf8\ub7f0 \uc7a5\uc18c\ub97c \uc88b\uc544\ud55c\ub2e8\ub2e4. \uadf8 \ud589\uc131\ub4e4\uc740 ^orange;\ubc29\uc0ac\uc131 \ud56d\uc131^white;\uc5d0\uc11c \ucc3e\uc744 \uc218 \uc788\ub2e8\ub2e4. \ub098\uc911\uc5d0 \ubcf4\uc790, \uc598\uc57c. \uc870\uc2ec\ud558\uace0, \uc990\uac70\uc6b4 \ud0d0\ud5d8\uc774 \ub418\uac70\ub77c!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/complete/text", "value": "\uc704\ub300\ud55c \uad70\uc8fc\uc758 \uc2e0\uc804\uc774\ub77c... \uadf8\uacf3\uc5d0 \uc788\uc744\uac70\uc57c! \uc774\uc81c \uc6b0\ub9ac\uac00 \ud544\uc694\ud55c \uc815\ubcf4\ub294 \ubaa8\ub450 \ubaa8\uc600\ub2e8\ub2e4. ^#71f2f6;\uc900\ube44\uac00 \ub418\uba74 \ubc29\uc8fc\ub85c \uc624\uac70\ub77c^white;, \uc598\uc57c."}, {"op": "replace", "path": "/scriptConfig/descriptions/findClues", "value": "^green;\ubb3c\uccb4\ub97c \uc870\uc0ac^reset;\ud558\uc5ec ^orange;\uc544\ube44\uc548 \ub2e8\uc11c^reset;\ub97c \ucc3e\uc73c\uc138\uc694."}, {"op": "replace", "path": "/scriptConfig/descriptions/turnIn", "value": "^orange;\ubc29\uc8fc^reset;\uc5d0 \uc788\ub294 ^orange;\uc5d0\uc2a4\ub354^reset;\uc5d0\uac8c\ub85c \ub3cc\uc544\uac00\uc138\uc694."}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/kluexstatue1/text", "value": "\uc774\uc0c1\ud55c \ub3d9\uc0c1\uc774\uad6c\ub098. \uadf8\ub9ac\uace0 \ubc11\uc5d0 \uc801\ud600\uc788\ub294 \uae00\uc740 \ub3d9\uc0c1\ubcf4\ub2e4\ub3c4 \ub354 \ud638\uae30\uc2ec\uc744 \uc720\ubc1c\ud558\ub294 \uad70. \uc0ac\uc18c\ud55c \ubd80\ubd84\uc5d0 \uc815\ubcf4\uac00 \uc228\uc5b4\uc788\ub294 \ubc95\uc774\ub780\ub2e4, \uc598\uc57c."}, {"op": "replace", "path": "/scriptConfig/radioMessages/startAlreadyComplete/text", "value": "\ub108\ub294 \uc544\ube44\uc548 \uace0\ub300 \uc720\ubb3c\ub4e4\uc758 \uc218\uc9d1\uac00\ub85c\uad6c\ub098! \ubcf4\uc544\ud558\ub2c8 \ub124\uac00 \uc774\ubbf8 \uc720\ubb3c\uc758 \uc704\uce58\ub97c \ucc3e\uc744 \ub9cc\ud55c \ucda9\ubd84\ud55c \ub2e8\uc11c\ub4e4\uc744 \ubaa8\uc558\ub2e8\ub2e4!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/9/text", "value": "\uc774 \ub2e8\uc11c\ub294 \uc228\uc5b4\uc788\ub294 \uc815\ubcf4\uc758 \ud78c\ud2b8\ub85c\uad70."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/8/text", "value": "\uc544\ubb34\ub9ac \uc791\uc740 \uc99d\uac70\ub77c\ub3c4 \uc6b0\uc8fc\uc758 \uac70\ub300\ud55c \ube44\ubc00\uc744 \ud478\ub294 \ub370 \ub3c4\uc6c0\uc774 \ub41c\ub2e8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/7/text", "value": "\uc544\ube44\uc548 \uc0ac\ub78c\ub4e4\uc774 \uba3c \uacfc\uac70\uc758 \uc911\uc694\ud55c \ubb3c\uac74\uc744 \uc5b4\ub514\uc5d0 \ubcf4\uad00\ud574\ub480\uc744\uae4c? \ubbf8\uc548\ud558\ub124, \uc598\uc57c. \uadf8\uc800 \uc0dd\uac01\uc911\uc774\uc5c8\ub2e8\ub2e4!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/6/text", "value": "\uc81c \uc544\ubb34\ub9ac \uc791\uc740 \uac83\ub3c4 \uc5bc\ub9c8\ub098 \ub3c4\uc6c0\uc774 \ub418\ub294\uc9c0 \uc54c\uac8c\ub41c\ub2e4\uba74 \uae5c\uc9dd \ub180\ub784\uac70\ub780\ub2e4!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/5/text", "value": "\ube44\ubc00\uc744 \ubc1d\ud600\ub0bc \uc218\ub85d \uacfc\uac70\uac00 \ubc1d\ud600\uc9c0\ub294 \ubc95\uc774\uc9c0. \uc88b\uc740 \ubc1c\uacac\uc774\ub780\ub2e4, \uc598\uc57c!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/4/text", "value": "\ud6cc\ub96d\ud558\uad6c\ub098, \ub124\uac00 \ucc3e\ub294 \uac83\ub4e4\uc740 \ubaa8\ub450 \uc774\ub860\uc758 \ud615\uc131\uc5d0 \uae30\uc5ec\ub97c \ud558\uace0 \uc788\ub2e8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/3/text", "value": "\uc791\uc740 \ub2e8\uc11c\uc9c0\ub9cc, \uadf8\ub798\ub3c4 \uc758\ubbf8 \uc788\ub294 \ub2e8\uc11c\uc9c0!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/2/text", "value": "\uc544\ube44\uc548 \uc0ac\ub78c\ub4e4\uc740 \uadf8\ub4e4\uc758 \uc2e0\uacfc \ub9e4\uc6b0 \ud765\ubbf8\ub85c\uc6b4 \uad00\uacc4\ub97c \ub9fa\uace0 \uc788\uad70. \uc774 \uc791\uc740 \ub2e8\uc11c\ub4e4\uc774 \ub354 \uac70\ub300\ud55c \uc9c4\uc2e4\uc744 \ucc3e\ub294 \ud78c\ud2b8\uac00 \ub420\uac70\uc57c."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/1/text", "value": "\ud765\ubbf8\ub86d\uad6c\ub098. \uc774\uac78\ub85c \ubd84\uba85 \ub0b4 \uc5f0\uad6c\uc5d0 \uc9c4\uc804\uc774 \uc788\uc744\uac70\ub780\ub2e4."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/0/text", "value": "\ud750\uc74c, \uae30\ubb18\ud558\uad70... \uc774 \uc815\ubcf4\uc640 \ub2e4\ub978 \uc815\ubcf4\ub97c \uac19\uc774 \ubd10\uc57c\uaca0\uc5b4."}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/kluexmap1/text", "value": "\uc774\uac74 \ub9c8\uce58 \uace0\ub300\uc758 \ud589\uc131 \uc9c0\ub3c4 \uac19\uad6c\ub098. \ud639\uc2dc \uc911\uc694\ud55c \uacf3\uc758 \uc704\uce58\ub97c \uc54c\ub824\uc8fc\ub294 \uac83\uc774 \uc544\ub2d0\uc9c0?"}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/kluexstatue2/text", "value": "\ud074\ub8e8\uc5d1\uc2a4 \uc758 \ud754\uce58 \uc54a\uc740 \uae30\ub85d\uc774\uad6c\ub098. \uadf8\ub9ac\uace0 \uadf8 \uc548\uc5d0 \ub4e4\uc5b4\uc788\ub294 \ub0b4\uc6a9\uc740 \uc0dd\uc18c\ud558\uace0 \uc218\ub9ce\uc744 \uac00\ub2a5\uc131\uc744 \ub0b4\uc7ac\ud558\uace0 \uc788\uc5b4!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/kluexmural1/text", "value": "\uc774 \ubcbd\ud654\ub294 \uc704\ub300\ud55c \ubcf4\ubb3c\ub4e4\uc5d0 \ub300\ud574 \uae30\ub85d\ub418\uc5b4 \uc788\uad6c\ub098... \uc544\ube44\uc548\uc758 \uc720\ubb3c\uc774 \uadf8\ub7f0 \uc2dd\uc73c\ub85c \ud45c\ud604\ub418\uc5c8\uc744 \uac00\ub2a5\uc131\uc740 \uc544\uc8fc \ub192\ub2e8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/kluexmural2/text", "value": "\uc774 \uace0\ub300 \ubcbd\ud654\ub294 \uc783\uc5b4\ubc84\ub9b0 \ub9ce\uc740 \uc2e0\ube44\ub97c \ubcf4\uc5ec\uc8fc\ub294\uad6c\ub098."}, {"op": "replace", "path": "/scriptConfig/radioMessages/startWithProgress/text", "value": "\uc544\ubb34\ub798\ub3c4 \ub124\uac00 \uc720\ubb3c\uc758 \ud589\ubc29\uc744 \ucc3e\uc744 \ub2e8\uc11c\uac00 \ud3ec\ud568\ub41c \ubb3c\uac74\uc744 \ubbf8\ub9ac \ucc3e\uc740 \ubaa8\uc591\uc774\uad6c\ub098, \ub124\uac00 \ucc3e\uc544\ub2e4\ub2c8\ub294 \ub3d9\uc548 \ub0b4\uac00 \ubd84\uc11d\ud558\ub3c4\ub85d\ud558\ub9c8!"}, {"op": "replace", "path": "/completionText", "value": "\ub9c8\uc9c0\ub9c9\uc73c\ub85c \ucc3e\uc740 \ub2e8\uc11c\uae4c\uc9c0 \ud569\ud574\uc11c, \uc544\ube44\uc548 \uc720\ubb3c\uc744 \ucc3e\uae30 \uc704\ud55c \ub2e8\uc11c\uac00 \ubaa8\ub450 \ubaa8\uc600\ub2e8\ub2e4! \uace0\ub41c \uc791\uc5c5\uc744 \ud574\uc918\uc11c \uace0\ub9d9\uad6c\ub098."}, {"op": "replace", "path": "/title", "value": "\uc544\ube44\uc548 \uc720\ubb3c\uc758 \ub2e8\uc11c \ucc3e\uae30"}]