[{"op": "replace", "path": "/scriptConfig/descriptions/findClues", "value": "^green;\ubb3c\uccb4\ub97c \uc870\uc0ac^reset;\ud558\uc5ec ^orange;\uae00\ub9ac\uce58 \ub2e8\uc11c^reset;\ub97c \ucc3e\uc73c\uc138\uc694."}, {"op": "replace", "path": "/scriptConfig/descriptions/turnIn", "value": "^orange;\ubc29\uc8fc^reset;\uc5d0 \uc788\ub294 ^orange;\uc5d0\uc2a4\ub354^reset;\uc5d0\uac8c\ub85c \ub3cc\uc544\uac00\uc138\uc694."}, {"op": "replace", "path": "/scriptConfig/radioMessages/startAlreadyComplete/text", "value": "\ub108\ub294 \uae00\ub9ac\uce58 \uace0\ub300 \uc720\ubb3c\ub4e4\uc758 \uc218\uc9d1\uac00\ub85c\uad6c\ub098! \ubcf4\uc544\ud558\ub2c8 \ub124\uac00 \uc774\ubbf8 \uc720\ubb3c\uc758 \uc704\uce58\ub97c \ucc3e\uc744 \ub9cc\ud55c \ucda9\ubd84\ud55c \ub2e8\uc11c\ub4e4\uc744 \ubaa8\uc558\ub2e8\ub2e4!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/complete/text", "value": "\ub0b4 \uc774\ub860\uc774 \ub9de\uc558\ub2e8\ub2e4! ^#71f2f6;\uc900\ube44\uac00 \ub418\uba74 \ubc29\uc8fc\ub85c \uc624\uac70\ub77c^white;. \uadf8\ub7ec\uba74 \uc720\ubb3c\uc774 \uc5b4\ub514\uc5d0 \ubcf4\uad00\ub418\uace0 \uc788\ub294\uc9c0 \ub9d0\ud574\uc8fc\uaca0\ub2e8\ub2e4, \uc598\uc57c."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/9/text", "value": "\uc774 \ub2e8\uc11c\ub294 \uc228\uc5b4\uc788\ub294 \uc815\ubcf4\uc758 \ud78c\ud2b8\ub85c\uad70."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/8/text", "value": "\uc544\ubb34\ub9ac \uc791\uc740 \uc99d\uac70\ub77c\ub3c4 \uc6b0\uc8fc\uc758 \uac70\ub300\ud55c \ube44\ubc00\uc744 \ud478\ub294 \ub370 \ub3c4\uc6c0\uc774 \ub41c\ub2e8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/7/text", "value": "\uc0c1\ub2f9\ud788 \ub3c5\ud2b9\ud558\uad6c\ub098! \ud558\uc9c0\ub9cc \ub354 \ub180\ub77c\uc6b4 \uc0ac\uc2e4\uc740 \uc6b0\ub9ac\uac00 \uc54c\uc544\ub0b8 \uc815\ubcf4\ubcf4\ub2e4 \ub354\uc6b1 \uac00\uce58\uc788\ub294 \ubb3c\uac74 \uc77c \uc218\ub3c4 \uc788\uc5b4..."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/6/text", "value": "\uc774 \ub2e8\uc11c\ub294 \ub0b4\uac00 \ucd94\uce21\ud558\uace0 \uc788\uc5c8\ub358 \uac8c \ub9de\ub2e4\ub294\uac78 \uc99d\uba85\ud574\uc8fc\ub294\uad70!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/5/text", "value": "\ud760, \ub0b4 \uc0dd\uac01\uc5d4..."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/4/text", "value": "\ud6cc\ub96d\ud558\uad6c\ub098. \ub124\uac00 \ucc3e\ub294 \uac83\ub4e4\uc740 \ubaa8\ub450 \uc774\ub860\uc758 \ud615\uc131\uc5d0 \uae30\uc5ec\ub97c \ud558\uace0 \uc788\ub2e8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/3/text", "value": "\uc791\uc740 \ub2e8\uc11c\uc9c0\ub9cc, \uadf8\ub798\ub3c4 \uc758\ubbf8 \uc788\ub294 \ub2e8\uc11c\uc9c0!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/2/text", "value": "\uacc4\uc18d\ud558\ub834, \uc598\uc57c. \uc774 \ubaa8\ub4e0\uac74 \uc4f8\ubaa8 \uc788\ub2e8\ub2e4!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/1/text", "value": "\ud765\ubbf8\ub86d\uad6c\ub098. \ubd84\uba85 \uc774\uac78\ub85c \ub0b4 \uc5f0\uad6c\uc5d0 \uc9c4\uc804\uc774 \uc788\uc744\uac70\uc57c."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/0/text", "value": "\ud750\uc74c, \uae30\ubb18\ud558\uad70... \uc774 \uc815\ubcf4\uc640 \ub2e4\ub978 \uc815\ubcf4\ub97c \uac19\uc774 \ubd10\uc57c\uaca0\uc5b4."}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/glitchscribe/text", "value": "\uc774 \ucc45\uc0c1 \uc704\uc5d0 \uc62c\ub824\uc9c4 \ud3b8\uc9c0\uc5d0\ub294 \uc545\uba85 \ub192\uc740 \uae00\ub9ac\uce58\ub97c \uc5b8\uae09\ud558\uace0 \uc788\uad6c\ub098. \ubb34\uc5c7\ubcf4\ub2e4\ub3c4, \uadf8\ub294 \ub9ce\uc740 \uc7ac\ubb3c\ub4e4\uc744 \uc18c\uc720\ud55c \ubd80\uc790\ub85c \ubcf4\uc774\ub294\uad70 ..."}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/glitchdecree/text", "value": "\ubc29\uae08 \ub124\uac00 \uc2a4\uce94\ud55c \ubb3c\uac74\uc740 \ubcf4\ubb3c\uc758 \uc800\uc7a5\uc18c\ub97c \uc5b8\uae09\ud558\uace0 \uc788\ub2e8\ub2e4. \uadc0\uc911\ud488\uc758 \uce90\uc2dc(cache)\ub97c \ubcf4\uad00\ud558\ub294 \uacf3\uc744 \ub9d0\uc774\uc57c. \uc774\uacf3\uc5d0 \uc720\ubb3c\ub9cc\ud07c\uc758 \uac00\uce58\ub97c \uac00\uc9c4 \ubcf4\ubb3c\ub3c4 \uc788\uc744\uae4c?"}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/glitchbaronfigure/text", "value": "\uc774 \uc870\uac01\uc0c1... \ub09c \uc774 \uc0ac\ub78c\uc744 \uc608\uc804\uc5d0 \ubcf8 \uc801\uc774 \uc788\ub2e8\ub2e4! \ucc45\uc5d0\uc11c \ubd24\uc5c8\ub098? \ud55c\ubc88 \uc870\uc0ac\ud574\ubd10\uc57c\uaca0\uad6c\ub098..."}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/glitchchess/text", "value": "\uc774 \uccb4\uc2a4 \ud14c\uc774\ube14\uc740 \uae30\ubb18\ud558\uad6c\ub098. \ub9d0\ub4e4\uc774 \uc190\uc73c\ub85c \uc81c\uc791\ub418\uc5b4\uc788\uc5b4, \uc219\ub828\uc790\uac00 \ub9cc\ub4e0 \uac83\uc73c\ub85c \ubcf4\uc774\ub294\uad6c\ub098. \uc774 \ub514\uc790\uc778\uc740 \uc870\uae08 \ud2b9\uc774\ud558\uad70. \uacfc\uc5f0 \uc774\uac83\uc758 \uae30\uc6d0\uc740 \ubb54\uc9c0 \uad81\uae08\ud574\uc9c0\ub294\ub370."}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/glitchhorndisplay/text", "value": "\ubb38\uc7a5\ud559\uc740 \uae00\ub9ac\uce58\uc5d0\uac8c \uc911\uc694\ud55c \ud559\ubb38\uc774\ub780\ub2e4. \uc774 \ubc30\ub108\uc5d0 \uc788\ub294 \uc778\uc7a5\uc740 \ub3c5\ud2b9\ud558\uace0, \ub098\ub294 \uc774\uac8c \uc720\ubb3c\uacfc \uac04\uc811\uc801\uc73c\ub85c \uad00\ub828\uc774 \uc788\ub2e4\uace0 \ubcf4\uace0\uc788\ub2e8\ub2e4. \ub098\ub294 \uc774\uac83\uc758 \uae30\uc6d0\uc5d0 \ub300\ud574\uc11c \uc5f0\uad6c\ud574 \ubd10\uc57c\uaca0\ub124."}, {"op": "replace", "path": "/scriptConfig/radioMessages/startWithProgress/text", "value": "\uc544\ubb34\ub798\ub3c4 \ub124\uac00 \uc720\ubb3c\uc758 \ud589\ubc29\uc744 \ucc3e\uc744 \ub2e8\uc11c\uac00 \ud3ec\ud568\ub41c \ubb3c\uac74\uc744 \ubbf8\ub9ac \ucc3e\uc740 \ubaa8\uc591\uc774\uad6c\ub098. \ub124\uac00 \ucc3e\uc544\ub2e4\ub2c8\ub294 \ub3d9\uc548 \ub0b4\uac00 \ubd84\uc11d\ud558\ub3c4\ub85d\ud558\ub9c8!"}, {"op": "replace", "path": "/text", "value": "\uc774\uc81c ^orange;\uae00\ub9ac\uce58 \uc720\ubb3c^reset;\uc744 \ucc3e\uc544\uc57c \ud55c\ub2e8\ub2e4. \uae00\ub9ac\uce58\ub294 ^orange;\ud654\uc0b0 \ud65c\ub3d9^reset; \uc774 \ud65c\ubc1c\ud55c \uacf3\uc5d0 \uc8fc\ub85c \uc790\ub9ac \uc7a1\uace0 \uc0b4\uace0\uc788\ub2e8\ub2e4(\uadf8\ub4e4\uc740 \uadf8\uacf3\uc5d0\uc11c \uc870\uae08\uc774\ub098\ub9c8 \uc704\ub85c\uac00 \ub418\ub294 \uacf3\uc744 \ucc3e\uc73c\ub824\ub294 \uac83 \uac19\uad6c\ub098). \ud654\uc0b0 \ud589\uc131\uc740 ^orange;\ubd88\ud0c0\ub294 \ud56d\uc131^reset; \uc8fc\ubcc0\uc5d0\uc11c \ucc3e\uc744 \uc218 \uc788\ub2e8\ub2e4. ^green;\uadf8\uacf3\uc774 ^orange;\ub2e8\uc11c^green;\ub97c \ucc3e\uae30\uc5d0 \uac00\uc7a5 \uc801\ud569\ud560\uac70\uc57c^white;. \ub098\ub294 \uae00\ub9ac\uce58\ub4e4\uc774 \uc880 \ud2b9\uc774\ud558\ub2e4\ub294 \uac83\uc744 \uc54c\uace0\uc788\ub2e8\ub2e4, \ud558\uc9c0\ub9cc \uadf8\ub4e4\uc744 \uc595\ubcf4\uc9c0\ub294 \uc54a\uc9c0; \uc77c\uc774 \ub05d\ub098\uba74 \uc790\uc2e0\uc744 \ubcf4\uc0b4\ud53c\ub834, \uc598\uc57c."}, {"op": "replace", "path": "/completionText", "value": "\uc774 \ub9c8\uc9c0\ub9c9 \ub2e8\uc11c\ub85c \uae00\ub9ac\uce58 \uc720\ubb3c\uc758 \uc704\uce58\ub97c \ud655\uc778\ud588\ub2e8\ub2e4! \uc644\ubcbd\ud574. \ucd95\ud558\ud55c\ub2e8\ub2e4, \uc598\uc57c."}, {"op": "replace", "path": "/title", "value": "\uae00\ub9ac\uce58 \uc720\ubb3c\uc758 \ub2e8\uc11c \ucc3e\uae30"}]