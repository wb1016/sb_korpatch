[{"op": "replace", "path": "/scriptConfig/descriptions/findClues", "value": "^green;\ubb3c\uccb4\ub97c \uc870\uc0ac^reset;\ud558\uc5ec ^orange;\uc5d0\uc774\ud399\uc2a4 \ub2e8\uc11c^reset;\ub97c \ucc3e\uc73c\uc138\uc694."}, {"op": "replace", "path": "/scriptConfig/descriptions/turnIn", "value": "^orange;\ubc29\uc8fc^reset;\uc5d0 \uc788\ub294 ^orange;\uc5d0\uc2a4\ub354^reset;\uc5d0\uac8c\ub85c \ub3cc\uc544\uac00\uc138\uc694."}, {"op": "replace", "path": "/scriptConfig/radioMessages/startAlreadyComplete/text", "value": "\ub108\ub294 \uc5d0\uc774\ud399\uc2a4 \uace0\ub300 \uc720\ubb3c\ub4e4\uc758 \uc218\uc9d1\uac00\ub85c\uad6c\ub098! \ubcf4\uc544\ud558\ub2c8 \ub124\uac00 \uc774\ubbf8 \uc720\ubb3c\uc758 \uc704\uce58\ub97c \ucc3e\uc744 \ub9cc\ud55c \ucda9\ubd84\ud55c \ub2e8\uc11c\ub4e4\uc744 \ubaa8\uc558\ub2e8\ub2e4!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/complete/text", "value": "\uc774\uc81c \uc5d0\uc774\ud399\uc2a4 \uc720\ubb3c\uc774 \uc5b4\ub514\uc5d0 \uc788\ub294\uc9c0 \uc54c \uac83 \uac19\uad6c\ub098!  ^#71f2f6;\uc900\ube44\uac00 \ub418\uba74 \ubc29\uc8fc\uc5d0 \uc788\ub294 \ub0b4\uac8c \uc624\ub834^white; \uc598\uc57c, \ub0b4\uac00 \uc0c1\uc138\ud55c \uc815\ubcf4\ub97c \uc54c\ub824\uc904\ud14c\ub2c8."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/9/text", "value": "\uc774 \ub2e8\uc11c\ub294 \uc228\uc5b4\uc788\ub294 \uc815\ubcf4\uc758 \ud78c\ud2b8\uad70."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/8/text", "value": "\uc544\ubb34\ub9ac \uc791\uc740 \uc99d\uac70\ub77c\ub3c4 \uc6b0\uc8fc\uc758 \uac70\ub300\ud55c \ube44\ubc00\uc744 \ud478\ub294 \ub370 \ub3c4\uc6c0\uc774 \ub41c\ub2e8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/7/text", "value": "\uc0c1\ub2f9\ud788 \ub3c5\ud2b9\ud558\uad6c\ub098! \ud558\uc9c0\ub9cc \ub354 \ub180\ub77c\uc6b4 \uc0ac\uc2e4\uc740 \uc6b0\ub9ac\uac00 \uc54c\uc544\ub0b8 \uc815\ubcf4\ubcf4\ub2e4 \ub354\uc6b1 \uac00\uce58\uc788\ub294 \ubb3c\uac74 \uc77c \uc218\ub3c4 \uc788\uc5b4..."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/6/text", "value": "\uc774 \ub2e8\uc11c\ub294 \ub0b4\uac00 \ucd94\uce21\ud558\uace0 \uc788\uc5c8\ub358 \uac8c \ub9de\ub2e4\ub294\uac78 \uc99d\uba85\ud574\uc8fc\ub294\uad6c\ub098!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/5/text", "value": "\ud760, \ub0b4 \uc0dd\uac01\uc5d4..."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/4/text", "value": "\ud6cc\ub96d\ud558\uad6c\ub098. \ub124\uac00 \ucc3e\ub294 \uac83\ub4e4\uc740 \ubaa8\ub450 \uc774\ub860\uc758 \ud615\uc131\uc5d0 \uae30\uc5ec\ub97c \ud558\uace0 \uc788\ub2e8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/3/text", "value": "\uc791\uc740 \ub2e8\uc11c\uc9c0\ub9cc, \uadf8\ub798\ub3c4 \uc758\ubbf8 \uc788\ub294 \ub2e8\uc11c\uc9c0!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/2/text", "value": "\uacc4\uc18d \ucc3e\uc544\ubcf4\uac70\ub77c \uc598\uc57c, \uc774 \ubaa8\ub4e0\uac74 \ub9e4\uc6b0 \ub3c4\uc6c0\uc774 \ub418\uace0 \uc788\ub2e8\ub2e4!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/1/text", "value": "\ud765\ubbf8\ub86d\uad6c\ub098. \uc774\uac78\ub85c \ubd84\uba85 \ub0b4 \uc5f0\uad6c\uc5d0 \uc9c4\uc804\uc774 \uc788\uc744\uac70\ub780\ub2e4."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/0/text", "value": "\ud750\uc74c, \uae30\ubb18\ud558\uad70... \uc774 \uc815\ubcf4\uc640 \ub2e4\ub978 \uc815\ubcf4\ub97c \uac19\uc774 \ubd10\uc57c\uaca0\uc5b4."}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/bigapedoll/text", "value": "\uc774 \uc778\ud615\uc740 \uc0ac\uc2e4 \ubab0\ub798\uce74\uba54\ub77c\uac00 \uc228\uaca8\uc838 \uc788\ub2e8\ub2e4... \ubbf8\ub2c8\ud06c\ub179\uc740 \ubaa8\ub4e0 \uc5d0\uc774\ud399\uc2a4\uc758 \ubb38\ud654\uc640 \uc0b6\uc744 \uc81c\uc5b4\ud558\ub294\uad70. \ub098\uc911\uc5d0 \uc774 \uce74\uba54\ub77c\uc758 \uc2e0\ud638\ub97c \ucd94\uc801\ud574\ubd10\uc57c\uaca0\uc5b4... \uc774\uac78 \ucc3e\uc544\uc918\uc11c \uace0\ub9d9\ub2e8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/miniknogintel1/text", "value": "\uc774\uac74 \uaf64\ub098 \uc720\uc6a9\ud55c \ubc1c\uacac\uc774\ub780\ub2e4! \ub098\ub294 \uc5ec\uae30\uc5d0 \uc720\ubb3c\uc758 \uc815\ubcf4\uac00 \ub9ce\uc774 \uc228\uc5b4\uc788\ub2e4\uace0 \ud655\uc2e4\ud55c\ub2e8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/bigapetargetmap/text", "value": "\ud760, \uc774 \uc9c0\ub3c4\ub294 \ube45 \uc5d0\uc774\ud504\uc758 \uc704\uce58\ub97c \ucd94\uc815\ud560 \uc218 \uc788\uad70. \ub0b4\uac00 \ubcf4\uae30\uc5d4 \ubbf8\ub2c8\ud06c\ub179\uc740 \uadf8\ub4e4\uc758 \uc911\uc694\ud55c \uc790\uc0b0\uc744 \uc720\uc9c0\ud558\ub824\ub294 \uac83 \ucc98\ub7fc \ubcf4\uc774\ub124. \ub0b4 \uc0dd\uac01\uc5d0 \uc774\uac74 \ub9e4\uc6b0 \uc720\uc6a9\ud55c \ub2e8\uc11c\ub85c \ubcf4\uc778\ub2e8\ub2e4, \uc598\uc57c."}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/miniknognewspaper/text", "value": "\uc774 \uc2e0\ubb38\uc740 \ub9ce\uc740 \uac70\uc9d3 \uc815\ubcf4\uc640 \uc120\uc804\uc73c\ub85c \uac00\ub4dd\ud558\uad70. \ud558\uc9c0\ub9cc \ub0b4 \uc0dd\uac01\uc5d4 \uc774 \uac70\uc9d3\ub9d0 \uc18d\uc5d0 \uc228\uaca8\uc9c4 \uc815\ubcf4\uac00 \uc788\uc744\uac70\ub77c \uc0dd\uac01\ud55c\ub2e8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/minidronerack/text", "value": "\ud6cc\ub96d\ud55c \ubc1c\uacac\uc774\ub780\ub2e4. \uc598\uc57c! \uc774 \ub4dc\ub860\uc758 \ub370\uc774\ud130\ubca0\uc774\uc2a4\ub294 \uc720\uc6a9\ud55c \uc815\ubcf4\uac00 \ub4e4\uc5b4\uc788\uc744\uac70\uc57c."}, {"op": "replace", "path": "/scriptConfig/radioMessages/startWithProgress/text", "value": "\uc544\ubb34\ub798\ub3c4 \ub124\uac00 \uc720\ubb3c\uc758 \ud589\ubc29\uc744 \ucc3e\uc744 \ub2e8\uc11c\uac00 \ud3ec\ud568\ub41c \ubb3c\uac74\uc744 \ubbf8\ub9ac \ucc3e\uc740 \ubaa8\uc591\uc774\uad6c\ub098. \ub124\uac00 \ucc3e\uc544\ub2e4\ub2c8\ub294 \ub3d9\uc548 \ub0b4\uac00 \ubd84\uc11d\ud558\ub3c4\ub85d\ud558\ub9c8!"}, {"op": "replace", "path": "/text", "value": "\ub0b4 \uc0dd\uac01\uc5d0\ub294 \uc774\uc81c ^orange;\uc5d0\uc774\ud399\uc2a4 \uc720\ubb3c^reset;\uc744 \ucc3e\uc544\uc57c \ud560 \uac83 \uac19\uad6c\ub098. \uc5d0\uc774\ud399\uc2a4\ub294 \ubcf4\ud1b5 ^orange;\uc5bc\uc5b4\ubd99\uc740 \ud56d\uc131^reset;\uc8fc\ubcc0\uc758 ^orange;\uc5bc\uc5b4\ubd99\uc740 \ud589\uc131^reset;\uc5d0\uc11c \ucc3e\uc744 \uc218 \uc788\ub2e8\ub2e4. \uadf8\ub7ec\ubbc0\ub85c \uadf8 \uacf3\uc5d0 ^green;\uc6b0\ub9ac\uac00 \ud544\uc694\ud55c ^orange;\ub2e8\uc11c^green;\ub97c \ucc3e\uc744 \uc88b\uc740 \uc7a5\uc18c\uac00 \uc788\uc744\uac70\uc57c^reset;. \uc637\uc744 \ub530\ub73b\ud558\uac8c \uc785\uace0 \uac00\uac70\ub77c, \uc598\uc57c. \uadf8\ub9ac\uace0 \uae30\uc5b5\ud558\ub834: \uc5d0\uc774\ud399\uc2a4\uc758 \uc0ac\ud68c\ub294 \uac11\uc790\uae30 \ubcc0\ud560 \uc218 \uc788\ub2e8\ub2e4, \uadf8\ub7ec\ubbc0\ub85c \ud56d\uc0c1 \ubb34\uc7a5\uc744 \ud558\uace0\ub2e4\ub2c8\ub834."}, {"op": "replace", "path": "/completionText", "value": "\ub124\uac00 \uc5d0\uc774\ud399\uc2a4 \uc720\ubb3c\uc744 \ucc3e\uc744 \ubaa8\ub4e0 \ub2e8\uc11c\ub97c \ucc3e\uc558\ub2e8\ub2e4! \uc774\uac74 \ud6cc\ub96d\ud55c \uc77c\uc774\ub780\ub2e4, \uc598\uc57c. - \uc774\uc81c \uc6b0\ub9ac\uac00 \uc5ec\uae30\uae4c\uc9c0 \uc654\uc5b4."}, {"op": "replace", "path": "/title", "value": "\uc5d0\uc774\ud399\uc2a4 \uc720\ubb3c\uc758 \ub2e8\uc11c \ucc3e\uae30"}]