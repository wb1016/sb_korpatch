[{"op": "replace", "path": "/scriptConfig/radioMessages/startAlreadyComplete/text", "value": "\ub108\ub294 \ud50c\ub85c\ub780 \uace0\ub300 \uc720\ubb3c\ub4e4\uc758 \uc218\uc9d1\uac00\ub85c\uad6c\ub098! \ubcf4\uc544\ud558\ub2c8 \ub124\uac00 \uc774\ubbf8 \uc720\ubb3c\uc758 \uc704\uce58\ub97c \ucc3e\uc744 \ub9cc\ud55c \ucda9\ubd84\ud55c \ub2e8\uc11c\ub4e4\uc744 \ubaa8\uc558\ub2e8\ub2e4!"}, {"op": "replace", "path": "/scriptConfig/descriptions/findClues", "value": "^green;\ubb3c\uccb4\ub97c \uc870\uc0ac^reset;\ud558\uc5ec ^orange;\ud50c\ub85c\ub780 \ub2e8\uc11c^reset;\ub97c \ucc3e\uc73c\uc138\uc694."}, {"op": "replace", "path": "/scriptConfig/descriptions/turnIn", "value": "^orange;\ubc29\uc8fc^reset;\uc5d0 \uc788\ub294 ^orange;\uc5d0\uc2a4\ub354^reset;\uc5d0\uac8c\ub85c \ub3cc\uc544\uac00\uc138\uc694."}, {"op": "replace", "path": "/scriptConfig/radioMessages/complete/text", "value": "\ud6cc\ub96d\ud574. \uc774\uc81c \uc720\ubb3c\uc758 \uc704\uce58\uac00 \uc5b4\ub514\uc778\uc9c0 \uc54c \uac83 \uac19\uad6c\ub098! ^#71f2f6;\uc900\ube44\uac00 \ub418\uba74 \ubc29\uc8fc\ub85c \uc624\uac70\ub77c^white;, \uc598\uc57c."}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/ixolingpike/text", "value": "\uc774 \uc720\uc801\uc744 \uad00\ub9ac\ud558\ub358 \uc0dd\ubb3c\uc740 \uc774 \uc9c0\uc5ed\uc758 \uace0\uc720 \uc0dd\ubb3c\uc774 \uc544\ub2c8\uc57c. \uc544\ubb34\ub798\ub3c4 \ub0b4 \ubd84\ub958 \uc11c\uc801\uc744 \ucc3e\uc544\uc11c \uc774 \uc0dd\ubb3c\uc758 \uae30\uc6d0\uacfc \uc911\uc694\ud55c \uc815\ubcf4\ub97c \ucc3e\uc544\ubd10\uc57c\uaca0\uad6c\ub098."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/9/text", "value": "\ubaa8\ub4e0 \ub2e8\uc11c\ub4e4\uc774 \uc6b0\ub9ac\uc5d0\uac8c \ubaa8\uc774\uace0 \uc788\uc5b4!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/8/text", "value": "\uc0c1\ub2f9\ud788 \ub3c5\ud2b9\ud558\uad6c\ub098! \ud558\uc9c0\ub9cc \ub354 \ub180\ub77c\uc6b4 \uc0ac\uc2e4\uc740 \uc6b0\ub9ac\uac00 \uc54c\uc544\ub0b8 \uc815\ubcf4\ubcf4\ub2e4 \ub354\uc6b1 \uac00\uce58\uc788\ub294 \ubb3c\uac74 \uc77c \uc218\ub3c4 \uc788\uc5b4..."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/7/text", "value": "\ud765\ubbf8\ub85c\uc6b4\uac74 \uadf8\ub4e4\uc774 \uc5b4\ub5a0\ud55c \ubb3c\uac74\uc744 \uc18c\uc911\ud788 \uc5ec\uae30\uace0 \uc788\uace0, \uadf8\uac83\uc744 \uc774\ub7f0 \ubc29\uc2dd\uc73c\ub85c \ubcf4\uc5ec\uc900\ub2e4\ub294 \uac83\uc774\ub780\ub2e4."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/6/text", "value": "\ud765\ubbf8\ub85c\uc6b4 \ubc1c\uacac\uc774\uad6c\ub098! \uacc4\uc18d\ud574, \uc798\ud558\uace0 \uc788\ub2e8\ub2e4!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/5/text", "value": "\uc774 \ub2e8\uc11c\ub294 \ub0b4\uac00 \ucd94\uce21\ud558\uace0 \uc788\uc5c8\ub358 \uac8c \ub9de\ub2e4\ub294\uac78 \uc99d\uba85\ud574\uc8fc\ub294\uad70!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/4/text", "value": "\ud50c\ub85c\ub780\uc740 \ud3ed\ub825 \ubb38\ud654\uac00 \ud615\uc131\ub418\uc5b4\uc788\uc5b4. \uac00\ub054\uc740 \ubb34\uc9c8\uc11c \ud558\uae30\ub3c4\ud588\uace0, \uac00\ub054\uc740 \uc870\uc9c1\uc801\uc73c\ub85c \uc6c0\uc9c1\uc774\uae30\ub3c4 \ud588\ub2e8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/3/text", "value": "\uc791\uc740 \ub2e8\uc11c\uc9c0\ub9cc, \uadf8\ub798\ub3c4 \uc758\ubbf8 \uc788\ub294 \ub2e8\uc11c\uc9c0!"}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/2/text", "value": "\ud50c\ub85c\ub780\uc740 \uc5ed\uc0ac\uc5d0\ub294 \uad00\uc2ec\uc744 \ub450\uc9c0 \uc54a\uc9c0\ub9cc, \uadf8\ub4e4\uc740 \uc804\ud1b5\uc744 \ub9e4\uc6b0 \uc18c\uc911\ud558\uac8c \uc0dd\uac01\ud55c\ub2e8\ub2e4. \uc774\uac74 \ubb38\ud654\ub97c \ucc3d\uc870\ud558\ub294 \ub370 \ub9ce\uc740 \ub3c4\uc6c0\uc774 \ub41c\ub2e8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/1/text", "value": "\ud760, \ub0b4 \uc0dd\uac01\uc5d4..."}, {"op": "replace", "path": "/scriptConfig/radioMessages/genericProgress/0/text", "value": "\ud750\uc74c, \uae30\ubb18\ud558\uad70... \uc774 \uc815\ubcf4\uc640 \ub2e4\ub978 \uc815\ubcf4\ub97c \uac19\uc774 \ubd10\uc57c\uaca0\uc5b4."}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/championspeardisplay/text", "value": "\uc804\ud1b5\uc801\uc778 \uaf43 \ubb38\ud654\uac00 \uadf8\ub4e4\uc758 \uc0ac\ub0e5\uacfc \uc0ac\ub0e5\uafbc\uc5d0\uac8c \uc911\uc694\ud55c \uc601\ud5a5\uc744 \ub07c\ucce4\ub2e4\ub77c... \ubd84\uba85 \uc774 \uc815\ubcf4\uac00 \uadf8\ub4e4\uc758 \uc720\ubb3c \uc704\uce58\ub97c \ucc3e\ub294\ub370 \ub3c4\uc6c0\uc744 \uc904 \uc218 \uc788\uc744\uac70\uc57c."}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/floranspinneret/text", "value": "\uc774 \ubc29\uc801\ub3cc\uae30\ub294 \ube44\uc815\uc0c1\uc801\uc73c\ub85c \ucee4. \ub3c4\ub370\uccb4 \uc5b4\ub5a4 \uc0dd\ubb3c\uc774 \uc774\ub7f0 \uae30\uad00\uc744 \uac16\uc9c0? \uc774\uac74 \ubd84\uba85\ud788 \ub2f9\ucca8\ub41c \ubcf5\uad8c\uc77c\uac70\uc57c. \ub0b4 \uc0dd\uac01\uc5d4..."}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/floranhunterpainting/text", "value": "\uc774 \uadf8\ub9bc\uc740 \ud50c\ub85c\ub780 \uc0ac\ub0ad\uafbc\uc758 \ubaa8\uc2b5\uc744 \uadf8\ub9b0\uac83 \uac19\uc544, \uadf8\ub4e4\uc758 \uc758\uc0c1\uc740 \uc758\uc2dd \uc808\ucc28\uc5d0 \uc785\ub294 \uc637 \ucc98\ub7fc \ubcf4\uc774\ub294\uad6c\ub098."}, {"op": "replace", "path": "/scriptConfig/radioMessages/uniqueProgress/floranwebbing/text", "value": "\uc774 \ucc10\ub4dd\ud55c \uac70\ubbf8\uc904\uc740 \ud2b9\uc774\ud55c \uc0c9\uc870\uc640 \uc9c8\uac10\uc744 \uc0ac\uc6a9\ud588\uad6c\ub098. \uc774 \ubb3c\uac74\uc758 \uc678\ud615\uc740 \uc0dd\uc0b0\ub418\uc5c8\uc744\ub54c \ud2b9\ubcc4\ud55c \ud2b9\uc9d5\uc744 \uac00\uc838\uc57c\ub9cc \ud588\uc744\uac70\uc57c. \uadf8\ub9ac\uace0 \uc544\ub9c8 \ud658\uacbd\uc801 \uc694\uc18c\ub85c \uc778\ud574 \uc81c\ud55c\ub418\uc5b4 \uc788\uc5c8\uaca0\uc9c0."}, {"op": "replace", "path": "/scriptConfig/radioMessages/startWithProgress/text", "value": "\uc544\ubb34\ub798\ub3c4 \ub124\uac00 \uc720\ubb3c\uc758 \ud589\ubc29\uc744 \ucc3e\uc744 \ub2e8\uc11c\uac00 \ud3ec\ud568\ub41c \ubb3c\uac74\uc744 \ubbf8\ub9ac \ucc3e\uc740 \ubaa8\uc591\uc774\uad6c\ub098. \ub124\uac00 \ucc3e\uc544\ub2e4\ub2c8\ub294 \ub3d9\uc548 \ub0b4\uac00 \ubd84\uc11d\ud558\ub3c4\ub85d\ud558\ub9c8!"}, {"op": "replace", "path": "/text", "value": "\uc774\uc81c \ub2c8\uac00 \uc2a4\uce94\uc5d0 \uc775\uc219\ud558\ub2c8, ^green;\uc774\uc81c\ubd80\ud130 \uc6b0\ub9ac\ub294 \uc800 ^orange;\uc720\ubb3c\ub4e4^green;\uc744 \ucc3e\ub294\ub370 \uc9d1\uc911\ud574\uc57c \ud55c\ub2e8\ub2e4,^white; \uc598\uc57c. \uadf8\ub798, \ud50c\ub85c\ub780 \ubd80\ud130 \uc2dc\uc791\ud574\ubcf4\uc790\uafb8\ub098... \ubd84\uba85 \uadf8\ub4e4\uc758 \uc2dd\ubbfc\uc9c0\uc5d0\uc11c \uc720\ubb3c\uc758 \uc704\uce58\ub97c \ucc3e\uc744 ^orange;\ub2e8\uc11c^reset;\uac00 \uc788\uc744\uac70\ub780\ub2e4. \ud50c\ub85c\ub780\uc740 \uc774\uc81c \uc5b4\ub514\ub4e0\uc9c0\uc5d0 \ubd84\ud3ec\ud558\uace0 \uc788\uc9c0\ub9cc, \uc5ec\uc804\ud788 ^orange;\uc232^white;\uc73c\ub85c \ubc00\uc9d1\ud55c \uad6c\uc5ed\uc744 \uc88b\uc544\ud55c\ub2e8\ub2e4. \uc2dd\ubb3c\ub4e4\ub85c \uac00\ub4dd\ud55c \ud589\uc131\uc740 ^orange;\uc628\ud654\ud55c \ud56d\uc131^white;\uc5d0\uc11c \ucc3e\uc744 \uc218 \uc788\uc744\uac70\uc57c.. \ud589\uc6b4\uc744 \ube48\ub2e8\ub2e4 \uc598\uc57c, \uadf8\ub9ac\uace0 \uace0\ub9d9\ub2e8\ub2e4."}, {"op": "replace", "path": "/completionText", "value": "\ub124\uac00 \ud574\ub0c8\ub2e8\ub2e4! \uadf8\uac8c \uc6b0\ub9ac\uac00 \ud544\uc694\ud55c \ub9c8\uc9c0\ub9c9 \ub2e8\uc11c\uc600\ub2e8\ub2e4. \uc774\uc81c \ud50c\ub85c\ub780 \uc720\ubb3c\uc774 \uc5b4\ub514\uc5d0 \uc788\ub294\uc9c0 \uc54c\uaca0\uad6c\ub098."}, {"op": "replace", "path": "/title", "value": "\ud50c\ub85c\ub780 \uc720\ubb3c\uc758 \ub2e8\uc11c \ucc3e\uae30"}]