[{"op": "replace", "path": "/techchallenge_morphball2/text", "value": "\ubcf4\uc544\ud558\ub2c8 ^#71f2f6;\uc810\ud504 \ub3c4\uc911 ^orange;\uc65c\uace1 \uad6c\uccb4 \uae30\uc220^#71f2f6;\uc744 \uc0ac\uc6a9\ud558\uc5ec^white; \uc774 \ud1b5\ub85c\ub85c \ub4e4\uc5b4\uac00\uc57c \ud560 \uac83 \uac19\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/techchallenge_dash2/text", "value": "\uc774 \ubb38\ub4e4\uc740 \ub9e4\uc6b0 \uc7a0\uae50 \uc5f4\ub838\ub2e4 \ub2eb\ud799\ub2c8\ub2e4. ^#71f2f6;\ubb38\uc5d0\uc11c \uac00\uae4c\uc6b4 \uacf3\uc5d0 \ubbf8\ub9ac \uc790\ub9ac\ub97c \uc7a1\uace0, \uc2a4\uc704\uce58\ub97c \uc791\ub3d9\uc2dc\ud0a4\uc790 \ub9c8\uc790 \ub3cc\uc9c4\uc744 \uc0ac\uc6a9\ud574\uc57c\ud569\ub2c8\ub2e4^white;."}, {"op": "replace", "path": "/techchallenge_doublejump1/text", "value": "^orange;\uacf5\uc911 \ub3c4\uc57d^#71f2f6;\uc744 \ud558\ub824\uba74 \uacf5\uc911\uc5d0\uc11c \uc2a4\ud398\uc774\uc2a4 \ubc14\ub97c \ub204\ub974\uc138\uc694^white;. \uacf5\uc911 \ub3c4\uc57d\uc73c\ub85c \uc870\uae08 \ub354 \ub192\uc774 \uc62c\ub77c\uac08 \uc218 \uc788\uc744\uac81\ub2c8\ub2e4."}, {"op": "replace", "path": "/techchallenge_dash3/text", "value": "\uc774 \ubb38\uc740 ^orange;\ub9e4\uc6b0 \uc9e7\uc740 \uc2dc\uac04 \ub3d9\uc548 \uc5f4\ub9bd\ub2c8\ub2e4^white;. \ub2f9\uc2e0\uc774 \uc88c\uc808\uac10\uc744 \uadf9\ubcf5\ud558\uace0 \uc778\ub0b4\uc2ec\uc744 \ud1b5\ud574 \uc131\uacf5\ud558\uba74 \uce6d\ucc2c\ud574\ub4dc\ub9b4\ub9cc\ud55c \uc77c\uc774\ub124\uc694."}, {"op": "replace", "path": "/techchallenge_dash1/text", "value": "\uc774 \uc7a5\uc560\ubb3c \ucf54\uc2a4\ub294 \ub2f9\uc2e0\uc774 \ubc1b\uc740 ^orange;\ub3cc\uc9c4 \uae30\uc220^white;\uc774 \ud544\uc694\ud569\ub2c8\ub2e4. \uc0ac\uc6a9\ud558\ub824\uba74 ^#71f2f6;A \ub610\ub294 D \ub97c \ube60\ub974\uac8c \ub450\ubc88 \ub20c\ub7ec \uc67c\ucabd\uc774\ub098 \uc624\ub978\ucabd\uc73c\ub85c \ub3cc\uc9c4\ud569\ub2c8\ub2e4^white;."}, {"op": "replace", "path": "/techchallenge_doublejump2/text", "value": "\uc81c\uac00 \uce21\uc815\ud574\ubcf8 \uacb0\uacfc, ^#71f2f6;\uc774 \ud50c\ub7ab\ud3fc\uc758 \uac00\uc7a5\uc790\ub9ac \uae4c\uc9c0 \ub3c4\ub2ec \ud6c4 ^orange;\uacf5\uc911 \ub3c4\uc57d^#71f2f6;\uc744 \uc0ac\uc6a9^white;\ud558\uc5ec \uc774\ub3d9\ud574\uc57c \ud560 \uac83 \uac19\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/techchallenge_morphball1/text", "value": "^#71f2f6;F\ub97c \ub204\ub974\uba74 ^orange;\uc65c\uace1 \uad6c\uccb4 \uae30\uc220^white;\uc744 \uc0ac\uc6a9\ud558\uc5ec \uc555\ucd95\ub41c \ud615\ud0dc\ub85c \ub4e4\uc5b4\uac14\ub2e4 \ub098\uc62c \uc218 \uc788\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/techchallenge_dash4/text", "value": "\uc774 \ubc18\uc9dd\uc774\ub294 \ubd88\ube5b\uc740 ^orange;\uadf9\uc800\uc628 \ubc1c\uc0dd\uae30^white;\uc785\ub2c8\ub2e4. \uc774\uac83\ub4e4\uc740 \uce58\uba85\uc801\uc774\uc9c4 \uc54a\uc9c0\ub9cc, \uc811\ucd09\uc2dc \ub2f9\uc2e0\uc758 \uc774\ub3d9\uc18d\ub3c4\ub97c \uc0c1\ub2f9\ud788 \ub0ae\ucdb0\uc904\uac83\uc785\ub2c8\ub2e4."}]