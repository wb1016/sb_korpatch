 },
 {
  "op": "replace", 
  "path": "/hungry/text", 
  "value": "\uC9C0\uAE08 \uB290\uAEF4\uC9C0\uB294 \uD5C8\uC57D\uD568\uC740 \uD5C8\uAE30 \uB54C\uBB38\uC778 \uAC83 \uAC19\uC2B5\uB2C8\uB2E4. \uC2E0\uCCB4\uC758 \uAE30\uB2A5\uC131\uC744 \uC720\uC9C0\uD558\uAE30 \uC704\uD574 \uBB34\uC5B8\uAC00 \uBA39\uC744 \uAC83\uC744 \uCC3E\uC544\uC57C \uD569\uB2C8\uB2E4." 
 },
 {
  "op": "replace", 
  "path": "/starving/text", 
  "value": "\uAD76\uC8FC\uB9AC\uACE0 \uACC4\uC2ED\uB2C8\uB2E4! \uB2F9\uC7A5 \uC74C\uC2DD\uC744 \uAD6C\uD574\uC11C \uC12D\uCDE8\uD558\uB294 \uAC83\uC774 \uAE09\uC120\uBB34\uC785\uB2C8\uB2E4."
 },
 {
  "op": "replace", 
  "path": "/pickuplogblock/text", 
  "value": "\uB098\uBB34\uB97C \uBC1C\uACAC\uD558\uC168\uC2B5\uB2C8\uB2E4. \uC81C \uB370\uC774\uD130\uBCA0\uC774\uC2A4\uC5D0 \uB530\uB974\uBA74 \uB098\uBB34\uB294 \uC544\uC8FC \uC4F0\uC784\uC0C8\uAC00 \uB9CE\uC740 \uBB3C\uC9C8\uC785\uB2C8\uB2E4. \uB354 \uB9CE\uC740 \uB098\uBB34\uB97C \uC8FD\uC5EC\uBC84\uB9AC\uC2DC\uACE0 \uC870\uD569 \uBA54\uB274\uB97C \uC0B4\uD3B4\uBCF4\uC2DC\uAE30\uB97C \uC81C\uC548\uD569\uB2C8\uB2E4."
 },
 {
  "op": "replace", 
  "path": "/pickupinventorstable/text", 
  "value": "\uBC29\uAE08 \uBC1C\uBA85\uAC00\uC758 \uC81C\uC791\uB300\uB97C \uB9CC\uB4DC\uC2E0 \uAC74\uAC00\uC694? \uC0DD\uC874 \uAC00\uB2A5\uC131\uC774 7.2% \uC624\uB978 \uAC83\uC73C\uB85C \uCD94\uC815\uB429\uB2C8\uB2E4."
 },
 {
  "op": "replace", 
  "path": "/pickupseed/text", 
  "value": "\uC528\uC557\uC744 \uBC1C\uACAC\uD558\uC2E0 \uAC83\uC73C\uB85C \uBCF4\uC774\uB294\uAD70\uC694. \uACBD\uC791\uB41C \uD1A0\uC591\uC5D0 \uC2EC\uACE0, \uC528\uC557\uC774 \uC790\uB77C\uB0A0 \uB54C\uAE4C\uC9C0 \uBB3C\uC744 \uC8FC\uC2DC\uB294 \uAC78 \uC81C\uC548\uD569\uB2C8\uB2E4. \uD544\uC694\uD55C \uB3C4\uAD6C\uB4E4\uC740 \uC218\uB835\uC6A9\uD488 \uC81C\uC791\uB300\uC5D0\uC11C \uB9CC\uB4DC\uC2E4 \uC218 \uC788\uC2B5\uB2C8\uB2E4."
 },
 {
  "op": "replace", 
  "path": "/pickupcampfire/text", 
  "value": "\uBD88\uC744 \uBC1C\uACAC\uD558\uC2E0 \uAC83 \uAC19\uAD70\uC694. \uC81C\uAC8C \uC800\uC7A5\uB41C \uB370\uC774\uD130\uC5D0 \uB530\uB974\uBA74 7140\uC5B5\uBC88\uC9F8\uB85C \uBD88\uC744 \uBC1C\uACAC\uD558\uC168\uC2B5\uB2C8\uB2E4. \uC798\uD558\uC168\uC2B5\uB2C8\uB2E4! \uBD88\uC744 \uC0AC\uC6A9\uD574 \uC694\uB9AC\uD558\uC2E4 \uC218 \uC788\uC2B5\uB2C8\uB2E4!"
 },
 {
  "op": "replace", 
  "path": "/pickupflag/text", 
  "value": "\uC774 \uAE43\uBC1C\uB85C \uD45C\uC2DC\uB41C \uC9C0\uC5ED\uC5D0 \uBC14\uB85C \uD154\uB808\uD3EC\uD2B8\uD558\uB3C4\uB85D \uC81C\uAC00 \uD568\uC120\uC758 \uD154\uB808\uD3EC\uD130\uB97C \uC7AC\uC124\uC815\uD560 \uC218 \uC788\uC2B5\uB2C8\uB2E4. \uC124\uCE58\uD558\uC2DC\uACE0 \uAE43\uBC1C\uACFC \uC0C1\uD638\uC791\uC6A9\uD558\uC154\uC11C \uC774\uB984\uC744 \uBD99\uC774\uC138\uC694."
 },
 {
  "op": "replace", 
  "path": "/pickupcoal/text", 
  "value": "\uC11D\uD0C4\uC744 \uBC1C\uACAC\uD558\uC168\uC2B5\uB2C8\uB2E4! \uC11D\uD0C4\uC744 \uD683\uBD88\uB85C \uBC14\uAFB8\uC5B4 \uAE38\uC744 \uBC1D\uD788\uC2E4 \uC218 \uC788\uC2B5\uB2C8\uB2E4. \uC5B4\uB460 \uC18D\uC5D0\uC120 \uCE58\uBA85\uC801\uC778 \uC0AC\uACE0\uC758 \uAC00\uB2A5\uC131\uC774 \uADF9\uC801\uC73C\uB85C \uC99D\uAC00\uD569\uB2C8\uB2E4. \uC870\uD569 \uBA54\uB274\uB97C \uCC3E\uC544\uBCF4\uC138\uC694."
 },
 {
  "op": "replace", 
  "path": "/pickupore/text", 
  "value": "\uAD11\uC11D\uC744 \uBC1C\uACAC\uD558\uC168\uC2B5\uB2C8\uB2E4. \uAD11\uC11D\uC740 \uD654\uB85C\uB97C \uC774\uC6A9\uD574 \uC8FC\uAD34\uB85C \uBC14\uAFC0 \uC218 \uC788\uACE0, \uC8FC\uAD34\uB294 \uB2E4\uC591\uD55C \uC7A5\uBE44\uB97C \uB9CC\uB4DC\uB294 \uB370\uC5D0 \uC720\uC6A9\uD569\uB2C8\uB2E4. \uADF8 \uC7A5\uBE44\uAC00 \uC218\uBA85\uC744 \uB298\uB824 \uC904\uC9C0\uB3C4 \uBAA8\uB974\uC8E0."
 },
 {
  "op": "replace", 
  "path": "/pickupcorefrag/text", 
  "value": "\uD589\uC131 \uD575 \uD30C\uD3B8\uC744 \uBC1C\uACAC\uD558\uC168\uC2B5\uB2C8\uB2E4! \uC4F8\uBAA8\uC788\uB294 \uBB3C\uAC74\uC785\uB2C8\uB2E4. \uC801\uC5B4\uB3C4 20\uAC1C\uB294 \uD544\uC694\uD560 \uAC83\uC73C\uB85C \uCD94\uC815\uB429\uB2C8\uB2E4."
 },
 {
  "op": "replace", 
  "path": "/pickupplantfibre/text", 
  "value": "\uC2DD\uBB3C \uC12C\uC720\uB97C \uBC1C\uACAC\uD558\uC168\uC2B5\uB2C8\uB2E4. \uC774\uAC83\uC73C\uB85C \uBB3C\uB808\uC5D0\uC11C \uC720\uC6A9\uD55C \uC9C1\uBB3C\uC744 \uC9E4 \uC218 \uC788\uC2B5\uB2C8\uB2E4."
 },
 {
  "op": "replace", 
  "path": "/pickupdye/text", 
  "value": "\uC5FC\uB8CC\uB97C \uBC1C\uACAC\uD558\uC168\uC2B5\uB2C8\uB2E4. \uC774\uAC83\uB4E4\uC744 \uAC00\uC9C0\uACE0 \uC758\uBCF5\uC744 \uC88B\uC544\uD558\uB294 \uC0C9\uC0C1\uC73C\uB85C \uC5FC\uC0C9\uD560 \uC218 \uC788\uC2B5\uB2C8\uB2E4. \uC6B0\uD074\uB9AD\uC73C\uB85C \uAC11\uC637\uC774\uB098 \uC637 \uC911 \uD558\uB098\uC5D0 \uC5FC\uB8CC\uB97C \uBC14\uB97C \uC218 \uC788\uC2B5\uB2C8\uB2E4."
 },
 {
  "op": "replace", 
  "path": "/pickupepp/text", 
  "value": "\uD658\uACBD \uBC29\uD638 \uC7A5\uBE44(EPP)\uB97C \uC5BB\uC73C\uC168\uC2B5\uB2C8\uB2E4. \uC774 \uC804\uBB38\uC801\uC778 \uC7A5\uCE58\uB85C \uC0AC\uC6A9\uC790\uAC00 \uB2E4\uB978 \uC0B4\uAE30 \uD798\uB4E0 \uD658\uACBD \uC18D\uC5D0\uC11C \uC0B4\uC544\uB0A8\uC744 \uC218 \uC788\uC2B5\uB2C8\uB2E4. \uB610\uD55C \uD655\uC7A5 \uBD80\uD488\uC73C\uB85C EPP\uB97C \uAC15\uD654\uC2DC\uD0AC \uC218 \uC788\uC2B5\uB2C8\uB2E4"
 },
 {
  "op": "replace", 
  "path": "/pickupaugment/text", 
  "value": "\uD655\uC7A5 \uBD80\uD488\uC744 \uD68D\uB4DD\uD558\uC168\uC2B5\uB2C8\uB2E4. \uC774 \uBD80\uD488\uB4E4\uC740 \uB2E4\uC591\uD55C \uC774\uB85C\uC6B4 \uD6A8\uACFC\uB4E4\uC744 \uC81C\uACF5\uD560 \uC218 \uC788\uC2B5\uB2C8\uB2E4. \uC6B0\uD074\uB9AD\uC73C\uB85C \uD658\uACBD \uBC29\uD638 \uC7A5\uBE44(EPP)\uC5D0 \uC124\uCE58\uD558\uC138\uC694."
 },
 {
  "op": "replace", 
  "path": "/pickupcollar/text", 
  "value": "\uBAAC\uC2A4\uD130\uC6A9 \uBAA9\uAC78\uC774\uB97C \uBC1C\uACAC\uD558\uC168\uC2B5\uB2C8\uB2E4. \uBAA9\uAC78\uC774\uB97C \uAE38\uB4E4\uC5EC\uC9C4 \uBAAC\uC2A4\uD130\uC5D0\uAC8C \uCC44\uC6B0\uBA74, \uB2E4\uC591\uD55C \uC774\uB85C\uC6B4 \uD6A8\uACFC\uB4E4\uC744 \uC81C\uACF5\uD560 \uC218 \uC788\uC2B5\uB2C8\uB2E4. \uC6B0\uD074\uB9AD\uC73C\uB85C \uBAAC\uC2A4\uD130\uAC00 \uB4E4\uC5B4\uC788\uB294 \uD3EC\uD68D \uC7A5\uCE58\uC5D0 \uBAA9\uAC78\uC774\uB97C \uCC44\uC6B8 \uC218 \uC788\uC2B5\uB2C8\uB2E4."
 },
 {
  "op": "replace", 
  "path": "/pickupcapturepod/text", 
  "value": "\uD3EC\uD68D \uC7A5\uCE58\uB97C \uD68D\uB4DD\uD558\uC168\uC2B5\uB2C8\uB2E4. \uBAAC\uC2A4\uD130\uAC00 \uCCB4\uB825\uC774 \uCDA9\uBD84\uD788 \uB0AE\uB2E4\uBA74 \uC774 \uC7A5\uCE58 \uC548\uC5D0 \uD3EC\uD68D\uB420 \uC218 \uC788\uC2B5\uB2C8\uB2E4. \uBAAC\uC2A4\uD130\uB97C \uD5A5\uD574 \uC7A5\uCE58\uB97C \uB358\uC838 \uD3EC\uD68D\uD558\uC138\uC694. \uD3EC\uD68D\uB41C \uBAAC\uC2A4\uD130\uB294 \uAE38\uB4E4\uC5EC\uC9D1\uB2C8\uB2E4."
 },
 {
  "op": "replace", 
  "path": "/pickupbow/text", 
  "value": "\uD65C\uC744 \uD68D\uB4DD\uD558\uC168\uC2B5\uB2C8\uB2E4. \uD65C\uB85C \uBAAC\uC2A4\uD130\uB97C \uC4F0\uB7EC\uD2B8\uB9AC\uB294 \uAC83\uC740 \uACE0\uAE30\uC640 \uD76C\uADC0\uD55C \uC81C\uC791 \uC7AC\uB8CC\uB97C \uC5BB\uC744 \uAC00\uB2A5\uC131\uC744 \uC0C1\uB2F9\uD788 \uB192\uC5EC\uC90D\uB2C8\uB2E4."
 },
 {
  "op": "replace", 
  "path": "/pickupmanipulatormodule/text", 
  "value": "\uBB3C\uC9C8 \uC870\uC791\uAE30 \uBAA8\uB4C8\uC744 \uD68D\uB4DD\uD558\uC168\uC2B5\uB2C8\uB2E4. \uCDA9\uBD84\uD55C \uC218\uC758 \uBAA8\uB4C8\uC73C\uB85C \uC624\uB978\uCABD \uB3C4\uAD6C \uBAA8\uC74C\uC5D0 \uC788\uB294 \uC5D1\uC138\uC2A4 \uD328\uB110\uC5D0\uC11C \uBB3C\uC9C8 \uC870\uC791\uAE30\uB97C \uC5C5\uADF8\uB808\uC774\uB4DC\uD560 \uC218 \uC788\uC2B5\uB2C8\uB2E4."
 },
 {
  "op": "replace", 
  "path": "/naturalcave1/text", 
  "value": "\uB2E4\uC591\uD55C \uC0DD\uBA85 \uC2E0\uD638\uAC00 \uC774 \uAD11\uC0B0 \uC548\uC5D0\uC11C \uAC10\uC9C0\uB418\uACE0 \uC788\uC2B5\uB2C8\uB2E4. \uADF8\uACF3\uC5D0\uC11C \uD589\uC131 \uD575 \uD30C\uD3B8\uC744 \uCC3E\uC544\uBCFC \uC0DD\uAC01\uC774\uC2DC\uB77C\uBA74, \uC6B0\uC120 \uCDA9\uBD84\uD788 \uBB34\uC7A5\uC744 \uAC16\uCD9C \uAC83\uC744 \uAD8C\uD574\uB4DC\uB9BD\uB2C8\uB2E4."
 },
 {
  "op": "replace", 
  "path": "/naturalcave2/text", 
  "value": "\uC790\uC2E0\uC758 \uAC1C\uC778 \uC804\uD22C \uB2A5\uB825\uC5D0 \uCDA9\uBD84\uD788 \uC790\uC2E0\uC774 \uC5C6\uC73C\uC2DC\uB2E4\uBA74, \uD589\uC131 \uD575 \uD30C\uD3B8\uC744 \uCC3E\uC73C\uB7EC \uD589\uC131\uC758 \uC911\uC2EC\uC73C\uB85C \uB354 \uAC00\uAE4C\uC774 \uB545\uC744 \uD30C\uB294 \uAC83\uC774 \uB2E4\uB978 \uD574\uACB0\uCC45\uC77C \uAC81\uB2C8\uB2E4"
