[{"op": "replace", "path": "/biomeairless/text", "value": "\uc774 \ub300\uae30\uac00 \uc5c6\ub294 \ud589\uc131\uc5d0\uc11c \ucc9c\ucc9c\ud788 \uc9c8\uc2dd\uc0ac\ud558\ub294 \uac83\uc740 \uc804\ud600 \ubcf4\ub78c\uc5c6\ub294 \ud589\ub3d9\uc785\ub2c8\ub2e4. \uc81c \uc0dd\uac01\uc5d0\ub294 \uc5ec\uae30\uc11c \ub5a0\ub09c \ub2e4\uc74c, \ud638\ud761 \uc9c0\uc6d0 EPP\ub97c \uc81c\uc791\ud558\uace0 \ub2e4\uc2dc \uc624\ub294 \uac8c \uc88b\uc544 \ubcf4\uc785\ub2c8\ub2e4."}, {"op": "replace", "path": "/techconsole/text", "value": "\uc774 \ubc29\uc5d0 \uc788\ub294 \ucf58\uc194\uc740 \ub2f9\uc2e0\uc774 \uc7a5\ucc29\ud560 \uae30\uc220\uc744 \uad50\uccb4\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4. \ucf58\uc194\uc744 \uc0ac\uc6a9\ud558\uc5ec \uc0c8\ub85c\uc6b4 \uae30\uc220\uc744 \ubc30\uc6b8 \uc218\ub3c4 \uc788\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/biomeheat/text", "value": "\uacbd\uace0! \uc774 \ud589\uc131\uc758 \uc628\ub3c4\uac00 \uce58\uba85\uc801\uc73c\ub85c \ub192\uc2b5\ub2c8\ub2e4!\n\ub354 \uc774\uc0c1\uc758 \ud0d0\ud5d8\uc740 \uc801\uc808\ud55c \ubcf4\ud638 \uc7a5\ube44\uac00 \uc5c6\uc744 \uacbd\uc6b0 \ub9e4\uc6b0 \uc704\ud5d8\ud569\ub2c8\ub2e4!"}, {"op": "replace", "path": "/biomecold/text", "value": "\uacbd\uace0! \uc774 \ud589\uc131\uc758 \uc628\ub3c4\uac00 \uce58\uba85\uc801\uc73c\ub85c \ub0ae\uc2b5\ub2c8\ub2e4!\n\ub354 \uc774\uc0c1\uc758 \ud0d0\ud5d8\uc740 \uc801\uc808\ud55c \ubcf4\ud638 \uc7a5\ube44\uac00 \uc5c6\uc744 \uacbd\uc6b0 \ub9e4\uc6b0 \uc704\ud5d8\ud569\ub2c8\ub2e4!"}, {"op": "replace", "path": "/ancientgateway/text", "value": "\uace0\ub300 \uc720\uc801\uc9c0\ub294 \uc740\ud558\uacc4\uc5d0\uc11c \ub9ce\uc774 \ucc3e\uc544\ubcfc \uc218 \uc788\uc2b5\ub2c8\ub2e4. \ub610\ud55c \uadfc\ucc98\uc5d0 \uacf5\uac04 \uc774\uc0c1\uc774 \ubc1c\uacac\ub418\uc5c8\uc2b5\ub2c8\ub2e4... \uc77c\uc885\uc758 \uc21c\uac04 \uc774\ub3d9 \uc7a5\uce58 \uc544\ub2d0\uae4c\uc694?"}, {"op": "replace", "path": "/erchiussickness/text", "value": "\ub2ec\uc5d0 \ub9e4\uc7a5\ub41c \uc5d0\ub974\ud0a4\uc6b0\uc2a4\uc5d0 \ub9e4\ud639\ub41c \uc0dd\uba85\uccb4\ub97c \uac10\uc9c0\ud588\uc2b5\ub2c8\ub2e4. \ub2f9\uc2e0\uc774 \uc5f0\ub8cc\ub97c \ub354 \ub9ce\uc774 \ucc44\ucde8\ud560\uc218\ub85d \uc774 \uc0dd\uba85\uccb4\uc758 \ub2f9\uc2e0\uc5d0 \ub300\ud55c \uad00\uc2ec \ub610\ud55c \uc99d\uac00\ud560 \uac83\uc785\ub2c8\ub2e4. \uc800\ub294 \uadf8 \uc0dd\uba85\uccb4\ub97c '\uba48\ucd9c \uc218 \uc5c6\uc774 \uce58\uba85\uc801'\uc774\ub77c\uace0 \ud558\uaca0\uc2b5\ub2c8\ub2e4. \ub2f9\uc2e0\uc758 \ub300\ub2f4\ud568\uc5d0 \uacbd\uc758\ub97c \ud45c\ud569\ub2c8\ub2e4."}, {"op": "replace", "path": "/biomeradiation/text", "value": "\uacbd\uace0! \uc774 \ud589\uc131\uc758 \ubc29\uc0ac\uc120 \uc218\uce58\uac00 \uc0c1\ub2f9\ud788 \uc704\ud5d8\ud569\ub2c8\ub2e4!\n\ub354 \uc774\uc0c1\uc758 \ud0d0\ud5d8\uc740 \uc801\uc808\ud55c \ubcf4\ud638 \uc7a5\ube44\uac00 \uc5c6\uc744 \uacbd\uc6b0 \ub9e4\uc6b0 \uc704\ud5d8\ud569\ub2c8\ub2e4!"}]