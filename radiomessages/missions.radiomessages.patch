[{"op": "replace", "path": "/hylotlmission05/text", "value": "\ub370\uc774\ud130\ub97c \ubd84\uc11d\ud574 \ubcf8 \uacb0\uacfc, \uc800\ub294 \uc774 \ubc29\uc5d0 \uc0b4\uc544\uc11c \ub3c4\ub2ec\ud558\ub294 \uac83\uc740 \ubd88\uac00\ub2a5\ud558\ub2e4\uace0 \uacb0\ub860\uc9c0\uc5c8\uc2b5\ub2c8\ub2e4. \ub2f9\uc2e0\uc758 \uadfc\uc131\uacfc \uc778\ub0b4\ub97c \ucd95\ud558\ub4dc\ub9bd\ub2c8\ub2e4!"}, {"op": "replace", "path": "/avianmission04/text", "value": "\ub098\ub294. \ubd88\uc774. \uc2eb\uc5b4."}, {"op": "replace", "path": "/lunarbase06a/text", "value": "\uc27f! \uc790\ub124\ub294 \uc804\ud600 \ub3c4\uc6c0\uc774 \ub418\uc9c8 \uc54a\uace0 \uc788\ub2e4\ub124."}, {"op": "replace", "path": "/avianmission02/text", "value": "\uc774\uc804 \ub370\uc774\ud130\uac00 \ub9d0\ud558\uae38 \uc774 \uace0\ub300 \uc2e0\uc804\uc758 \ubcf4\uc548 \uc2dc\uc2a4\ud15c\uc740 \uad00\ub9ac\uac00 \ub418\uc9c0 \uc54a\uc544 \uac70\uc758 \ub9dd\uac00\uc84c\uc2b5\ub2c8\ub2e4... \ub9e4\uac1c \ubcc0\uc218\ub97c \uc7ac\uce21\uc815\ud558\uace0\uc788\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/avianmission03/text", "value": "\uac08\ub7ab\uae38\uc774 \uac10\uc9c0\ub418\uc5c8\uc2b5\ub2c8\ub2e4. \uc800\ub294 \uc624\ub978\ucabd\uacfc \uc67c\ucabd \ubaa8\ub450\ub97c \uac00\ubcfc\uac83\uc744 \uad8c\ud569\ub2c8\ub2e4. \uc720\uac10\uc2a4\ub7fd\uac8c\ub3c4 \uc774\uac74 \uc120\ud0dd\uc9c0\uac00 \uc544\ub2d9\ub2c8\ub2e4."}, {"op": "replace", "path": "/apexmission04a/text", "value": "\uadfc\uc131\uacfc \uc6a9\uae30\ub97c \uac00\uc9c0\uace0 \ubcf5\uc7a1\ud55c \ubaa9\ud45c\ub97c \ub2ec\uc131\ud558\ub824\ub294 \ub2f9\uc2e0\uc758 \ubaa8\uc2b5\uc5d0 \ubc15\uc218\ub97c \ubcf4\ub0c5\ub2c8\ub2e4."}, {"op": "replace", "path": "/apexmission03/text", "value": "\uc5f0\ub3d9\ub41c \ud68c\ub85c\uc785\ub2c8\ub2e4. \uc774 \ubc29\ubcbd\uc744 \uc5f4\uba74 \ub300\uc751\ud558\ub294 \uc9c0\uc0c1\uc758 \ubc29\ubcbd\ub3c4 \uc5f4\ub9b4 \uac83\uc785\ub2c8\ub2e4."}, {"op": "replace", "path": "/avianmission05/text", "value": "\uc800\uac83\ub4e4\uc744 \ud53c\ud574\uc57c\ub9cc \ud574! \uc800\uac74 \uce7c\ub85c \ucc0c\ub97c \uc288 \uc5c6\uc5b4! \uc800\uac74 \ucd1d\uc73c\ub85c \uc3e0 \uc288 \uc5c6\uc5b4! \uadf8\uc800 \ud53c\ud574 \ub2e4\ub2c8\uace0 \ubc1c\uacac\ub418\uc9c0 \uc54a\uae38 \ube4c\uc5b4\uc57c \ud574."}, {"op": "replace", "path": "/hylotlmission03/text", "value": "\ub2f9\uc2e0\uc740 \uc774\uc81c \ucd5c\ud558\uce35\uc5d0 \uc811\uadfc\ud558\uace0 \uc788\uc2b5\ub2c8\ub2e4. \uce21\uc815 \uc218\uce58\uac00 \uc65c\uace1\ub418\uace0 \uc788\uc9c0\ub9cc \uc6d0\ud558\uc2e0\ub2e4\uba74 \uc704\ud5d8 \uc218\uc900\uc744 \uc54c\ub824\ub4dc\ub9b4 \uc218 \uc788\uc2b5\ub2c8\ub2e4..."}, {"op": "replace", "path": "/lunarbase08/text", "value": "\uc798\ud588\ub2e8\ub2e4!"}, {"op": "replace", "path": "/tentaclemission-destruction1/text", "value": "\ub124\uac00 \ud574\ub0c8\uc5b4! ...\ud558\uc9c0\ub9cc \ubb54\uac00 \uc798\ubabb\ub418\uc5c8\uc5b4. \ud30c\uba78\uc774... \uad6c\uc870\uac00 \ubd88\uc548\uc815\ud574\uc9c0\uace0\uc788\uc5b4!"}, {"op": "replace", "path": "/apexmission01/text", "value": "\uc544\ubb34\ub798\ub3c4 \uc6b0\ub9ac\uac00 \ubc18\uad70\uc758 \uc9c0\uc815\ubd80 \uacf5\uaca9\uc744 \uc6b0\uc5f0\ud788 \ub9de\ub2e5\ub728\ub9b0 \uac83 \uac19\uad6c\ub098. \ub0b4 \uc0dd\uac01\uc5d4 \uc774\uac74 \uc6b0\ub9ac \uc591\uce21 \ubaa8\ub450\uc5d0\uac8c \uc720\ub9ac\ud560 \uac83 \uac19\ub2e8\ub2e4."}, {"op": "replace", "path": "/hylotlmission04/text", "value": "\uc800 \uc5ec\uc790... \uc800 \uc5ec\uc790\uac00 \ub9d0\ud55c \uac8c... \uc774 \ubaa8\ub4e0 \uac8c \ub05d\ub098\uba74, \uc815\ub9d0 \uc0ac\uc2e4\uc774 \ub420\uae4c?"}, {"op": "replace", "path": "/tentaclemission-bottom2/text", "value": "SAIL\uc774 \ub9de\uc2b5\ub2c8\ub2e4. \ub2f9\uc2e0 \ubc1c \ubc11\uc5d0 \ube48 \ud2c8\uc774 \uc788\uc5b4\uc694. \ub2f9\uc2e0\uacfc \uc774 \uc720\uc801\uc758 \uc911\uc2ec \uad6c\uc870\ubb3c \uc0ac\uc774\uc5d0 \ub9e4\uc6b0 \ud070 \ud2c8\uc774 \uc788\uc2b5\ub2c8\ub2e4. \uc544\ub798\ub85c \uac08 \uc218 \uc788\ub294 \uc548\uc804\ud55c \ubc29\ubc95\uc744 \ucc3e\uc544\ub0b4\uc57c \ud560 \uac81\ub2c8\ub2e4."}, {"op": "replace", "path": "/hylotlmission02/text", "value": "\ud604\uc7ac \uc678\ubd80 \uc555\ub825\uc740 15 \ucffc\uc2dc\uc785\ub2c8\ub2e4. \uac80\uc0ac \uacb0\uacfc \uc774 \uac74\ucd95\ubb3c\uc740 80 \ucffc\uc2dc \uc774\uc0c1\uc758 \uc555\ub825\uc744 \ubc84\ud2f8 \uc218 \uc788\uc2b5\ub2c8\ub2e4. \uc0dd\uc874 \ud655\ub960\uc740 \ub180\ub77c\uc6b8 \uc815\ub3c4\ub85c \ub192\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/floranmission105/text", "value": "\uc774 \ubb3c\uc9c8\uc740 \uc720\uae30\uc801\uc785\ub2c8\ub2e4. \ubd84\uc11d\uc744 \ud574\ubd10\uc57c \ud655\uc2e4\ud574\uc9c0\uaca0\uc9c0\ub9cc, \uc774 \uc815\ub3c4\uc758 \ubb3c\uc9c8\uc744 \uc0dd\uc0b0\ud558\ub824\uba74 \uc0c1\ub2f9\ud788 \ud070 \uc0dd\ubb3c\uc774\uc5b4\uc57c \ud560 \uac83\uc785\ub2c8\ub2e4."}, {"op": "replace", "path": "/glitchmission01/text", "value": "\uc774 \uad6c\uc5ed\uc744 \uc9c0\ud0a4\ub294 \uc790\ub294 \"\ub0a8\uc791\"\uc73c\ub85c \uc54c\ub824\uc9c4 \uae00\ub9ac\uce58\ub780\ub2e4. \uadf8\ub294 \uc740\ud1f4\ud55c \uc601\uc6c5\uc774\uc9c0\ub9cc, \ub2e4\ub978 \uc774\ub4e4 \ub9d0\uc5d0 \uc758\ud558\uba74 \uc870\uae08... \uad34\uc9dc\ub77c\uace0 \uc54c\ub824\uc838 \uc788\uc9c0..."}, {"op": "replace", "path": "/floranmission104/text", "value": "\uc774 \ud50c\ub85c\ub780\ub4e4\uc774 \ud558\ub294 \uc77c\uc774 \ubb34\uc5c7\uc778\uc9c0\ub294 \ubab0\ub77c\ub3c4 \uadf8\ub4e4\uc758 \ubb34\ub364\uc740 \ubd84\uba85\ud788 \uc911\uc694\ud560 \uac70\uc57c. \ub098\ubcf4\ub2e4\ub3c4 \ubc8c\ub808\uc7a1\uc774\ub97c \ubabb \ud558\ub294\uad6c\ub098! \uc870\uc2ec\ud574\uc57c \ud55c\ub2e8\ub2e4. \uc598\uc57c."}, {"op": "replace", "path": "/tentaclemission-destruction2/text", "value": "\uace7 \ud3ed\ubc1c\ud560 \uac70\uc57c! \ube68\ub9ac \ub098\uc640!"}, {"op": "replace", "path": "/lunarbase02/text", "value": "\uc804\uc5ed \ubd09\uc1c4\uac00 \ubc1c\ub3d9\ub41c \uac83\uc73c\ub85c \ubcf4\uc785\ub2c8\ub2e4. \uc804\ub825\uc744 \ubcf5\uad6c\ud558\uae30 \uc704\ud574 \ucc28\ub2e8\uae30\ub97c \ucc3e\uc544\uc57c \ud569\ub2c8\ub2e4. \ub0b4\ubd80\uac00 \uc5b4\ub450\uc6b0\ub2c8 \uc2e0\uc911\ud788 \ud589\ub3d9\ud558\uc2dc\uae38 \ubc14\ub78d\ub2c8\ub2e4."}, {"op": "replace", "path": "/lunarbase05/text", "value": "\uc800\ub4e4\uc744 \ubcf4\uac8c\ub098. \uc5d0\ub974\ud0a4\uc6b0\uc2a4 \uacb0\uc815\uc5d0 \uc7a5\uae30\uac04 \ub178\ucd9c\ub418\uc5b4 \uc2ec\ud558\uac8c \ubcc0\ud615\ub418\uc5c8\uad70! \uc774 \ubd88\uc30d\ud55c \uc0ac\ub78c\ub4e4\uc740 \uc790\uae30 \uc758\uc2dd\uc744 \ud1b5\uc81c\ud560 \uc218 \uc5c6\uc5b4."}, {"op": "replace", "path": "/avianmission06/text", "value": "\uc774 \uc7a5\uc18c\ub294 \uc544\ub291\ud55c \ud3b8\uc5d0 \uc18d\ud558\ub294\uad70\uc694... \ubb3c\ub860 \uc800 \uc704\ud5d8\ud55c \uac00\uc2dc\ub97c \ube7c\uba74 \ub9d0\uc774\uc8e0. \uadf8\ub807\uace0\ub9d0\uace0\uc694..."}, {"op": "replace", "path": "/apexmission04/text", "value": "\uc774\uacf3\uc5d0\ub294 \ub610 \ub2e4\ub978 \ubc29\ubcbd\uc774 \ubcc4\ub3c4\uc758 \ubc1c\uc804\uae30\ub85c \uc720\uc9c0\ub418\uace0 \uc788\uc2b5\ub2c8\ub2e4. \ub2f9\uc2e0\uc774 \uc774 \ubcf4\ud638\ub9c9\uc744 \ud574\uc81c\ud558\uace0 \uc804\ub825\uc744 \ucc28\ub2e8\ud558\uc5ec \ubc29\ubcbd\uc744 \ubb34\ub825\ud654\uc2dc\ucf1c\uc57c \ud560 \uac83 \uac19\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/floranmission103/text", "value": "\uc815\ub9d0 \uc0ac\ub098\uc6b4 \ud50c\ub85c\ub780\ub4e4\uc774\uad6c\ub098! \ub0b4 \uc5b4\ub9b0 \uc2dc\uc808\uc774 \uc0dd\uac01\ub098\ub294\uad6c\ub098, \ubb3c\ub860 \ub098\ubb47\uc78e\uc740 \ub35c \ub2ec\ub824 \uc788\uc5c8\uc9c0\ub9cc."}, {"op": "replace", "path": "/apexmission06/text", "value": "\uc774\uac74 \uc815\ub9d0 \uc798 \ub9cc\ub4e4\uc5b4\uc9c4 \uc9c0\uc815\ubd80\uc758 \uc694\uc0c8\ub85c\uad6c\ub098... \ub098\ub294 \ub124\uac00 \uc138\uc2ec\ud558\uace0 \uc2e0\uc911\ud55c \uac78 \uc54c\uace0 \uc788\ub2e8\ub2e4, \uc598\uc57c. \ud558\uc9c0\ub9cc \uc774\ubc88\uc5d0\ub294 \ud2b9\ud788\ub098 \uc870\uc2ec\ud574\uc57c \ud560 \uc2dc\uac04\uc778 \uac83 \uac19\uad6c\ub098."}, {"op": "replace", "path": "/tentaclemission-bossroom2/text", "value": "\ud074\ub8e8\uc5d1\uc2a4\uaed8\uc11c \uc778\ub3c4\ud574 \uc8fc\uc2dc\uae30\ub97c."}, {"op": "replace", "path": "/tentaclemission-arrived2/text", "value": "\ub108\ub294 \ud30c\uba78\uc758 \uadfc\uc6d0 \uc704\uc5d0 \uc11c \uc788\ub2e8\ub2e4, \uc598\uc57c. \ud45c\uba74\uc5d0\uc11c\ub294 \ub9ce\uc740 \ud53c\ud574\ub97c \uc904 \uc218 \uc5c6\uc744 \uac8c\uc57c. \ubc11\uc73c\ub85c \ub0b4\ub824\uac08 \ubc29\ubc95\uc744 \ucc3e\uc544\ubcf4\uac70\ub77c."}, {"op": "replace", "path": "/lunarbase08a/text", "value": "\uc774 \uc815\ub3c4\uc758 \uc5d0\ub974\ud0a4\uc6b0\uc2a4\uba74 \ud568\uc120\uc744 \uc218\ub9ac\ud558\ub294 \ub370\uc5d0\ub294 \ucda9\ubd84\ud560 \uac81\ub2c8\ub2e4."}, {"op": "replace", "path": "/hylotlmission03b/text", "value": "\uac71\uc815\ud558\uc9c0 \ub9c8! \ub0af\uc158 \ub9f9\uc288\ub4e4\uc740 \ub124 \uc8fc\ud2b9\uae30\uc796\uc544!"}, {"op": "replace", "path": "/glitchmission01a/text", "value": "\ub098\uc0ac\uac00 \uc880 \ube60\uc9c4 \uc0ac\ub78c\uc774\uc9c0\ub9cc, \uc704\ud5d8\ud558\uc9c0\ub294 \uc54a\ub2e8\ub2e4."}, {"op": "replace", "path": "/apexmission02a/text", "value": "\uc81c \ud0d0\uc0c9\uae30\uac00 \ub2e4\ub978 \uacbd\ube44\ubcd1\uc744 \uac10\uc9c0\ud588\uc2b5\ub2c8\ub2e4... \uadf8\ub9ac\uace0 \ud55c \uc0ac\ub78c \ub354."}, {"op": "replace", "path": "/floranmission106/text", "value": "\uc870\uc0ac\uc5d0 \ub530\ub974\uba74 \uc774 \uc0dd\ubb3c\ub4e4\uc740 \uc131\uccb4\uac00 \uc544\ub2cc \uac83\uc73c\ub85c \ubcf4\uc785\ub2c8\ub2e4. \uc774\ub4e4\uc740 \ubc29\uae08 \ubd80\ud654\ud588\uace0, \ub2f9\uc2e0\uc740 \uadf8\ub4e4\uc758 \ub465\uc9c0\uc5d0 \ub4e4\uc5b4\uac04 \uac83\uc73c\ub85c \ubcf4\uc785\ub2c8\ub2e4."}, {"op": "replace", "path": "/tentaclemission-arrived/text", "value": "\ud310\ub3c5 \uacb0\uacfc \uc774 \uad6c\uc870\ubb3c\uc740 \ud589\uc131\uc73c\ub85c \ubcf4\uc785\ub2c8\ub2e4. \uc0ac\uc2e4 \uc774 \ud589\uc131\uc740 \ud558\ub098\uc758 \uac70\ub300\ud55c \uc0dd\ubb3c\uc785\ub2c8\ub2e4."}, {"op": "replace", "path": "/apexmission06a/text", "value": "\ube45 \uc5d0\uc774\ud504\ub294 \ubab9\uc2dc \uc704\ud5d8\ud55c \uc801\uc774\ub780\ub2e4."}, {"op": "replace", "path": "/lunarbase01/text", "value": "\uc0dd\uccb4 \uc2e0\ud638\uac00 \uc9c0\uba74 \uc544\ub798\uc5d0\uc11c \ubc1c\uacac\ub418\uc5c8\uc2b5\ub2c8\ub2e4. \uc2e0\ud638 \uac15\ub3c4\ub294... \ucd94\uc815\ud558\uae30 \uc5b4\ub835\uc2b5\ub2c8\ub2e4. \ub2f9\uc2e0\uc758 \uc6a9\uae30\ub97c \ubbf8\ub9ac \uce6d\ucc2c\ub4dc\ub9bd\ub2c8\ub2e4."}, {"op": "replace", "path": "/tentaclemission-bottom/text", "value": "\ub2f9\uc2e0 \uadfc\ucc98\uc5d0 \ub113\uace0 \ub9e4\uc6b0 \uae4a\uc740 \ud2c8\uc774 \ubc1c\uacac\ub418\uc5c8\uc2b5\ub2c8\ub2e4. \uc81c \uc0dd\uac01\uc5d4 \ud2c8\uc0c8\ub97c \ud53c\ud574 \ub2e4\ub2c8\uba70 \ub099\uc0ac\ub97c \ud53c\ud574\uc57c \ud560 \uac83 \uac19\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/lunarbase04/text", "value": "\uc5d0\ub974\ud0a4\uc6b0\uc2a4 \ubc29\uc0ac\uc120 \uc218\uce58\uac00 94 rad\uc5d0 \ub3c4\ub2ec\ud588\uc2b5\ub2c8\ub2e4. \ub2f9\uc2e0\uc740 \ud604\uc7ac \uc704\ud5d8\ud55c \uc0c1\ud669\uc5d0 \uc788\uc2b5\ub2c8\ub2e4. \ub2f9\uc2e0\uc758 \uc704\ud5d8 \uc218\uc900\uc740 14,607 morts \uc785\ub2c8\ub2e4."}, {"op": "replace", "path": "/hylotlmission03a/text", "value": "\uc774 \uad11\uc2e0\ub3c4\ub4e4\uc5d0 \ub300\ud574 \uc5b4\ub514\uc11c \ub4e4\uc740 \uc801\uc774 \uc788\ub294\ub370, \uc870\uae08\ubc16\uc5d0 \ubaa8\ub974\uc9c0... \uad81\uae08\ud574\uc9c0\ub294\ub370...? \ud558\uc9c0\ub9cc \uc774\ub7f0 \uac78 \ub204\uac00 \uc54c\uaca0\ub098."}, {"op": "replace", "path": "/lunarbase07/text", "value": "\uc77c\ubc18\uc801\uc778 \ubb34\uae30\ub85c\ub294 \uc5d0\ub974\ud0a4\uc6b0\uc2a4 \uacb0\uc815\uc744 0%\uc758 \ud655\ub960\ub85c \uc190\uc0c1\uc2dc\ud0ac \uac83\uc785\ub2c8\ub2e4. \uacb0\uc815\uc744 \uacf5\uaca9\ud560 \ub2e4\ub978 \ubc29\ubc95\uc744 \ucc3e\uc544\uc57c \ud569\ub2c8\ub2e4."}, {"op": "replace", "path": "/lunarbase04a/text", "value": "\uc778\uacf5\uc9c0\ub2a5\uc5d0 \ub300\ud574\uc11c\ub294 \ubbf8\uc548\ud558\uad6c\ub098, \uc598\uc57c."}, {"op": "replace", "path": "/avianmission01/text", "value": "\uadf8\uac00 \uc790\ub294 \ub3d9\uc548 \uc774 \uc8fc\ubcc0\uc744 \ub458\ub7ec\ubcf4\ub294\uac8c \uac00\uc7a5 \uc88b\uc744 \uac83 \uac19\uad6c\ub098. \uadf8\ub97c \uac71\uc815\uc2dc\ud0a4\uace0 \uc2f6\uc9c0\ub294 \uc54a\uc744 \ud14c\ub2c8\uae4c."}, {"op": "replace", "path": "/lunarbase06/text", "value": "\uc5d0\ub974\ud0a4\uc6b0\uc2a4 \ubc29\uc0ac\uc120 \uc218\uce58\uac00 101 rad\uc5d0 \ub3c4\ub2ec\ud588\uc2b5\ub2c8\ub2e4. \ub2f9\uc2e0\uc758 \uc704\ud5d8 \uc218\uc900\uc740 18,472 morts \uc785\ub2c8\ub2e4..."}, {"op": "replace", "path": "/tentaclemission-destruction3/text", "value": "\uc7e4\ud55c\ud14c \uba85\ub839\ud558\uc9c0 \ub9c8, \uc0dd\uc120\ub300\uac00\ub9ac! \uc7e4\ub4e4\uc740 \ub354 \uc2ec\ud55c \uaf34\ub3c4 \ub2f9\ud588\ub2e4\uace0!"}, {"op": "replace", "path": "/hylotlmission01/text", "value": "\uc774 \uc12c\uc740 \"\ub300\uc0ac\ucc30 \ub3c4\uc11c\uad00\"\uc758 \uc785\uad6c\uc785\ub2c8\ub2e4."}, {"op": "replace", "path": "/floranmission102/text", "value": "\uc774 \ud130\ub110\uc5d0\uc11c \ub9ce\uc740 \uc0dd\uccb4 \uc2e0\ud638\uac00 \uac10\uc9c0\ub418\uba70, 97%\uac00 \uc801\ub300\uc801\uc785\ub2c8\ub2e4. \ub2f9\uc2e0\uc758 \uc6a9\uae30\ub97c \uce6d\ucc2c\ud574\ub4dc\ub9ac\uc8e0."}, {"op": "replace", "path": "/tentaclemission-bossroom/text", "value": "\uc720\uc801\uc758 \uc2ec\uc7a5\uc5d0 \ub3c4\ucc29\ud588\uad6c\ub098. \uadf8\uac83\uc744 \ubc18\ub4dc\uc2dc \ud30c\uad34\ud574\uc57c \ud55c\ub2e8\ub2e4."}, {"op": "replace", "path": "/lunarbase03/text", "value": "\uc800 \uc0dd\uba85\uccb4\ub294 \uc5d0\ub974\ud0a4\uc6b0\uc2a4 \ubd84\uc790\uc640 \uc778\uac04\uc758 DNA\uac00 \uacb0\ud569\ub418\uc5b4 \uc0dd\uc131\ub41c \uac83 \uac19\uc2b5\ub2c8\ub2e4. \uacf5\uaca9\uc801\uc778 \ud589\ub3d9\uc744 \ucde8\ud560 \ud655\ub960\uc774 \ub192\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/apexmission02/text", "value": "\uc2a4\uce94 \uacb0\uacfc \uc774 \uc2dc\uc124\uc740 \uc911\ubb34\uc7a5\ud55c \uacbd\ube44\ubcd1\uc5d0 \uc758\ud574 \ubc29\uc5b4\ub418\uace0 \uc788\uc73c\uba70, \ubaa8\ub4e0 \uc9c0\uc5ed\uc744 \uc21c\ucc30\ud558\uace0 \uc788\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/tentaclemission-arrived3/text", "value": "\uc870\uc2ec\ud574. \uc774\uacf3\uc5d0 \uc788\ub294 \ubaa8\ub4e0 \uc0dd\ubb3c\uacfc \ubb3c\uc9c8\ub4e4\uc774 \ub108\uc5d0\uac8c \uc801\ub300\uc801\uc77c \ud14c\ub2c8\uae4c..."}, {"op": "replace", "path": "/floranmission101/text", "value": "\uc774 \uc720\uc801\uc9c0\ub294 \uc544\uc8fc \uc624\ub798\ub418\uc5c8\uc73c\uba70 \ud50c\ub85c\ub780\uc758 \uc131\uc5ed\uc73c\ub85c \ubcf4\uc785\ub2c8\ub2e4. \uc81c \ub370\uc774\ud130\uc5d0 \ub530\ub974\uba74 \uc774\uacf3\uc740 \uc5b4\ub461\uace0 \ubd88\uae38\ud558\ub2e4\uace0 \ud558\ub124\uc694."}, {"op": "replace", "path": "/apexmission03a/text", "value": "\ubcf4\uc544\ud558\ub2c8 \uc6b0\ub9ac\uac00 \uadf8\uac04 \uc5b5\uc555\ub418\uc5b4 \uc628 \ubc18\ub780\uad70\uc758 \uc804\uc7c1\uc5d0 \uae0d\uc815\uc801\uc778 \uc601\ud5a5\uc744 \ubbf8\uce58\uace0 \uc788\uad6c\ub098! \ud6cc\ub96d\ud558\ub2e4. \ud2f0\ub04c \ubaa8\uc544 \ud0dc\uc0b0\uc774\uc9c0."}, {"op": "replace", "path": "/apexmission05/text", "value": "\uc774 \uc5f0\uad6c\uc18c\uc758 \uc9c0\ud558\uc5d0\uc11c \uc774\uc0c1\ud55c \uc0dd\uccb4 \uc2e0\ud638\uac00 \uac10\uc9c0\ub429\ub2c8\ub2e4."}]