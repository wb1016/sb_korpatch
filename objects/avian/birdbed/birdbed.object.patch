[{"op": "replace", "path": "/novakidDescription", "value": "\uc9da\ub2e8\uc744 \uc313\uc544\ub193\ub294\ub2e4\uace0 \uce68\ub300\uac00 \ud3b8\uc548\ud574\uc9c0\uc9c4 \uc54a\ub294\ub2e4."}, {"op": "replace", "path": "/hylotlDescription", "value": "\ub3cc\ub369\uc774\ub791 \uac19\uc774 \uc790\ub824\uba74 \uaf64\ub098 \uac15\uc778\ud55c \uc0ac\ub78c\uc774\uc5b4\uc57c\uaca0\uad70. \uc804\uc0ac\uc758 \uce68\ub300\ub2e4."}, {"op": "replace", "path": "/avianDescription", "value": "\uc218\ud638\uc790\ub4e4\uc758 \uce68\ub300\ub2e4. \uc218\ud638\uc790\ub4e4\uc740 \uc544\ube44\uc548 \uc131\uc9c0\ub97c \uc9c0\ud0a8\ub2e4. \uadf8\ub4e4\uc740 \ub3c5\uc2e4\ud568\uc73c\ub85c \uc0b6\uc744 \uc774\uc5b4\uac00\ub294 \uc57c\ub9cc\uc801\uc778 \uc6a9\uc0ac\ub4e4\uc774\ub2e4."}, {"op": "replace", "path": "/shortdescription", "value": "\uc218\ud638\uc790\uc758 \ub465\uc9c0"}, {"op": "replace", "path": "/description", "value": "\ub2e8\ub2e8\ud55c \ub3cc \uce68\ub300 \uc704\uc5d0 \uac74\ucd08 \ub465\uc9c0\uac00 \uc788\ub2e4."}, {"op": "replace", "path": "/humanDescription", "value": "\uc11d\uc7ac \uce68\ub300\ub2e4. \uac74\ucd08\ub97c \uae54\uc544\uc11c \uadf8\ub098\ub9c8 \ud3b8\uc548\ud558\uac8c \uc790\ub824\uace0 \ud55c \ubaa8\uc591\uc774\ub2e4. \ub3cc\uc740 \ub9cc\uc9c0\uba74 \ucc28\uac11\ub2e4."}, {"op": "replace", "path": "/floranDescription", "value": "\uc100\uc0e4\ub78c \uac74\ucd08 \ub465\uc9c0 \ub9cc\ub4e4\uc5c8\ub2e4. \ud50c\ub85c\ub780\ub3c4 \ube44\uc2b7\ud55c \uce68\ub300 \uc4f4\ub2e4."}, {"op": "replace", "path": "/apexDescription", "value": "\uac74\ucd08\uac00 \ub193\uc778 \uce68\ub300\ub2e4. \ub180\ub77c\uc6b8 \ub9cc\ud07c \ubd88\ud3b8\ud574\ubcf4\uc778\ub2e4."}, {"op": "replace", "path": "/glitchDescription", "value": "\uc6b0\ub824. \uc774 \uce68\ub300\ub294 \uc815\ub9d0\ub85c \uc778\uccb4 \uacf5\ud559\uc801\uc774\uc9c0 \ubabb\ud558\uad70."}]