[{"op": "replace", "path": "/hylotlDescription", "value": "\uc9c8\uc740 \ub0ae\uc9c0\ub9cc\uc11c\ub3c4 \uc774 \ubc30\ub108\ub97c \ub9cc\ub4e4 \ub54c \uc0c1\ub2f9\ud788 \uc990\uac81\uac8c \uc9e0 \uac78 \uc54c \uc218 \uc788\ub2e4. \ubcf4\uae30\ub9cc \ud574\ub3c4 \ub9c8\uc74c\uc774 \ub530\ub73b\ud574\uc9c4\ub2e4."}, {"op": "replace", "path": "/humanDescription", "value": "\uc0c9\uae54\ub4e4\uc880 \ubd10. \uc774\uac70 \ub9cc\ub4e4\uae30 \uc7ac\ubc0c\uaca0\ub2e4."}, {"op": "replace", "path": "/novakidDescription", "value": "\uc774 \ubc30\ub108\uc5d4 \ud798\uc138\uace0 \uac15\ud55c \uc7ac\ubd09\uc220\uc774 \ub4e4\uc5b4\uac14\ub294\uac78."}, {"op": "replace", "path": "/avianDescription", "value": "\uc774\ub7f0 \ubb34\ub2ac \ubc30\ub108\ub4e4\uc740 \ubc30\uad50\uc790\ub4e4\uc911\uc5d0\uc11c \uac00\uc7a5 \uc2e4\ub825\uc774 \ubd80\uc871\ud55c \uc774\ub4e4\uc774 \ub9cc\ub4e0\ub2e4. \ud558\uc9c0\ub9cc \ub2e4\uc218\uac00 \uac78\ub9ac\uace0 \ub098\uba74 \uc0c9\ucc44\uc758 \ubb3c\uacb0\uc744 \ub9cc\ub4e4\uc5b4\ub0b8\ub2e4."}, {"op": "replace", "path": "/apexDescription", "value": "\uc544\ube44\uc548 \ubb34\ub2ac \ubc30\ub108\ub77c. \ubb34\ub2ac\ub294 \uc870\uc545\ud558\uc9c0\ub9cc \uc7ac\uc9c8\uc740 \uc0dd\uae30\ub118\uce5c\ub2e4."}, {"op": "replace", "path": "/glitchDescription", "value": "\uc990\uac70\uc6c0. \ud6cc\ub96d\ud55c \uc0c9\uc0c1\uc774\ub2e4."}, {"op": "replace", "path": "/floranDescription", "value": "\ubc1d\uc740 \uc101\uae54\uc5d0 \uc6c3\uae34 \ubb34\ub2ac."}, {"op": "replace", "path": "/shortdescription", "value": "\uc544\ube44\uc548 \ubb34\ub2ac \ubc30\ub108"}, {"op": "replace", "path": "/description", "value": "\ud654\ub824\ud55c \ube44\ub2e8\uc73c\ub85c \ub9cc\ub4e4\uc5b4\uc9c4 \uc544\ube44\uc548 \ubc30\ub108\ub2e4."}]