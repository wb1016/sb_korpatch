[{"op": "replace", "path": "/avianDescription", "value": "\uc774 \ub2e8\uc9c0\uc5d0 \ub4e0 \uc7bf\ub354\ubbf8\ub294 \ubd80\ub044\ub7fd\uac8c \uc8fd\uc740 \uc544\ube44\uc548\uc758 \uc720\ud574\ub2e4. \ud1a0\ub07c \ud45c\uc2dc\ub294 \uc548\uc5d0 \ub4e0 \uc544\ube44\uc548\uc774 \uc601\uacc4\uc5d0\uc11c \uc601\uc6d0\ud788 \ub0a0\uac1c\ub97c \uac16\uc9c0 \ubabb \ud560 \uac83\uc744 \uc758\ubbf8\ud55c\ub2e4."}, {"op": "replace", "path": "/novakidDescription", "value": "\uc0ac\uaca9 \uc5f0\uc2b5\uc6a9\uc73c\ub85c \ub531 \uc88b\uc544\ubcf4\uc774\ub294 \ub2e8\uc9c0\ub2e4."}, {"op": "replace", "path": "/hylotlDescription", "value": "\uc65c \uc544\ube44\uc548\uc774 \ub9dd\uc790\ub4e4\uc758 \uc720\ud574\ub97c \uc774\ub7f0 \ub2e8\uc9c0\uc5d0 \ub2f4\uc544\ub480\uc744\uae4c?"}, {"op": "replace", "path": "/humanDescription", "value": "\ud1a0\ub07c \ub2e8\uc9c0\ub2e4. \uc720\ud574\ub85c \uac00\ub4dd \ucc28\uc788\ub2e4."}, {"op": "replace", "path": "/apexDescription", "value": "\ub2e8\uc9c0 \uc548\uc5d0 \uc7bf\ub354\ubbf8\uac00 \uac00\ub4dd\ud558\ub2e4. \uc720\ud574\uc77c \uc9c0\ub3c4 \ubaa8\ub974\uaca0\ub2e4."}, {"op": "replace", "path": "/glitchDescription", "value": "\ud63c\ub780. \uc774 \uc7ac\ub294 \uc544\ube44\uc548\uc5d0\uac8c\uc11c \ub098\uc654\uc9c0\ub9cc, \uc65c \ub2e8\uc9c0\ub294 \uc774\ub7f0 \ubaa8\uc591\uc0c8\uc77c\uae4c?"}, {"op": "replace", "path": "/floranDescription", "value": "\uc774\uac70 \uc544\ube44\uc548 \uc7ac\uc57c \uc544\ub2c8\uba74 \ub2e4\ub978 \uc115\ubb3c \uc7ac\uc57c?"}, {"op": "replace", "path": "/shortdescription", "value": "\ud1a0\ub07c \ub2e8\uc9c0"}, {"op": "replace", "path": "/description", "value": "\uc7bf\ub354\ubbf8\ub85c \uac00\ub4dd\ucc2c \ud1a0\ub07c \ubaa8\uc591 \ub2e8\uc9c0\ub2e4. \ubd80\uc234\ubcfc\uae4c."}]