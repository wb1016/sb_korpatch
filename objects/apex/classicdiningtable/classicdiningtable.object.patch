[{"op": "replace", "path": "/glitchDescription", "value": "\ud5a5\uc218. \uae00\ub9ac\uce58\ub4e4\uc740 \uc774\ub7f0 \ud0c1\uc790\uc5d0\uc11c \uc5f0\ud68c\ub97c \uc5f0\ub2e4. \ucda4\ucd94\uace0 \ub178\ub798\ud558\uace0, \uc5b4\ub9bf\uad11\ub300\ub4e4\uc774 \uc7ac\uc8fc\ub97c \ubd80\ub824\uc11c \uc190\ub2d8\ub4e4\uc5d0\uac8c \uc6c3\uc74c\ub3c4 \uc900\ub2e4."}, {"op": "replace", "path": "/apexDescription", "value": "\uc5d0\uc774\ud399\uc2a4 \uae30\uc900\uc5d0 \ub9de\uc9c0 \uc54a\uac8c \uc0ac\uce58\uc2a4\ub7fd\ub2e4."}, {"op": "replace", "path": "/floranDescription", "value": "\uc9c4\uc288\uc169\ucc2c\uc774\ub2e4! \uc5c4\uccad \ud070 \ud0c1\uc790 \uba39\uc744 \uac70 \uc794\ub729 \ub193\uc744 \uc288 \uc788\ub2e4!"}, {"op": "replace", "path": "/description", "value": "\uc120\uc5b8\ubb38\uc744 \uc791\uc131\ud558\uace0 \uc2f6\uc744 \ub54c \uc4f0\ub294 \ud0c1\uc790\ub2e4."}, {"op": "replace", "path": "/novakidDescription", "value": "\uc5c4\uccad \ud070 \uc2dd\ud0c1\uc774\ub124! \uc5ec\uae30\uc120 \uce74\ub4dc\uac8c\uc784 \ubabb\ud574\uba39\uaca0\uc5b4."}, {"op": "replace", "path": "/hylotlDescription", "value": "\ud558\uc774\ub85c\ud2c0\uc5d0\uac8c\ub294 \uc2dd\ud0c1\uc744 \ucc28\ub9ac\uae30 \uc804\uc5d0 \ube48 \uc790\ub9ac\uac00 \uc788\uc73c\uba74 \uc548 \ub41c\ub2e4\ub294 \uc778\uc2dd\uc774 \uc788\ub2e4. \uc5ec\uae30\uc120 \uadf8\ub7f0 \uac70 \ud558\uba74 \uc548 \ub418\uaca0\ub2e4."}, {"op": "replace", "path": "/avianDescription", "value": "\uc774\ub807\uac8c \ud070 \uc2dd\ud0c1\uc740 \ubcf8 \uc801\uc774 \uc5c6\ub2e4. \uc544\ube44\uc548\uc740 \ud63c\uc790 \ubc25 \uba39\ub294 \uac78 \uc120\ud638\ud55c\ub2e4."}, {"op": "replace", "path": "/humanDescription", "value": "\uc2dd\ud0c1\uc880 \ubcf4\uac8c. \uac70\uc758 \uc4f0\uc9c0\ub3c4 \uc54a\uc740 \uc0c8 \uac83 \uac19\ub2e4."}, {"op": "replace", "path": "/shortdescription", "value": "\uace0\uc804\uc801 \uc2dd\ud0c1"}]