[{"op": "replace", "path": "/hylotlDescription", "value": "\uc774 \uc11c\ubc84\ub294 \ud558\uc774\ub85c\ud2c0 \ub3c4\uc11c\uad00 \ucc9c \uac1c \ubd84\ub7c9\uc744 \ucc44\uc6b0\uae30\uc5d0 \ucda9\ubd84\ud574 \ubcf4\uc5ec."}, {"op": "replace", "path": "/novakidDescription", "value": "\uc774 \uae30\uacc4 \ub369\uc774\ub294 \uc790\ub8cc\ub97c \uc800\uc7a5\ud574."}, {"op": "replace", "path": "/glitchDescription", "value": "\ubd80\ub044\ub7ec\uc6c0. \uc11c\ubc84\uac00 \ub098\uc870\ucc28 \uc6b0\ud68c\ud558\uc9c0 \ubabb\ud560 \uc815\ub3c4\uc758 \ubcf4\uc548\uc73c\ub85c \ubcf4\ud638\ub418\uace0 \uc788\uad70."}, {"op": "replace", "path": "/avianDescription", "value": "\uae5c\ubc15\uc774\ub294 \ubd88\ube5b\uc774 \ub9ce\ub2e4."}, {"op": "replace", "path": "/humanDescription", "value": "\ub370\uc774\ud130 \uc11c\ubc84 \uac19\uc544 \ubcf4\uc774\ub294\ub370. \uc5ec\uae30\uc11c \ubb54\uac00\ub97c \uc5bb\uc5b4\uac08 \ubc29\ubc95\uc740 \uc5c6\uc5b4."}, {"op": "replace", "path": "/apexDescription", "value": "\uc811\uadfc\uc774 \ubd88\uac00\ub2a5\ud55c \ubcf4\uc548 \uc11c\ubc84\uc57c."}, {"op": "replace", "path": "/floranDescription", "value": "\ube5b\ub098\ub294 \uc0f9\uc790\uac00 \ub538\uac01\uac70\ub9ac\ub294 \uc18c\ub9ac\ub97c \ub0b4."}, {"op": "replace", "path": "/shortdescription", "value": "\uba4b\uc9c4 \uc11c\ubc84"}, {"op": "replace", "path": "/description", "value": "\uc11c\ubc84\ucc98\ub7fc \ubcf4\uc778\ub2e4. \uc5b4\ub5a4 \uc885\ub958\uc758 \uac8c\uc784\uc744 \ud638\uc2a4\ud2b8 \ud560 \uc218 \uc788\ub294\uc9c0 \uad81\uae08\ud558\ub2e4."}]