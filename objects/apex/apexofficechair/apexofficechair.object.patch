[{"op": "replace", "path": "/hylotlDescription", "value": "\ubd88\ud3b8\ud55c \uc758\uc790\ub2e4. \ub354 \uc624\ub798 \uc549\uc544\uc788\ub2e4\uac04 \uace0\ud1b5\uc774 \uc5c4\uc2b5\ud560 \uac83 \uac19\ub2e4."}, {"op": "replace", "path": "/avianDescription", "value": "\uc774 \ubb34\uc2a8 \ub9c8\uadc0\uc758 \uc7a5\ub09c\uc774\uc57c, \uc758\uc790\uc5d0 \ubc14\ud034\ub97c \ub2ec\ub2e4\ub2c8?"}, {"op": "replace", "path": "/novakidDescription", "value": "\ubcf4\uae30\ubcf4\ub2e4 \ud6e8\uc52c \ubd88\ud3b8\ud574. \uac89\ubcf4\uae30\uc5d0\ub3c4 \ubd88\ud3b8\ud574 \ubcf4\uc774\uace0."}, {"op": "replace", "path": "/humanDescription", "value": "\ubcf4\uae30\ub9cc \ud574\ub3c4 \uc6b0\uc6b8\ud574\uc9c0\ub294 \ubc14\ud034\ub2ec\ub9b0 \uc0ac\ubb34\uc2e4 \uc758\uc790\ub2e4. \uc5bc\ub9c8\ub098 \ub9ce\uc740 \uc0ac\ub78c\ub4e4\uc774 \uc544\uc9c1\ub3c4 \uc774\ub7f0 \uac78 \uc4f4\ub2f4?"}, {"op": "replace", "path": "/glitchDescription", "value": "\ubb34\uac10\ub3d9. \uc778\uccb4\uacf5\ud559\uc740 \uace0\ub824\ud558\uc9c0\ub3c4 \uc54a\uc558\ub124."}, {"op": "replace", "path": "/floranDescription", "value": "\uac00\uc8fd\uc5d0\uc154 \ub9db\uc788\ub294 \ub0c4\uc100 \ub09c\ub2e4."}, {"op": "replace", "path": "/shortdescription", "value": "\uc0ac\ubb34\uc2e4 \uc758\uc790"}, {"op": "replace", "path": "/description", "value": "\uc81c\uc2dd \uc0ac\ubb34\uc2e4 \uc758\uc790\ub2e4."}, {"op": "replace", "path": "/apexDescription", "value": "\uc0ac\ubb34\uc2e4 \uc9c1\uc6d0\ub4e4\uc774 \uc4f0\ub358 \uae30\ubcf8 \uc758\uc790\ub2e4. \ub514\uc790\uc778 \ub54c\ubb38\uc5d0 \ubd88\ud3b8\ud558\ub2e4."}]