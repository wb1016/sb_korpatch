[{"op": "replace", "path": "/glitchDescription", "value": "\ud63c\ub780. \uc774 \uc811\uc2dc\uc5d0 \uadf8\ub824\uc9c4 \uc5d0\uc774\ud399\uc2a4\ub294 \ud6e8\uc52c \uc9c4\ud654\ud55c \uc885\uc73c\ub85c \ubcf4\uc778\ub2e4. \uc65c \uc774\ub7f0 \uadf8\ub9bc\uc774 \uc774\ub9ac \uc624\ub798\ub41c \uc811\uc2dc\uc5d0 \ub0a8\uc544\uc788\ub294 \uac70\ub78c."}, {"op": "replace", "path": "/novakidDescription", "value": "\uc804\uc2dc\ub300\uc5d0 \uc811\uc2dc\uac00 \ud55c\uac00\ub4dd\uc774\ub2e4. \uc5b4\ub5bb\uac8c \uaebc\ub0b4\uc57c \ud560 \uc9c0 \uac10\ub3c4 \uc548 \uc7a1\ud78c\ub2e4."}, {"op": "replace", "path": "/hylotlDescription", "value": "\uc774 \uadf8\ub9bc\ub4e4\ub9cc \ubcf4\uace0 \ud310\ub2e8\ud558\uc790\uba74 \uc5d0\uc774\ud399\uc2a4\ub3c4 \ud55c\ub54c\ub294 \uace0\uadc0\ud55c \uc885\uc871\uc774\uc5c8\ub358 \uac83 \uac19\ub2e4."}, {"op": "replace", "path": "/avianDescription", "value": "\uc5ec\uae30 \uc788\ub294 \uc5d0\uc774\ud399\uc2a4\ub4e4\uc740 \ud138\uc774 \uc5c6\ub2e4. \uc5d0\uc774\ud399\uc2a4\uac00 \ud655\uc2e4\ud558\ub2e4\ub294 \uc804\uc81c \ud558\uc5d0."}, {"op": "replace", "path": "/humanDescription", "value": "\uc5ec\uae30 \uc788\ub294 \ub3c4\uc790\uae30\uc5d4 \uc628\ud1b5 \ud138 \uc5c6\ub294 \uc5d0\uc774\ud399\uc2a4 \uadf8\ub9bc \ubfd0\uc774\ub124."}, {"op": "replace", "path": "/apexDescription", "value": "\ub3c4\uc790\uae30\uc5d0 \uc720\uc804\uc790\uac00 \ubcc0\uc9c8\ub418\uae30 \uc774\uc804\uc758 \uc5d0\uc774\ud399\uc2a4 \uadf8\ub9bc\uc774 \uadf8\ub824\uc838\uc788\ub2e4. \uadf8 \uc2dc\uc808\ub85c \ub2e4\uc2dc \ub3cc\uc544\uac00\uae30\uc5d4 \uc6b0\ub9ac \ubaa8\ub450 \ub108\ubb34 \uba40\ub9ac \uc640\ubc84\ub838\ub2e4."}, {"op": "replace", "path": "/floranDescription", "value": "\uc6c3\uae34 \uc5d0\uc774\ud399\uc2a4\ub2e4. \uc65c \uc811\uc2dc \uac00\ub46c\ub194? \ub3c4\ub9dd\uce60 \uc288\ub3c4 \uc5c6\uc744 \ud150\ub370."}, {"op": "replace", "path": "/shortdescription", "value": "\uace0\uc804\uc801 \ub3c4\uc790\uae30 \ubcf4\uad00\ud568"}, {"op": "replace", "path": "/description", "value": "\uc9d1 \uc548\uc5d0\uc11c \uacf5\ub180\uc774\ub97c \ud558\uba74 \uc548 \ub418\ub294 \uc774\uc720."}]