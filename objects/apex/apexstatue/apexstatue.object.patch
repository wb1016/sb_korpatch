[{"op": "replace", "path": "/novakidDescription", "value": "\uc2a4\uc2a4\ub85c\uc758 \ubaa9\uc228\uc744 \ud76c\uc0dd\ud55c \uc790\ub97c \uc704\ud55c \ub3d9\uc0c1\uc774\ub2e4. \uc6d0\ud574\uc11c \uadf8\ub7ac\uc744\uae4c?"}, {"op": "replace", "path": "/avianDescription", "value": "\uacfc\ud559\uc790\ub2e4. \ubcc4\uad00\uce21\uc790\ub4e4\uc740 \uc774\ub530\uae08 \uacfc\ud559\uc5d0 \ub0b4\uc7ac\ub41c \uc704\ud5d8\uc131\uc744 \uacbd\uace0\ud558\uace4 \ud588\ub2e4."}, {"op": "replace", "path": "/glitchDescription", "value": "\uc9c8\ubb38. '\uc790\uc2e0\uc758 \ubab8\uc744 \uacfc\ud559\uc5d0 \ubc14\uce5c' \uacfc\ud559\uc790. \ub2e4\ub978 \uc120\ud0dd\uc9c0\uac00 \uc5c6\uc5c8\ub358 \uac74 \uc544\ub2d0\uae4c?"}, {"op": "replace", "path": "/apexDescription", "value": "\uc790\uc2e0\uc758 \uc720\uc804\uc790\ub97c \ubc14\uafd4\ubcf4\ub824\ub2e4 \uc8fd\uc740 \uacfc\ud559\uc790\ub2e4. \uadf8\ub294 \ub2e4\ub978 \ubaa8\ub4e0 \uc5d0\uc774\ud399\uc2a4\uc758 \uadc0\uac10\uc73c\ub85c \uc4f0\uc600\ub2e4. \uc9c0\uc815\ubd80\ub294 \uc774\uac83\uc774 \uc5d0\uc774\ud399\uc2a4\uac00 \uc131\ucde8\ud560 \uc218 \uc788\ub294 \uac00\uc7a5 \ud070 \uc601\uad11\uc774\ub77c \ud55c\ub2e4."}, {"op": "replace", "path": "/floranDescription", "value": "\uc790\uae30 \ubab8 \uacfc\ud559\uc5d0 \ubc14\uce5c \uba4d\uccad\ud55c \uc5d0\uc774\ud399\uc2a4. \uc2e0\uc158\ud55c \uace0\uae30 \uc544\uae5d\ub2e4."}, {"op": "replace", "path": "/hylotlDescription", "value": "\uc790\uacb0\uc740 \ud558\uc774\ub85c\ud2c0 \uc0ac\ud68c\uc5d0\uc11c\ub3c4 \uba85\uc608\ub85c\uc6b4 \uc77c\uc774\uc9c0\ub9cc, \uc120\uc758\ub97c \uac00\uc84c\uc744 \ub54c\ub9cc \uadf8\ub807\ub2e4."}, {"op": "replace", "path": "/humanDescription", "value": "\ubcf4\uc544\ud558\ub2c8 \uc790\uae30 \ubab8\uc744 \uc790\uae30 \uc2e4\ud5d8\uc5d0 \ubc14\uce5c \uac83 \uac19\uc740\ub370. \uace0\uadc0\ud55c \uac74\uc9c0 \uba4d\uccad\ud55c \uac74\uc9c0."}, {"op": "replace", "path": "/description", "value": "\uacfc\ud559\uc5d0 '\ubab8\uc744 \ubc14\uce5c' \uacfc\ud559\uc790 \uac19\ub2e4."}, {"op": "replace", "path": "/shortdescription", "value": "\uc5d0\uc774\ud399\uc2a4 \uacfc\ud559\uc790 \ub3d9\uc0c1"}]