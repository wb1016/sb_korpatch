[{"op": "replace", "path": "/hylotlDescription", "value": "\uc774\ub7f0 \ucc45\ub4e4\uc740 \uc11c\ub85c \ub3cc\ub824\uac00\uba74\uc11c \ubd10\uc57c\uc9c0. \uc9c0\uc2dd\uc740 \uc790\uc720\ub85c\uc6cc\uc57c \ud574."}, {"op": "replace", "path": "/avianDescription", "value": "\uc544, \uc601\uad11\uc2a4\ub7ec\uc6b4 \ub3c4\uc11c\uad00 \ud45c\uc2dc\ub2e4. \uc544\ube44\uc548 \ub300\ub2e4\uc218\uac00 \uc77d\uc744 \uc904\uc740 \ubaa8\ub974\uc9c0\ub9cc\uc11c\ub3c4 \ucc45\uc744 \uacbd\uc678\ud55c\ub2e4."}, {"op": "replace", "path": "/humanDescription", "value": "\uae08\uc73c\ub85c \uc7a5\uc2dd\ub41c \ucc45\uaf42\uc774?"}, {"op": "replace", "path": "/floranDescription", "value": "\ud50c\ub85c\ub780 \ud53c\ub791 \ubf08\ub85c \uc9c0\uc2dd \ub098\ub208\ub2e4. \ucc45\ubcf4\ub2e4 \ub0c4\uc100\uac00 \uc54c\ub824\uc8fc\ub294 \uac70 \ub9ce\ub2e4."}, {"op": "replace", "path": "/novakidDescription", "value": "\uae54\uc308\ud55c \ucc45\uaf42\uc774\uc5d0 \uadf8\ub0e5 \ucc45 \uba87 \uad8c \ubfd0."}, {"op": "replace", "path": "/apexDescription", "value": "\uc9c0\uc815\ubd80\uc5d0\uc11c \ucc45\uc5d0 \ub300\ud55c \uc811\uadfc\uc744 \uae08\ud588\uae30\uc5d0 \uc6b0\ub9b0 \ucc45\ub9cc \ubcf4\uba74 \ud0dc\uc6cc\uc57c \ud55c\ub2e4\ub294 \uc0dd\uac01\ubfd0\uc774\ub2e4."}, {"op": "replace", "path": "/glitchDescription", "value": "\ud0d0\uad6c. \uc5ec\uae30\uc5d0\uc11c \ub0b4\uac00 \uc4f8\ub9cc\ud55c \uc9c0\uc2dd\uc744 \ucc3e\uc744 \uc218 \uc788\uc744 \uc9c0\ub3c4 \ubaa8\ub974\uaca0\ub2e4."}, {"op": "replace", "path": "/shortdescription", "value": "\uace0\ud48d\uc801\uc778 \ucc45\uaf42\uc774"}, {"op": "replace", "path": "/description", "value": "\uace0\uc804 \ubb38\ud559\uc73c\ub85c \uac00\ub4dd \ucc2c \ucc45\uaf42\uc774\ub2e4."}]