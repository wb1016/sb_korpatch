[{"op": "replace", "path": "/hylotlDescription", "value": "\uc778\uac04 \ubcbd\ubcf4\uc5d0 \uc5d0\uc774\ud399\uc2a4 \uc545\ub2f9\uc774\ub77c. \uad81\uae08\ud558\ub124."}, {"op": "replace", "path": "/avianDescription", "value": "\ube45 \uc5d0\uc774\ud504\ub97c \ub098\ud0c0\ub0b8 \ubcbd\ubcf4\ub2e4. \uc778\uac04\uc774\ub791 \uc5d0\uc774\ud399\uc2a4\ub791 \ubb34\uc2a8 \uc5f0\uad00\uc774 \uc788\ub098?"}, {"op": "replace", "path": "/humanDescription", "value": "\ub0af\uc774 \uc775\uc740 \ub9cc\ud654 \uce90\ub9ad\ud130\uc778\ub370. \uc7a0\uae50... \uc800\uac70 \ud639\uc2dc \ube45 \uc5d0\uc774\ud504\uc57c?"}, {"op": "replace", "path": "/apexDescription", "value": "\ube45 \uc5d0\uc774\ud504? \uc5ec\uae30\uc5d0? \uc778\uac04\ub4e4\uc774 \uc65c \uc774\ub7f0 \ubcbd\ubcf4\ub97c \ub9cc\ub4e4\uc5b4? \ub18d\ub2f4\ud558\uc790\ub294 \uac74\uac00?"}, {"op": "replace", "path": "/glitchDescription", "value": "\uc218\uc0c1. \uc65c \uc778\uac04\ub4e4\uc774 \ube45 \uc5d0\uc774\ud504\uc758 \ubcbd\ubcf4\ub97c \uac00\uc9c0\uace0 \uc788\uc744\uae4c?"}, {"op": "replace", "path": "/floranDescription", "value": "\ud50c\ub85c\ub780 \uc65c \ube45 \uc5d0\uc774\ud504\uac00 \uc5ec\uae30 \ubd99\uc5b4\uc788\ub294 \uc9c0 \ubaa8\ub974\uaca0\ub2e4. \ud138\ud22c\uc169\uc774 \uc778\uac04?"}, {"op": "replace", "path": "/shortdescription", "value": "\uc9c4\uc815 \ud3ec\uc2a4\ud130"}, {"op": "replace", "path": "/description", "value": "\ud654\ub0bc \ud544\uc694 \uc5c6\uc5b4\uc694."}, {"op": "replace", "path": "/novakidDescription", "value": "\uc5d0\uc774\ud399\uc2a4\ub2e4. \ud654\ub0bc \ud544\uc694 \uc5c6\ub2e8\ub2e4."}]