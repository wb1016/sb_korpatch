[{"op": "replace", "path": "/glitchDescription", "value": "\uac71\uc815. \uc774 \uc870\uc791\uae30\ub97c \uc774\uc6a9\ud558\ub358 \uac00\uc7a5 \ucd5c\uadfc\uc758 \uc0ac\uc6a9\uc790\ub294 \ubbf8\uccd0\ub3cc\uc544\ubc84\ub9b0 \uac83 \uac19\ub2e4. \uacbd\uace0 \uba54\uc2dc\uc9c0\uac00 \uc0c1\ub2f9\uc218 \ub5a0\uc788\ub2e4."}, {"op": "replace", "path": "/novakidDescription", "value": "\ubb38\uc6cc\ud0b9\uc744 \uae08\uc9c0\ud558\uc790\ub294 \ud0c4\uc6d0\uc11c\ub2e4. \uadf8 \ub4a4\uc5d0\ub294 \uc591\ud30c \uc218\ud504 \uc870\ub9ac\ubc95\uc774 \uc788\ub2e4."}, {"op": "replace", "path": "/avianDescription", "value": "\ud55c \ud300\uc774 \uc800\uc8fc\ubc1b\uc740 \uc720\ubb3c\uc744 \ubc1c\uad74\ud558\ub7ec \ub098\uac14\ub2e4\uac00 \ub2e4\uc2dc\ub294 \ub3cc\uc544\uc624\uc9c0 \uc54a\uc558\ub2e4. \ub2e4\ub978 \uae38\ub85c \uc0dc\ub098?"}, {"op": "replace", "path": "/hylotlDescription", "value": "\ud589\uc131\uc758 \ubc14\ub2e4\ub97c \uc2f9 \ub9d0\ub824\ubc84\ub9ac\uc790\ub294 \uacc4\ud68d\ub3c4 \ubcf4\uc778\ub2e4. \ub2e4\ud589\ud788\ub3c4 \uc2e4\ud589\ub418\uc9c4 \uc54a\uc558\ub2e4."}, {"op": "replace", "path": "/humanDescription", "value": "\uc624\uc624\uc624, \uae30\ubc00 \uc815\ubcf4\ub2e4. \ub0b4 \uadc0\uc5d0 \ub3c4\uccad\uc7a5\uce58\uac00 \uc788\ub2e4\ub2c8! \ub204\uac00 \uc54c\uc558\uaca0\uc5b4?"}, {"op": "replace", "path": "/apexDescription", "value": "\uc774 \ud589\uc131\uc740 \uc6d0\ub798 \ud3ed\ubc1c\ud560 \ud589\uc131\uc774\uc5c8\ub2e4\ub294 \uac83 \uac19\ub2e4. \uac70\uc9d3 \uacbd\ubcf4\uc778 \ubaa8\uc591\uc774\ub2e4."}, {"op": "replace", "path": "/floranDescription", "value": "\uc0e4\ub0e5... \uc794\uce58... \ud50c\ub85c\ub780 \uc0e4\ub0e5 \uc794\uce58 \uc5f0\ub2e4."}, {"op": "replace", "path": "/shortdescription", "value": "\ub113\uc740 \uc870\uc791\uae30"}, {"op": "replace", "path": "/description", "value": "\ud654\uba74\uc5d0  \ucd5c\uc0c1\uc704 \ube44\ubc00 \uc784\ubb34 \uc815\ubcf4\uac00 \uac00\ub4dd \ucc28 \ubcf4\uc778\ub2e4... \uc9c0\ub8e8\ud574."}]