[{"op": "replace", "path": "/novakidDescription", "value": "\ub204\uad70\uac00 \uc774 \uc790\ub8cc\uc5d0 \uad00\uc2ec\uc744 \ubcf4\uc77c \uc0ac\ub78c\uc774 \uc800 \uc5b4\ub518\uac00\uc5d0 \uaf2d \uc788\uc744 \uac70\ub2e4. \ub098\ub294 \uc544\ub2c8\ub2e4."}, {"op": "replace", "path": "/hylotlDescription", "value": "\uc8fc\ubcc0 \ud658\uacbd\uc5d0 \ub300\ud55c \uc790\ub8cc\ub2e4. \ud558\uc774\ub85c\ud2c0\uacfc \ub9c8\ucc2c\uac00\uc9c0\ub85c \uc778\uac04\ub4e4 \ub610\ud55c \uc790\uc2e0\ub4e4\uc774 \ubc29\ubb38\ud558\ub294 \uacf3\uc5d0 \uc8fc\uc758\ub97c \uae30\uc6b8\uc778\ub2e4."}, {"op": "replace", "path": "/avianDescription", "value": "\uadf8\ub9ac \uacfc\ud559\uc801\uc73c\ub85c \ubcf4\uc774\uc9c4 \uc54a\ub294\ub2e4. \ud658\uacbd\uc5d0 \uad00\ud55c \ud3c9\uac00\uac00 *\ubb50 \uaf64 \uad1c\ucc2e\ub124* \ub77c\uace0 \uc4f0\uc5ec\uc788\ub2e4."}, {"op": "replace", "path": "/humanDescription", "value": "\ubc14\uae65 \ud658\uacbd\uc774 \uc778\uac04\ub4e4\uc5d0\uac8c \uc548\uc804\ud558\ub2e4. \ud37d\uc774\ub098 \uc720\uc6a9\ud558\ub2e4. \uc774\ubbf8 \ubc14\uae65\uc5d0\uc11c \uc794\ub729 \ub4b9\uad74\ub2e4 \uc654\ub294\ub370."}, {"op": "replace", "path": "/apexDescription", "value": "\uc774 \uc790\ub8cc\uc5d0 \ub530\ub974\uba74 \ubc14\uae65\uc758 \ud658\uacbd\uc740 \uac70\uc8fc\uc5d0 \uc801\ud569\ud574 \ubcf4\uc778\ub2e4."}, {"op": "replace", "path": "/glitchDescription", "value": "\ubb34\uc2dc. \uc774 \uc790\ub8cc\uac00 \ubb50\ub77c\ub4e0\uac04\uc5d0 \ubc14\uae65\ud658\uacbd\uc740 \uae00\ub9ac\uce58\uc5d0\uac8c \ub531 \ub9de\ub2e4."}, {"op": "replace", "path": "/floranDescription", "value": "\ud50c\ub85c\ub780 \uc774 \uc790\ub8cc \uc7ac\ubbf8\uc5c6\ub2e4."}, {"op": "replace", "path": "/shortdescription", "value": "\ud658\uacbd \uc790\ub8cc \ud328\ub110"}, {"op": "replace", "path": "/description", "value": "\uc774 \ubc29\uacf5\ud638 \ubc14\uae65\uc758 \ud658\uacbd\uc744 \ubd84\uc11d\ud55c \uac83\uc73c\ub85c \ubcf4\uc778\ub2e4."}]