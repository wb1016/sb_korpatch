[{"op": "replace", "path": "/glitchDescription", "value": "\uce5c\uc808. \uad50\uc815\uc744 \uc57d\uac04\ud55c\ub2e4\uba74 \uae00\ub9ac\uce58\uc758 \ub208\uc740 \uc2dd\ubcc4\uae30\ub97c \uc18d\uc77c \uc218 \uc788\uc9c0."}, {"op": "replace", "path": "/hylotlDescription", "value": "\ub208 \uc2dd\ubcc4\uae30\ub124. \uc6b0\ub9ac \uba38\ub9ac\ub85c\ub294 \uc774\ub7f0\uac70\uc5d4 \uc5b4\uc6b8\ub9ac\uc9c0 \uc54a\ub294\ub2e4\uace0 \ubcf4\ub294\ub370."}, {"op": "replace", "path": "/avianDescription", "value": "\uc778\uac04\uc871\uc758 \ub208\uc744 \ud655\uc778\ud558\ub294 \uc7a5\uce58\ub2e4. \uc5b4\ub5a4 \ubaa9\uc801\uc73c\ub85c?"}, {"op": "replace", "path": "/humanDescription", "value": "\ud64d\ucc44 \uc2dd\ubcc4\uae30. \ub204\uad70\uac00\uac00 \ub208\uc744 \ubb38\uc9c8\ub824\uc904... \uc54c\uc558\uc5b4. \ub18d\ub2f4\uadf8\ub9cc\ud560\uaed8"}, {"op": "replace", "path": "/apexDescription", "value": "\ud64d\ucc44 \uc2dd\ubcc4\uae30\ub85c\uad70. \uc544\ud399\uc2a4\uc758 \ub208\uc740 \uad6c\ubcc4\ud558\uc9c0 \ubabb\ud558\ub294\uac70 \uac19\uc740\ub370."}, {"op": "replace", "path": "/floranDescription", "value": "\uae30\uacc4 \ub208 \ubcf8\ub2e4. \ud50c\ub85c\ub780 \uc2eb\ub2e4!"}, {"op": "replace", "path": "/shortdescription", "value": "\ud64d\ucc44 \uc2dd\ubcc4\uae30"}, {"op": "replace", "path": "/description", "value": "\uc774 \uae30\uae30\ub294 \ub208\uc54c\uc744 \uc2dd\ubcc4\ud574 \uac1c\uc778 \uc815\ubcf4\ub97c \ud310\ubcc4\ud55c\ub2e4."}, {"op": "replace", "path": "/novakidDescription", "value": "\ub208\uc54c \uc2dd\ubcc4\uae30? \ub0b4\uac00 \uc5b4\ub5bb\uac8c \ud574\uc57c \uc9c0\ub098\uac08 \uc218 \uc788\ub294\uac74\ub370?"}]