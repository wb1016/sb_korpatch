[{"op": "replace", "path": "/glitchDescription", "value": "\ud638\uae30\uc2ec. \ud654\uba74\uc5d0 \uba54\uc2dc\uc9c0\uac00 \ub5a0\uc788\ub2e4. \"\ub124\uac00 \uc65c \ud2c0\ub838\ub294 \uc9c0 \uc54c\ub824\uc8fc\ub9c8...\"  \ub098\uba38\uc9c0\ub294 \uc54c\uc544\ubcfc \uc218\uac00 \uc5c6\ub2e4."}, {"op": "replace", "path": "/novakidDescription", "value": "PC\uac00 \ub193\uc5ec\uc788\ub2e4. \uc5ff\ubcf4\ub294 \ucde8\ubbf8\ub294 \uc5c6\ub2e4."}, {"op": "replace", "path": "/hylotlDescription", "value": "\uc774 \uba54\ubaa8\ub4e4\ub85c \ubcf4\uac74\ub300,  \uc778\uac04\ub4e4\uc740 \ucef4\ud4e8\ud130\ub85c \ud1a0\ub860\ud560 \ub54c \ud0a4\ubcf4\ub4dc\uc5d0 \uc5bc\uad74\uc744 \ucc98\ubc15\uae30\ub9cc \ud588\uc744 \ud14c\uc9c0."}, {"op": "replace", "path": "/avianDescription", "value": "\ud654\uba74\uc5d0 \uba54\uc2dc\uc9c0\uac00 \ub5a0\uc788\ub2e4. \"\uc5b4\uc81c\uc790 \ub3c5\uc131 \uc2e4\ud5d8\uc758 \uacb0\uacfc\ub294 \ud655\uc815\uc801\uc774\uc9c0 \uc54a\uc74c. \uc77c\uc815\uc744 \ub298\ub824\ub2ec\ub77c. \ud074\ub77c\uc774\ub108 \ubc15\uc0ac.\" \uadf8\ub798."}, {"op": "replace", "path": "/humanDescription", "value": "\uc774\uba54\uc77c \uacc4\uc815\uc744 \ubcf4\uc544\ud558\ub2c8 '\ub2d8\uc544 \ub2f5\ubcc0\uc880\uc694' \uc720\ud615\uc5d0 \uac78\ub9b0 \ubaa8\uc591\uc774\ub2e4. \ubd88\uc30d\ud558\uae30\ub3c4 \ud558\uc9c0."}, {"op": "replace", "path": "/apexDescription", "value": "\ud654\uba74\uc5d0 \uba54\uc2dc\uc9c0\uac00 \ub5a0\uc788\ub2e4. \"\ub9c8\uc800\ub9ac\uc5d0\uac8c. \uc81c\ubc1c \uace0\uc591\uc774\uc880 \ub0b4\ucad3\uc544\uc918\uc694. \uc0ac\ub791\ud558\ub294 \ub370\uc774\ube44\ub4dc\uac00.\" \ud765\ubbf8\ub86d\uad70."}, {"op": "replace", "path": "/floranDescription", "value": "\uc74c... \ud654\uba74\uc5d0 \uba54\uc170\uc9c0 \ub5a0\uc788\ub2e4. \"\uc77c\uc774 \ub05d\ub098\uba74 \uacc4\ub780\uc880 \uc0e4\uc640\uc694.\""}, {"op": "replace", "path": "/shortdescription", "value": "\uc791\uc740 \uc870\uc791\uae30"}, {"op": "replace", "path": "/description", "value": "\u