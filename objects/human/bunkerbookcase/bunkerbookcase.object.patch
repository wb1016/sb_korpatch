[{"op": "replace", "path": "/glitchDescription", "value": "\uad00\uc2ec. \uc774 \ucc45\ub4e4 \uc911\uc5d0\ub294 \uc218\ubc31 \ub144 \ub41c \uac83\ub3c4 \uc788\ub2e4. \uc0b4\uc9dd \ub9cc\uc9c0\uae30\ub9cc \ud574\ub3c4 \ubd80\uc2a4\ub7ec\uc9c8\uac83 \uac19\ub2e4."}, {"op": "replace", "path": "/description", "value": "\uc5ec\uae30 \uc774\ub7f0 \ucc45\ub3c4 \uc788\ub2e4. '\ubc14\ubcf4\ub4e4\ub3c4 \uc54c \uc218 \uc788\ub294 \uc2dc\uac04\uc744 \uac70\uafb8\ub85c \ub3cc\ub9ac\ub294 \ubc95'"}, {"op": "replace", "path": "/novakidDescription", "value": "\ucc45\uc7a5\uc774\ub2e4. \uc790\ub9bd\ud558\ub294 \ucc45\ub4e4\ub3c4 \uaf64 \uc788\uad6c\ub9cc..."}, {"op": "replace", "path": "/hylotlDescription", "value": "\uc778\uac04 \uc5ed\uc0ac\ub85c \uac00\ub4dd\ud55c \uc11c\uc801\ub4e4\uc774\ub2e4. \uc774 \ucc45\uc740 \ubc15\uc950\ucc98\ub7fc \uc637\uc744 \uc785\uace0 \ubc94\uc8c4\uc640 \uc2f8\uc6b0\ub294 \ub0a8\uc790\uc758 \uc5f0\ub300\uae30\ub97c \ub2e4\ub8e8\uace0 \uc788\ub2e4."}, {"op": "replace", "path": "/avianDescription", "value": "\uacfc\ud559 \uc11c\uc801\uc774 \uac00\ub4dd\uc774\ub2e4. \uc885\uad50\uc801\uc778 \uae00\uc740 \ubcc4\ub85c \ubcf4\uc774\uc9c0 \uc54a\ub294\ub2e4."}, {"op": "replace", "path": "/humanDescription", "value": "\uc775\uc219\ud55c \ucc45\uc774 \uc794\ub729\uc774\ub2e4. '\uc804\uc7c1\uacfc \ud3c9\ud654' \uc0ac\ubcf8\ub3c4 \uc788\ub2e4. \uc544\ubb34\ub3c4 \uc190\ub300\uc9c0 \uc54a\uc740 \uac74 \ub2f9\uc5f0\uc9c0\uc0ac\ub2e4."}, {"op": "replace", "path": "/apexDescription", "value": "\ucc45\uc7a5\uc774\ub2e4. \uc7ac\ubc0c\ub294 \ucc45\ub4e4\ub85c \uac00\ub4dd\uc774\ub2e4. \uc9c0\uc815\ubd80\ub77c\uba74 \uc804\ubd80 \ud0dc\uc6cc\ubc84\ub838\uaca0\uc9c0."}, {"op": "replace", "path": "/floranDescription", "value": "\uae00\uc528\ub9cc \uc794\ub729, \uadf8\ub9bc\uc740 \uc5c6\ub2e4."}, {"op": "replace", "path": "/shortdescription", "value": "\uae08\uc18d \ucc45\uc7a5"}]