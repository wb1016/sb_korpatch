[{"op": "replace", "path": "/avianDescription", "value": "\uc11d\uc0c1\uc774\uae34 \ud558\uc9c0\ub9cc, \ucc28\ub77c\ub9ac \ud3c9\uc18c\ubcf4\ub2e8 \uc774 \ud50c\ub85c\ub780\uc774 \ub354 \ud638\uac10\uc774 \uac00\ub124."}, {"op": "replace", "path": "/novakidDescription", "value": "\uc774 \uc78e\uc0ac\uadc0 \ubd80\ubd84\uc744 \uc870\uac01\ud558\ub294\uac74 \uc5c4\uccad \ud798\ub4e4\uc5c8\uaca0\ub2e4."}, {"op": "replace", "path": "/hylotlDescription", "value": "\uc11d\uc0c1\uc758 \ube48 \ub3cc\uc774 \ucc28\uac11\uace0 \uc8fd\uc5b4\uc788\ub294 \ud50c\ub85c\ub780\uc758 \ub208\uc744 \uc798 \ubb18\uc0ac\ud588\ub2e4."}, {"op": "replace", "path": "/humanDescription", "value": "\uc0ac\ub791\uc2a4\ub7ec\uc6b4 \ud50c\ub85c\ub780\uc774\ub2e4. \ub0b4\uac00 \uc774\ub7f0 \ub9d0\uc744 \ud560 \uc904\uc774\uc57c."}, {"op": "replace", "path": "/apexDescription", "value": "\uc0c1\ub0e5\ud574 \ubcf4\uc774\ub294 \ud50c\ub85c\ub780\uc774\ub2e4. \uc704\ud611\uc801\uc774\ub2e4."}, {"op": "replace", "path": "/glitchDescription", "value": "\ucd94\ub860. \uc774 \uc11d\uc0c1\uc740 \ud50c\ub85c\ub780\uc758 \uc9c0\ub3c4\uc790\ub97c \ubb18\uc0ac\ud55c \uac83 \uac19\ub2e4."}, {"op": "replace", "path": "/floranDescription", "value": "\uc6d0\uc608\uac00. \ubcc4\ub09c \ud50c\ub85c\ub780\uc774\ub2e4. \uc6c3\uae34 \ub0c4\uc100 \ub09c\ub2e4."}, {"op": "replace", "path": "/shortdescription", "value": "\ud50c\ub85c\ub780 \uc0ac\uc554 \uc11d\uc0c1"}, {"op": "replace", "path": "/description", "value": "\ud50c\ub85c\ub780\uc758 \uc11d\uc0c1\uc774\ub2e4. \ub2f9\uc2e0\uc744 \ucc0c\ub974\uc9c0\ub294 \uc54a\uc744\uac83\uc774\ub2e4... \uc544\ub9c8."}]