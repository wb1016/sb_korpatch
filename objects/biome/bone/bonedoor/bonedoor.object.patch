[{"op": "replace", "path": "/humanDescription", "value": "\ub9cc\uc9c0\uba74 \ucc28\uac11\ub2e4. \ub0c9\uae30\uac00 \ub0b4 \ucc99\ucd94\ub97c \ud0c0\uace0 \ud758\ub7ec\ub0b4\ub9b0\ub2e4. \uc800 \uba40\ub9ac \uc5b4\ub514\uc120\uac00 \uac1c\uac00 \uc9d6\ub294\ub2e4. \uc7a0\uae50, \uc774\uac70 \uadf8\ub0e5 \uc5d0\uc774\ud399\uc2a4 \ub178\ub798 \uac00\uc0ac\uc796\uc544."}, {"op": "replace", "path": "/novakidDescription", "value": "\uc8fd\uc74c\uc758 \ubb38."}, {"op": "replace", "path": "/hylotlDescription", "value": "\ubd88\uacbd\ud55c \uc8fd\uc74c\uc758 \ubb38\uc774\ub2e4. \uc774\ub9ac\ub3c4 \uae30\uad34\ud560 \uc218\uac00. \uadf8\ub098\ub9c8 \uacbd\ucca9\uc740 \uc544\uc8fc \ub9e4\ub044\ub7fd\uac8c \uc6c0\uc9c1\uc778\ub2e4."}, {"op": "replace", "path": "/avianDescription", "value": "\ubd84\uba85, \uc774\ub7f0 \ubb38 \ub4a4\uc5d0\ub294 \ubd84\uba85 \uc874\uacbd\ud560 \ub9cc\ud55c \ubb3c\uac74\uc774 \ub193\uc5ec\uc788\uaca0\uc9c0."}, {"op": "replace", "path": "/apexDescription", "value": "\ub290\ub08c\uc774 \uc601 \uc88b\uc9c0 \uc54a\uc740 \ubf08 \ubb38\uc774\ub2e4. \uc9c0\ub098\uac00\uae30\ub3c4 \ubb34\uc12d\ub2e4."}, {"op": "replace", "path": "/glitchDescription", "value": "\uc775\uc0b4. \uc774\ub7f0 \uac78 \uc800\uc2b9\uae38 \ubb38\ud131\uc774\ub77c\uace0 \ud558\ub294 \ubaa8\uc591\uc774\ub2e4."}, {"op": "replace", "path": "/floranDescription", "value": "\ud50c\ub85c\ub780 \ubb38\uc9dd\uc5d0\uc11c \ub3d9\uc9c8\uac10 \ub290\ub080\ub2e4. \ucc28\uc154 \uc5f0\ub2e4."}, {"op": "replace", "path": "/shortdescription", "value": "\ubf08 \ubb38"}, {"op": "replace", "path": "/description", "value": "\ubf08\ub85c \ub9cc\ub4e0 \ubb38\uc774\ub2e4. \uc5b4\ub514\ub85c \uac00\ub294 \ubb38\uc77c\uae4c? \uc9c0\uc625? \uc544\ub2c8\uba74 \uc695\uc2e4?"}]