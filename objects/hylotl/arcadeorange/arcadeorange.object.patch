[{"op": "replace", "path": "/glitchDescription", "value": "\uc7ac\ubbf8. \ucd5c\uace0\uc810\uc218\ub97c \uc5bb\uae30 \uc704\ud574 \uc21c\uc11c\ub300\ub85c \ubc84\uac70\ub97c \ub4a4\uc9d1\ub294 \uac74 \uc9c0\ub8e8\ud55c \uc77c\uc778\ub370\ub3c4 \uaf64 \uc911\ub3c5\uc801\uc774\ub2e4. 10\uc810 \ub9cc\uc810\uc5d0 7\uc810."}, {"op": "replace", "path": "/novakidDescription", "value": "\uc774 \uac8c\uc784\uc740 \ucd94\uc7a1\ud55c \ubc84\uac70 \uc601\uc6c5\ub4e4\uc774\ub2e4. \uc65c \ubc84\uac70\ub97c \ub4a4\uc9d1\ub294 \ucc99\ub9cc \ud560\uae4c? \ud670?"}, {"op": "replace", "path": "/avianDescription", "value": "\ucd94\uc7a1\ud55c \ubc84\uac70 \uc601\uc6c5\ub4e4\uc5d0\uc11c \uc88b\uc740 \uc810\uc218\ub97c \ub530\ub294 \ubc95\uc740 \ubc18\uc0ac\uc2e0\uacbd\uc744 \ucd5c\ub300\ud55c \uace4\ub450\uc138\uc6b0\ub294 \uac83 \ubfd0."}, {"op": "replace", "path": "/humanDescription", "value": "\ucd94\uc7a1\ud55c \ubc84\uac70 \uc601\uc6c5\ub4e4. \ubc84\uac70 \ub4a4\uc9d1\ub294 \uac8c\uc784\uc774\ub77c! \uc774\uc0c1\ud558\uac8c \ud765\ubbf8\uc9c4\uc9c4\ud558\ub124."}, {"op": "replace", "path": "/apexDescription", "value": "\ucd94\uc7a1\ud55c \ubc84\uac70 \uc601\uc6c5\ub4e4\uc740 \ubc84\uac70\ub97c \ub4a4\uc9d1\ub294 \uac8c\uc784\uc774\ub2e4. \ub098 \ubc84\uac70 \uc88b\uc740\ub370!"}, {"op": "replace", "path": "/hylotlDescription", "value": "\ucd94\uc7a1\ud55c \ubc84\uac70 \uc601\uc6c5\ub4e4\uc740 \uc5b4\ub9b0\uc560\ub4e4\uc744 \ubc14\uc058\uace0 \ub610 \uc7ac\ubc0c\uac8c \ud574\uc8fc\uae30 \uc88b\uc740 \uac8c\uc784\uc774\ub2e4."}, {"op": "replace", "path": "/floranDescription", "value": "\uace0\uae30 \ub4a4\uc9d1\ub294 \uac8c\uc784. \ud50c\ub85c\ub780 \ubc30\uace0\ud504\ub2e4."}, {"op": "replace", "path": "/shortdescription", "value": "\uc8fc\ud669 \uc624\ub77d\uc2e4 \uac8c\uc784\uae30"}, {"op": "replace", "path": "/description", "value": "\uc774 \uae30\uacc4\ub294 '\ucd94\uc7a1\ud55c \ubc84\uac70 \uc601\uc6c5\ub4e4'\ub77c\ub294 \uac8c\uc784\uc744 \uac00\ub3d9\ud55c\ub2e4."}]