[{"op": "replace", "path": "/avianDescription", "value": "\uc880\ube44 \ubc14\ub098\ub098 \uc800\ud0dd. \uc800\ud0dd\uc5d0 \ubcf4\uad00\ub41c \ubc14\ub098\ub098\ub97c \uc9c0\ud0a4\ub824\uace0 \uc880\ube44\ub97c \uc3f4\uc8fd\uc778\ub2e8 \ub9d0\uc774\uc9c0. \uc7ac\ubc0c\ub294 \ucee8\uc149\uc774\ub124."}, {"op": "replace", "path": "/novakidDescription", "value": "\uc880\ube44 \ubc14\ub098\ub098 \uc800\ud0dd? \uc774\uc81c \uc880\ube44 \uac8c\uc784\uc740 \ub2e4 \ud574\ubd24\uad6c\ub9cc."}, {"op": "replace", "path": "/hylotlDescription", "value": "\ub204\uad70\uac00 \uc9d1\uc744 \uc9c0\ud0a4\ub824\ub294 \uc2ec\uc815\uc740 \uc774\ud574\ud558\uaca0\ub2e4\ub9cc, \uc880\ube44 \ubc14\ub098\ub098 \uc800\ud0dd\uc740 \uc9c4\uc9dc \uc794\uc778\ud55c \uac8c\uc784\uc774\ub2e4."}, {"op": "replace", "path": "/floranDescription", "value": "\uc65c \uc9d5\uadf8\ub7ec\uc6b4 \ubc14\ub098\ub098 \uc9c0\ucf1c? \uc880\ube44\ub97c \uba39\uc5b4!"}, {"op": "replace", "path": "/humanDescription", "value": "\uc880\ube44 \ubc14\ub098\ub098 \uc800\ud0dd. \ubc14\ub098\ub098 \ubcf4\uc874\uc740 \uc5d0\uc774\ud399\uc2a4\uac00 \uac00\uc7a5 \uc911\uc694\uc2dc\ud558\ub294 \uac70\ub2c8\uae4c \uac54\ub4e4 \ud558\ub77c\uace0 \ub46c\uc57c\uaca0\ub2e4."}, {"op": "replace", "path": "/apexDescription", "value": "\uc880\ube44 \ubc14\ub098\ub098 \uc800\ud0dd\uc774\uc57c\ub9d0\ub85c \uace0\uc804 \uc5d0\uc774\ud399\uc2a4 \uac8c\uc784\uc774\uc9c0! \uc5b4\ub838\uc744 \ub550 \uc644\uc804 \ubb34\uc11c\uc6e0\ub294\ub370."}, {"op": "replace", "path": "/glitchDescription", "value": "\ud610\uc624.\uc880\ube44 \ubc14\ub098\ub098 \uc800\ud0dd\uc5d0 \ub4f1\uc7a5\ud558\ub294 \uc0dd\ubb3c\ub4e4\uc774 \ud604\uc2e4\uc5d0\ub3c4 \uc874\uc7ac\ud560\uae4c?"}, {"op": "replace", "path": "/shortdescription", "value": "\ub178\ub780 \uc624\ub77d\uc2e4 \uac8c\uc784\uae30"}, {"op": "replace", "path": "/description", "value": "\uc774 \uae30\uacc4\ub294 '\uc880\ube44 \ubc14\ub098\ub098 \uc800\ud0dd'\ub77c\ub294 \uac8c\uc784\uc744 \uac00\ub3d9\ud