[{"op": "replace", "path": "/glitchDescription", "value": "\uc5b4\ub824\uc6c0. \uc18c\ub9ac\uc9c0\ub974\ub294 \ud328\uc158 \uc694\uc6d0\uc774\ub77c\ub294 \uac8c\uc784\uc778\ub370 \uaf64 \uc801\uc751\ud558\uae30 \ud798\ub4e4\ub2e4. 10\uc810 \ub9cc\uc810\uc5d0 6\uc810."}, {"op": "replace", "path": "/shortdescription", "value": "\ubcf4\ub77c \uc624\ub77d\uc2e4 \uac8c\uc784\uae30"}, {"op": "replace", "path": "/description", "value": "\uc774 \uae30\uacc4\ub294 '\uc18c\ub9ac\uc9c0\ub974\ub294 \ud328\uc158 \uc694\uc6d0'\ub77c\ub294 \uac8c\uc784\uc744 \uac00\ub3d9\ud55c\ub2e4."}, {"op": "replace", "path": "/novakidDescription", "value": "\uc774 \ud328\uc158 \uc694\uc6d0\uc778\uc9c0 \ubb54\uc9c0 \uc18c\ub9ac\ub294 \uc9c4\uc9dc \ub9ce\uc774 \uc9c0\ub974\ub124. \uc65c \uc774\ub7ec\ub294 \uc9c0 \uc54c\uc544\uc57c \ud558\ub098?"}, {"op": "replace", "path": "/hylotlDescription", "value": "\uc18c\ub9ac\uc9c0\ub974\ub294 \ud328\uc158 \uc694\uc6d0 \uac8c\uc784\uc740 \uc0ac\uc2e4 \uc2e4\uc81c \ubc94\uc8c4 \uc774\uc57c\uae30\ub97c \uc608\uc220\uc801\uc73c\ub85c \uc7ac\uad6c\uc131\ud55c \uac8c\uc784\uc774\ub77c\ub294 \ub8e8\uba38\uac00 \uc788\uc5c8\ub2e4."}, {"op": "replace", "path": "/avianDescription", "value": "\uc18c\ub9ac\uc9c0\ub974\ub294 \ud328\uc158 \uc694\uc6d0\uc774\ub77c\ub294 \ucd94\ub9ac \uac8c\uc784\uc774\ub2e4. \uc65c \uc18c\ub9ac\uc9c0\ub974\ub294 \uac74\ub370? \uadf8 \uc774\uc720\ub97c \ucd94\ub9ac\ud574\uc57c \ud558\ub098?"}, {"op": "replace", "path": "/humanDescription", "value": "\ud328\uc158 \uc694\uc6d0\ub4e4\uc774 \ub2e4\ub4e4 \uc18c\ub9ac\uc9c0\ub974\ub294 \ud328\uc158 \uc694\uc6d0\uc758 \uadf8\ub188\ucc98\ub7fc \ubb34\uc12d\ub098? \uc560\ucd08\uc5d0 \ud328\uc158\uc694\uc6d0\uc774\ub77c\ub294 \uac8c \ubb54\ub370?"}, {"op": "replace", "path": "/apexDescription", "value": "\uc18c\ub9ac\uc9c0\ub974\ub294 \ud328\uc158 \uc694\uc6d0\uc774\ub77c\ub294 \uac8c\uc784\uc774\ub2e4. \uc5ec\uae30\uc11c \ubcf8 \uac83\ub4e4... \uc808\ub300 \uc78a\uc9c0 \ubabb \ud560 \uac70\ub2e4."}, {"op": "replace", "path": "/floranDescription", "value": "\uc18c\ub9ac\uc9c0\ub974\ub294 \ud328\uc158 \uc694\uc6d0\uc774\ub77c\ub294 \uac8c\uc784\uc774\ub2e4. \ud50c\ub85c\ub780 \uc774 \uac8c\uc784 \uc88b\ub2e4."}]