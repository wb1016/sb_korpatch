[{"op": "replace", "path": "/avianDescription", "value": "\uc774 \ubcf4\uad00\ud568\uc740 \uac00\uc8fd\uc744 \ub298\uc5ec \ub9cc\ub4e4\uc5b4\uc84c\uc5b4. \uc555\ub825\uc774 \uac00\ud574\uc9c0\uba74 \ub354 \ub298\uc5b4\ub0a0\uaebc\uc57c."}, {"op": "replace", "path": "/novakidDescription", "value": "\ubcf4\uad00\ud568\uc5d0 \uc4f0\uae30\uc5d0\ub294 \uaf64\ub098 \uadf8\ub7f4\uc2f8\ud55c \uc1a1\uacf3\ub2cc\ub370."}, {"op": "replace", "path": "/hylotlDescription", "value": "\uc0dd\uba85\uc744 \uc8fd\uc5ec\uc11c \ub2e4 \ub41c \ub514\uc790\uc778\uc5d0 \ucf54\ub97c \ud480\uc5b4\ub1a8\uad70."}, {"op": "replace", "path": "/apexDescription", "value": "\uadf8\uc744\ub9b0 \uac00\uc8fd\uacfc \uc1a1\uacf3\ub2c8\ub85c \ub9cc\ub4e0 \ubcf4\uad00\ud568\uc774\uc57c."}, {"op": "replace", "path": "/glitchDescription", "value": "\uac10\uba85. \ud601\uc2e0\uc801\uc778 \ub514\uc790\uc778\uc774\ub85c\uad70, \ud544\uc694\uc5d0 \uc758\ud574 \uac00\uc8fd\uc774 \ub298\uc5b4\ub09c\ub2e4\ub2c8."}, {"op": "replace", "path": "/floranDescription", "value": "\ud50c\ub85c\ub780 \uac00\uc8fd \ubcf4\uad00\ud568 \uc88b\uc544\ud574. \ubcf4\uad00\ud568\uc774 \uaf49 \ucc28\uba74 \ube75\ube75\ud574\uc838."}, {"op": "replace", "path": "/description", "value": "\ub124\uac1c\uc758 \ucee4\ub2e4\ub780 \uc1a1\uacf3\ub2c8 \uc0ac\uc774\uc5d0 \uac00\uc8fd\uc744 \ubed7\uccd0 \ub9cc\ub4e0 \ubcf4\uad00\ud568\uc774\ub2e4."}, {"op": "replace", "path": "/humanDescription", "value": "\ucc45\uc7a5 \uc790\uccb4\ub97c \uac00\uc8fd\uc73c\ub85c \ub9cc\ub4e4\uc5c8\uc5b4. \uba4b\uc9c4\ub370."}, {"op": "replace", "path": "/shortdescription", "value": "\uac70\ub300\ud55c \uc6d0\uc2dc \ubcf4\uad00\ud568"}]