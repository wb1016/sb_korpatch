[{"op": "replace", "path": "/hylotlDescription", "value": "\ud50c\ub85c\ub780\uc774 \ud39c\uc744 \ub4e4\uc5c8\ub2e4\uace0? \uc9c4\ud654\uc758 \uc2dc\uc791\uc774 \ub418\ub294 \uac74\uac00."}, {"op": "replace", "path": "/avianDescription", "value": "\uc774\uac74... \uc544\ube44\uc548 \uc0c1\ud615 \ubb38\uc790\uc640 \ub9ce\uc774 \ub2ee\uc740 \uac83 \uac19\ub2e4. \ud50c\ub85c\ub780\uc774 \uc544\ube44\uc548\uc5b4\ub97c \ubc30\uc6b0\ub824\uace0 \ud558\ub294\uac74\uac00?"}, {"op": "replace", "path": "/novakidDescription", "value": "\uadf8\uc744\ub9b0 \uac00\uc8fd\uc5d0 \uc870\uc7a1\ud55c \ub099\uc11c\ub97c \ud574\ub1a8\ub2e4. \ubcc4\uac70 \uc544\ub0d0."}, {"op": "replace", "path": "/humanDescription", "value": "\ud50c\ub85c\ub780\uc758 \ubb38\uc790\uac00 \uc774\ub807\uac8c \uc0dd\uacbc\uc744 \uac70\ub77c\uace4 \uc0c1\uc0c1\ub3c4 \ubabb\ud588\ub294\ub370."}, {"op": "replace", "path": "/apexDescription", "value": "\ud50c\ub85c\ub780\uc758 \ubb38\uc790\ub294 \uc88b\uac8c \uccd0\uc918\ub3c4 \uc6d0\uc2dc\uc801\uc778 \uc218\uc900\uc774\ub2e4."}, {"op": "replace", "path": "/glitchDescription", "value": "\ud63c\ub780. \uc774 \uc0c1\ud615\ubb38\uc790\ub97c \ubc88\uc5ed\ud558\ub824 \uc560\ub97c \uc4f0\uace0 \uc788\ub2e4. \uc54c\uace0 \uc788\ub294 \uc5b8\uc5b4 \uc911 \uac00\uc7a5 \ube44\uc2b7\ud55c \uac83\uc740 \uc544\ube44\uc548 \ubb38\uc790\ub2e4."}, {"op": "replace", "path": "/floranDescription", "value": "\ud50c\ub85c\ub780 \uc774\uc81c \uc885\uc774\uc5d0 \uae00 \uc4f0\uae30 \uc2dc\uc791\ud55c\ub2e4."}, {"op": "replace", "path": "/shortdescription", "value": "\uc6d0\uc2dc \uc0c1\ud615\ubb38\uc790"}, {"op": "replace", "path": "/description", "value": "\ub204\uad70\uac00 \uac00\uc8fd \uc870\uac01\uc5d0 \uc0c1\ud615\ubb38\uc790\ub97c \uadf8\ub824\ub193\uc740 \uac83 \uac19\ub2e4."}]