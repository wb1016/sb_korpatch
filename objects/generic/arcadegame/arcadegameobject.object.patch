[{"op": "replace", "path": "/novakidDescription", "value": "\uba54\uc774\uc988\ubc14\uc6b4\ub4dc64? \ubbf8\uad81 \uadf8\uac70 \uc644\uc804 \uc2dc\uac04\ub0ad\ube44 \uc544\ub2c8\ub0d0."}, {"op": "replace", "path": "/hylotlDescription", "value": "\ud750\ub9bf\ud55c \ube5b\uc774 \ub4e4\uc5b4\uc624\ub294 \ud1b5\ub85c\ub97c \uc9c0\ub098 \uc790\uc720\ub85c\uc774 \ubc97\uc5b4\ub09c\ub2e4... \uba54\uc774\uc988\ubc14\uc6b4\ub4dc64\uc5d0\ub294 \uadf8 \ub2e8\uc21c\uc131\uc774 \uc544\ub984\ub2f5\ub2e4."}, {"op": "replace", "path": "/avianDescription", "value": "\uba54\uc774\uc988\ubc14\uc6b4\ub4dc64\ub294 \uc544\ube44\uc548 \ud300\uc5d0\uc11c \ub9cc\ub4e0 \uac70\ub2e4. \uc544\ube44\uc548 \ube44\ub514\uc624 \uac8c\uc784\uc774 \ub9ce\uc9c0 \uc54a\uc740\ub370... \ub0b4\uac00 \ud558\ub098 \ub9cc\ub4e4\uc5b4\ubcfc\uae4c."}, {"op": "replace", "path": "/humanDescription", "value": "\uba54\uc774\uc988\ubc14\uc6b4\ub4dc64\ub294 \uc9c0\uad6c\uc5d0\uc11c \ucc98\uc74c \ud50c\ub808\uc774\ud588\ub358 \uac8c\uc784\uc744 \ub5a0\uc624\ub974\uac8c \ud574\uc900\ub2e4. \ub9e4\ub825\uc801\uc774\uc57c."}, {"op": "replace", "path": "/apexDescription", "value": "\ub09c \uba54\uc774\uc988\ubc14\uc6b4\ub4dc64\ub294 \ubcc4\ub85c\ub2e4. \uc9c0\uc815\ubd80 \uc2e4\ud5d8\uc5d0 \uad00\ud55c \uad34\ub2f4\uc744 \ub4e4\uc740 \uc801\uc774 \uc788\uc5b4\uc11c... \ubbf8\uad81 \uc598\uae30\ub9cc \ub098\uc640\ub3c4 \uae30\uac81\ud55c\ub2e4."}, {"op": "replace", "path": "/glitchDescription", "value": "\uc758\uc9c0. \uc5b8\uc820\uac00 \uc774 \uc9c0\uc625\uac19\uc740 \ubbf8\uad81\uc744 \ubc97\uc5b4\ub098 \uba54\uc774\uc988\ubc14\uc6b4\ub4dc64\ub97c \uae68\uace0\uc57c \ub9d0\uaca0\uc5b4! 10\uc810 \ub9cc\uc810\uc5d0 8\uc810."}, {"op": "replace", "path": "/floranDescription", "value": "\ud50c\ub85c\ub780 \uba54\uc774\uc988\ubc14\uc6b4\ub4dc64 \uc644\uc804 \uc7ac\ubbf8\uc5c6\ub2e4. \ub2e4 \uae68\uace0 \uace0\uae30\ub3c4 \uc548 \uc900\ub2e4."}, {"op": "replace", "path": "/shortdescription", "value": "\uba54\uc774\uc988\ubc14\uc6b4\ub4dc64 \uae30\uae30"}, {"op": "replace", "path": "/description", "value": "\"\uba54\uc774\uc988\ubc14\uc6b4\ub4dc64\" \uac8c\uc784\uc744 \uc704\ud55c \uc624\ub77d\uc2e4 \uae30\uae30\uc774\ub2e4."}]