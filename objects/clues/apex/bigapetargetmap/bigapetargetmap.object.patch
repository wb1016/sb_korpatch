[{"op": "replace", "path": "/novakidDescription", "value": "\uc774 \ubcbd\ubcf4\uc5d4 \ube45 \uc5d0\uc774\ud504\uc758 \uac70\ucc98\uc5d0 \ub300\ud55c \uc815\ubcf4\uac00 \uc4f0\uc5ec\uc788\ub2e4. \uc5c4\uccad \ud654\ub09c \uce5c\uad6c\ub4e4\uc5d0\uac8c \uc801\ud78c \uac83 \uac19\ub2e4."}, {"op": "replace", "path": "/hylotlDescription", "value": "\uc774 \uc9c0\ub3c4\uc5d4 \ud718\uac08\uaca8 \uc4f4 \ub099\uc11c\uc640 \ube45 \uc5d0\uc774\ud504\uc758 \uc704\uce58\uc5d0 \ub300\ud55c \uc815\ubcf4\uac00 \uc801\ud600\uc788\ub2e4. \ub204\uad70\uac00 \ube45 \uc5d0\uc774\ud504\ub97c \ucc3e\uace0 \uc2f6\uc5b4\uc11c \uc548\ub2ec\uc774 \ub09c \ubaa8\uc591\uc774\ub2e4."}, {"op": "replace", "path": "/avianDescription", "value": "\ube45 \uc5d0\uc774\ud504\uac00 \uc5b4\ub514 \uc788\ub294\uc9c0\uc5d0 \uad00\ud55c \uc815\ubcf4\uc640 \uba54\ubaa8\uac00 \uc794\ub729 \ubd99\uc5b4\uc788\ub2e4."}, {"op": "replace", "path": "/humanDescription", "value": "\uc774 \uc9c0\ub3c4\uc5d4 \ube45 \uc5d0\uc774\ud504\uc758 \uac70\ucc98\uc5d0 \ub300\ud55c \uae00\uc774 \uc794\ub729 \uc801\ud600\uc788\ub2e4. \uc4f4 \uc0ac\ub78c\uc774 \ub3c5\uc7ac\uc790\uc5d0\uac8c \ubc18\uae30\ub97c \ub4dc\ub294 \uc0ac\ub78c\uc778\uac00\ubcf4\ub2e4."}, {"op": "replace", "path": "/apexDescription", "value": "\uc774 \uc9c0\ub3c4\uc5d4 \ud718\uac08\uaca8 \uc4f4 \uba54\ubaa8\uc640 \ube45 \uc5d0\uc774\ud504\uac00 \uc788\uc744 \ubc95\ud55c \uacf3\uc758 \uc88c\ud45c\uac00 \ubd99\uc5b4\uc788\ub2e4."}, {"op": "replace", "path": "/glitchDescription", "value": "\ud638\uae30\uc2ec. \uc774 \uc9c0\ub3c4\ub294 \uc5d0\uc774\ud399\uc2a4 \uc778\uad6c\uc758 \uc0ac\uc2e4\uc0c1 \uc9c0\ub3c4\uc790\uc778 \ube45 \uc5d0\uc774\ud504\ub97c \ucd94\uc801\ud558\ub294 \uc815\ubcf4\ub97c \ub2f4\uace0 \uc788\ub2e4."}, {"op": "replace", "path": "/floranDescription", "value": "\ud50c\ub85c\ub780 \uc774 \uc9c0\ub3c4 \uc5b4\ub514 \uc4f0\ub294\uc9c0 \uc798 \ubaa8\ub974\uaca0\ub2e4."}, {"op": "replace", "path": "/shortdescription", "value": "\ube45 \uc5d0\uc774\ud504 \ubaa9\ud45c \uc9c0\ub3c4"}, {"op": "replace", "path": "/description", "value": "\ube45 \uc5d0\uc774\ud504\ub97c \ucd94\uc801\ud558\ub294 \ub370 \uc4f0\uc778 \ub09c\uc7a1\ud55c \uc9c0\ub3c4\ub2e4. \uadf8\ub9b0 \uc0ac\ub78c\uc774 \uc5c4\uccad \ud654\ub09c \ubaa8\uc591\uc774\ub2e4."}]