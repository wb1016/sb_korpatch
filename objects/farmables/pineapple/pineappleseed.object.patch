[{"op": "replace", "path": "/novakidDescription", "value": "\uc774\ub807\uac8c \ubfb0\uc871\ud55c \ubb3c\uac74 \ub0b4\ubd80\ub294 \uc774\ub807\uac8c \ub2ec\ucf64\ud560 \uac70\ub77c \ub204\uac00 \uc0c1\uc0c1\uc774\ub098 \ud588\uaca0\uc5b4?"}, {"op": "replace", "path": "/hylotlDescription", "value": "\uc9c8\ub9b4 \ub9cc\ud07c \ub2ec\ub2e4. \uc800 \ubc14\ub2e4 \ubc11\uc5d0\uc11c\ub294 \ud30c\uc778\uc560\ud50c\uc744 \ub208 \uc53b\uace0 \ubd10\ub3c4 \ucc3e\uc744 \uc218 \uc5c6\ub2e4."}, {"op": "replace", "path": "/humanDescription", "value": "\ud30c\uc778\uc560\ud50c. \ud6c4\uc2dd\uc73c\ub85c \uba39\uac70\ub098, \ud53c\uc790\uc5d0 \uc62c\ub9ac\uac70\ub098!"}, {"op": "replace", "path": "/apexDescription", "value": "\ud30c\uc778\uc560\ud50c\uc740 \ub2ec\ucf64\ud558\uace0 \uc999\uc774 \ud48d\ubd80\ud558\ub2e4. \uc0c1\ucf8c\ud55c \uba39\uac70\ub9ac\ub2e4."}, {"op": "replace", "path": "/glitchDescription", "value": "\uc790\ubd80\uc2ec. \uae00\ub9ac\uce58\ub294 \ud30c\uc778\uc560\ud50c\uc744 \ud6c8\ub828\uc6a9 \ud5c8\uc218\uc544\ube44\ub85c \uc4f4\ub2e4\uace0 \ud55c\ub2e4."}, {"op": "replace", "path": "/floranDescription", "value": "\ud30c\uc778\uc560\ud50c \ud50c\ub85c\ub780 \uba38\ub9ac\ud1b5\ucc98\ub7fc \uc115\uacbc\ub2e4."}, {"op": "replace", "path": "/avianDescription", "value": "\uc78e\uc0ac\uadc0\ub294 \uaf2d \uae43\ud138\uac19\uc740\ub370, \uc804\ud600 \ubd80\ub4dc\ub7fd\uc9c0 \uc54a\ub2e4."}, {"op": "replace", "path": "/shortdescription", "value": "\ud30c\uc778\uc560\ud50c \uc528\uc557"}, {"op": "replace", "path": "/description", "value": "\ud30c\uc778\uc560\ud50c, \uc81c\uc77c \uba3c\uc800 \uc798\ub77c\uc11c \uc5f4\uc5b4\uc57c \ud55c\ub2e4."}]