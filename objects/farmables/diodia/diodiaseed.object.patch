[{"op": "replace", "path": "/apexDescription", "value": "\ub2e4\uc774\uc624\ub514\uc544\uac00 \uac00\uc7a5 \uba39\uae30 \uc88b\uc740 \uc74c\uc2dd\uc740 \uc544\ub2c8\ub354\ub77c\ub3c4 \uae30\ub974\uae34 \uc27d\ub2e4."}, {"op": "replace", "path": "/novakidDescription", "value": "\uad6c\ub9ac\ub9db \ub098\ub294 \ub2e4\uc774\uc624\ub514\uc544\ub294 \uae00\ub9ac\uce58\uc758 \ubaa8\ub4e0 \ubd84\ub178 \ub9db\uc774\ub2e4."}, {"op": "replace", "path": "/avianDescription", "value": "\ub2e4\uc774\uc624\ub514\uc544\ub294 \uaf2d \uacac\uacfc\ub958\ucc98\ub7fc \uc0dd\uacbc\ub294\ub370 \ub9db\uc740 \uc804\ud600 \ub2e4\ub974\ub2e4."}, {"op": "replace", "path": "/glitchDescription", "value": "\ubaa8\uc695. \ub2e4\uc774\uc624\ub514\uc544\ub294 \uc9c4\uc815\uc73c\ub85c \uacfc\uc18c\ud3c9\uac00\ub41c \uc791\ubb3c\uc774\ub2e4."}, {"op": "replace", "path": "/floranDescription", "value": "\ud53c \ub9db\uc774 \ub09c\ub2e4. \ud50c\ub85c\ub780 \ub300\uccb4\uc7ac\ub85c \ub2e4\uc774\uc624\ub514\uc544 \uc4f8 \uc288 \uc788\uaca0\ub2e4."}, {"op": "replace", "path": "/description", "value": "\ub2e4\uc774\uc624\ub514\uc544, \uad6c\ub9ac\ub9db\uc774 \ub098\ub294 \ubd80\ub4dc\ub7ec\uc6b4 \ubfcc\ub9ac \ucc44\uc18c."}, {"op": "replace", "path": "/hylotlDescription", "value": "\uc4f4 \ubfcc\ub9ac\ucc44\uc18c\ub2e4. \ud48d\ubbf8\ub97c \ub354\ud558\uae30\uc5d0 \uc88b\uaca0\ub2e4."}, {"op": "replace", "path": "/humanDescription", "value": "\uac10\uc790\ucc98\ub7fc \uc0dd\uacbc\ub294\ub370 \ub3d9\uc804 \ub9db\uc774 \ub09c\ub2e4. \uc190\ud574\ubcf4\ub294 \uc7a5\uc0ac\ub2e4."}, {"op": "replace", "path": "/shortdescription", "value": "\ub2e4\uc774\uc624\ub514\uc544 \uc528\uc557"}]