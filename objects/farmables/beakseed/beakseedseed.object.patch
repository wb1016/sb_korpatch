[{"op": "replace", "path": "/glitchDescription", "value": "\ucca0\ud559. \ubd80\ub9ac\uc528\uac00 \ubd80\ub9ac\ucc98\ub7fc \uc0dd\uae34 \uac78\uae4c, \ubd80\ub9ac\uac00 \ubd80\ub9ac\uc528\ucc98\ub7fc \uc0dd\uae34 \uac78\uae4c?"}, {"op": "replace", "path": "/hylotlDescription", "value": "\ubd80\ub9ac\uc528\ub294 \ub180\ub78d\ub3c4\ub85d \ub9db\uc788\ub2e4. \uc774\ucc98\ub7fc \ub9db\uc788\ub294 \ubd80\ub9ac\ub294 \uc138\uc0c1 \uc5b4\ub514\uc5d0\ub3c4 \uc5c6\uc744 \uac70\ub2e4."}, {"op": "replace", "path": "/shortdescription", "value": "\ubd80\ub9ac\uc528 \uc528\uc557"}, {"op": "replace", "path": "/apexDescription", "value": "\ubd80\ub9ac\uc528\ub294 \ube44\uc0c1\uc2dd\uc801\uc73c\ub85c \ubc14\uc0ad\ud558\uc9c0\ub9cc, \uadf8\ub9ac \uc88b\uc9c0\ub9cc\uc740 \uc54a\ub2e4."}, {"op": "replace", "path": "/novakidDescription", "value": "\uc2e4\uc218\ub85c \uc9c4\uc9dc \ubd80\ub9ac\ub97c \uba39\uace0 \uc2f6\uc5b4\ud558\ub294 \uc0ac\ub78c\uc740 \uc5c6\uaca0\uc9c0."}, {"op": "replace", "path": "/avianDescription", "value": "\uc794\uce6b\uc0c1\uc5d0\uc11c \ubd80\ub9ac\uc528\ub97c \uba39\ub294 \uc544\ube44\uc548\uc5d0 \ub300\ud55c \uc804\ub798\ub3d9\ud654\uac00 \uc788\uc5c8\ub2e4. \uadf8\uac70 \uc88b\uc544\ud558\ub294\ub370."}, {"op": "replace", "path": "/floranDescription", "value": "\ubd80\ub9ac\uc528 \ubc14\uc0e5\ud558\uc9c0\ub9cc \ub2e4\ub978 \uc8fc\ub465\uc774\ucc98\ub7fc \uc9c8\ucc99\uc774\uc9c0 \uc54a\ub294\ub2e4."}, {"op": "replace", "path": "/humanDescription", "value": "\uc544\ube44\uc548 \uba74\uc0c1\ucc98\ub7fc \uc0dd\uae34 \uc791\ubb3c\uc774\ub2e4. \uc880 \ubd88\uae38\ud55c\ub370."}, {"op": "replace", "path": "/description", "value": "\ubc14\uc0ad \ubc14\uc0ad\ud55c \ubd80\ub9ac\uc528\uac00 \uc790\ub780\ub2e4. \ucf67\uad6c\uba4d \ubd80\ubd84\uc774 \uac00\uc7a5 \ub9db\uc788\ub2e4\uace0 \ud55c\ub2e4."}]