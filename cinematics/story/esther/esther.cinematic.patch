[
 {
  "op": "replace", 
  "path": "/panels/5/text", 
  "value": "\uBC18\uAC11\uAD6C\uB098, \uC598\uC57C! \uB4E4\uACE0 \uC788\uB294 \uAC74 \uBB3C\uC9C8\uC870\uC791\uAE30\uB2C8? \uC88B\uC544, \uB108\uB294 \uC218\uD638\uC790\uB85C\uAD6C\uB098."
 },
 {
  "op": "replace", 
  "path": "/panels/6/text", 
  "value": "\uC720\uAC10\uC2A4\uB7FD\uAC8C\uB3C4... \uC544\uB9C8 \uC9C0\uAD6C\uAC00 \uB054\uCC0D\uD558\uAC8C \uBA78\uB9DD\uD55C \uC774\uD6C4 \uB9C8\uC9C0\uB9C9 \uC218\uD638\uC790\uACA0\uC9C0."
 },
 {
  "op": "replace", 
  "path": "/panels/7/text", 
  "value": "\uC6B0\uB9AC\uAC00 \uC9C0\uAE08 \uB9CC\uB098\uB294 \uAC83\uC740 \uC2AC\uD504\uAC8C\uB3C4 \uBD88\uAC00\uD53C\uD558\uB2E8\uB2E4..." 
 },
 {
  "op": "replace", 
  "path": "/panels/8/text", 
  "value": "\uB0B4 \uC774\uB984\uC740 ^orange;\uC5D0\uC2A4\uB354 \uBE0C\uB77C\uC774\uD2B8^white;\uB780\uB2E4. \uB300 \uC218\uD638\uC790\uC600\uC9C0."
 },
 {
  "op": "replace", 
  "path": "/panels/9/text", 
  "value": "\uB098\uB294 \uBA87 \uB144 \uC804\uC5D0 \uC740\uD1F4\uB97C \uD588\uB2E8\uB2E4. \uADF8\uAC74 \uC815\uB9D0 \uB09C\uAC10\uD55C \uACB0\uC815\uC774\uC5C8\uC9C0. \uD558\uC9C0\uB9CC \uB0B4 \uC5F0\uAD6C\uB294 \uBC29\uCE58\uB418\uC5B4\uC120 \uC548\uB410\uC5C8\uC5B4."
 },
 {
  "op": "replace", 
  "path": "/panels/10/text", 
  "value": "\uB09C \uC624\uB298\uB0A0\uC744 \uAC71\uC815\uD558\uAC8C \uB9CC\uB4DC\uB294 \uC624\uB7AB\uB3D9\uC548 \uC78A\uD600\uC9C4 \uAC83\uB4E4\uC744 \uBC1C\uACAC\uD588\uB2E8\uB2E4."
 },
 {
  "op": "replace", 
  "path": "/panels/13/text", 
  "value": "\uC624\uB798 \uC804, \uACBD\uC791\uC790\uB77C\uACE0 \uC54C\uB824\uC9C4 \uD55C \uAC15\uB825\uD55C \uC874\uC7AC\uAC00 \uC788\uC5C8\uACE0, \uADF8 \uC874\uC7AC\uB294 \uC6B0\uC8FC\uC758 \uADE0\uD615\uC744 \uC720\uC9C0\uD588\uC9C0."
 },
 {
  "op": "replace", 
  "path": "/panels/14/text", 
  "value": "\uACBD\uC791\uC790\uC758 \uC601\uD5A5\uB825 \uC544\uB798\uC5D0\uC11C, \uD654\uD569\uC774 \uAC00\uB4DD\uD588\uC5B4. \uC5B5\uAC81\uC758 \uC2DC\uAC04\uB3D9\uC548 \uADF8\uB7AC\uB2E8\uB2E4."
 },
 {
  "op": "replace", 
  "path": "/panels/16/text", 
  "value": "\uD558\uC9C0\uB9CC \uC774 \uACE0\uC694\uD568\uC5D0\uB3C4 \uB05D\uC774 \uC788\uC74C\uC774 \uB4DC\uB7EC\uB0AC\uB2E8\uB2E4. \uD55C \uC874\uC7AC\uAC00 \uB098\uD0C0\uB0AC\uC5B4. \uC99D\uC624\uC640 \uD30C\uAD34\uC758 \uC874\uC7AC\uAC00 \uB9D0\uC774\uC57C. \uBC14\uB85C \uD30C\uBA78\uC774\uC5C8\uC9C0."
 },
 {
  "op": "replace", 
  "path": "/panels/17/text", 
  "value": "\uD30C\uBA78\uC740 \uC5B4\uB5A0\uD55C \uD615\uD0DC\uC758 \uC0DD\uBA85\uB3C4 \uC6A9\uB0A9\uD560 \uC218 \uC5C6\uC5C8\uACE0, \uADF8\uB798\uC11C \uC6B0\uC8FC\uC5D0\uC11C \uD30C\uAD34\uB97C \uAC70\uB46C\uB4E4\uC774\uAE30 \uC2DC\uC791\uD588\uB2E8\uB2E4."
 },
 {
  "op": "replace", 
  "path": "/panels/18/text", 
  "value": "\uC774 \uC874\uC7AC\uB294 \uBAA8\uB4E0 \uC0DD\uBA85\uC744 \uC81C\uAC70\uD558\uAE30 \uC704\uD574\uC11C \uC628\uD798\uC744 \uB2E4\uD588\uACE0, \uC131\uACF5\uD560\uBED4 \uD588\uB2E8\uB2E4. \uACBD\uC791\uC790\uC758 \uD76C\uC0DD\uC774 \uC5C6\uC5C8\uB354\uB77C\uBA74 \uB9D0\uC774\uC9C0."
 },
 {
  "op": "replace", 
  "path": "/panels/20/text", 
  "value": "\uACBD\uC791\uC790\uB294 \uD30C\uBA78\uC744 \uB9C9\uC544\uC130\uACE0, \uADF8\uAC83\uC744 \uBD09\uC778\uD558\uB294 \uB370\uC5D0 \uC131\uACF5\uD588\uC5B4."
 },
 {
  "op": "replace", 
  "path": "/panels/21/text", 
  "value": "\uADF8\uB7EC\uB098 \uADF8\uB807\uAC8C \uD558\uBA74\uC11C, \uACBD\uC791\uC790\uB294 \uC790\uC2E0\uC758 \uD798\uC744 \uC644\uC804\uD788 \uB2E4 \uC368\uBC84\uB838\uB2E8\uB2E4."
 },
 {
  "op": "replace", 
  "path": "/panels/23/text", 
  "value": "\uB9C8\uC9C0\uB9C9 \uC5D0\uB108\uC9C0\uAC00 \uC18C\uBA78\uD558\uAE30 \uC804\uC5D0, \uACBD\uC791\uC790\uB294 \uC120\uD0DD\uBC1B\uC740 \uC5EC\uC12F \uC885\uC871\uC5D0\uAC8C \uADC0\uC911\uD55C \uC720\uBB3C\uC744 \uC120\uBB3C\uD588\uB2E8\uB2E4." 
 },
 {
  "op": "replace", 
  "path": "/panels/25/text", 
  "value": "\uC774 \uC5EC\uC12F \uC720\uBB3C\uC740 \uACB0\uD569\uD588\uC744 \uB54C \uD30C\uBA78\uB85C \uD5A5\uD558\uB294 \uBB38\uC744 \uC5EC\uB294 \uC5F4\uC1E0\uAC00 \uB418\uC9C0."
 },
 {
  "op": "replace", 
  "path": "/panels/26/text", 
  "value": "\uBD09\uC778\uC744 \uC9C0\uD0A4\uB294 \uC2DC\uC124\uC774 \uBB34\uB108\uC9C0\uACE0 \uC788\uB2E8\uB2E4. \uC6B0\uB9AC\uB294 \uBC18\uB4DC\uC2DC \uADF8 \uC720\uBB3C\uB4E4\uC744 \uBAA8\uC544\uC11C, \uD30C\uBA78\uC5D0 \uB3C4\uB2EC\uD558\uACE0, \uADF8\uAC83\uC744 \uC644\uC804\uD788 \uBC15\uBA78\uD574\uC57C\uB9CC \uD574."
 },
 {
  "op": "replace", 
  "path": "/panels/29/text", 
  "value": "\uC598\uC57C, \uC774\uAC74 \uBC1B\uC544\uB4E4\uC5EC\uC57C \uD560 \uAC83\uC774 \uB9CE\uACE0, \uB108\uC5D0\uAC8C \uC5C4\uCCAD\uB09C \uBD80\uD0C1\uC744 \uD558\uB294 \uAC83\uC774\uC9C0\uB9CC... \uB098\uB294 \uB124\uAC00 \uB3C4\uC640\uC92C\uC73C\uBA74 \uC88B\uACA0\uAD6C\uB098."
 },
 {
  "op": "replace", 
  "path": "/panels/30/text", 
  "value": "\uC720\uBB3C\uB4E4\uC740 \uC628 \uBCC4\uB4E4\uC5D0 \uD769\uC5B4\uC838 \uC788\uC5B4\uC11C, \uB098 \uD63C\uC790\uC11C\uB294 \uCC3E\uC544\uB0BC \uC218 \uC5C6\uB2E8\uB2E4."
 },
 {
  "op": "replace", 
  "path": "/panels/31/text", 
  "value": "\uB2E4\uB978 \uD589\uC131\uB4E4\uC774 \uC9C0\uAD6C\uC758 \uC804\uCCA0\uC744 \uBC1F\uAE30 \uC804\uC5D0, \uC6B0\uB9AC\uB294 \uAF2D \uC720\uBB3C\uC744 \uCC3E\uACE0 \uD30C\uBA78\uC744 \uBFCC\uB9AC\uBF51\uC544\uC57C \uD55C\uB2E8\uB2E4."
 }
]