[{"op": "replace", "path": "/panels/31/text", "value": "\ub2e4\ub978 \ud589\uc131\ub4e4\ub3c4 \uc9c0\uad6c\ucc98\ub7fc \ub418\uae30\uc804\uc5d0, \uc6b0\ub9ac\ub294 \ubc18\ub4dc\uc2dc \uc774\uac83\ub4e4\uc744 \ucc3e\uc544\uc11c \ud30c\uba78\uc744 \ud30c\uad34\ud574\uc57c \ud55c\ub2e8\ub2e4."}, {"op": "replace", "path": "/panels/30/text", "value": "\uc774 \uc720\ubb3c\uc740 \uc628 \uc131\uc6b4\uc5d0 \ud37c\uc838\uc788\uace0, \ub09c \uadf8\uac83\ub4e4\uc744 \ud63c\uc790\uc120 \ucc3e\uc744 \uc218 \uc5c6\ub2e8\ub2e4."}, {"op": "replace", "path": "/panels/29/text", "value": "\uc774 \uc77c\uc740 \uc2dc\uac04\uc774 \ub9ce\uc774 \uac78\ub9b4\uac70\ub780\ub2e4. \uc598\uc57c. \uac8c\ub2e4\uac00 \ub124\uac8c\ub294 \ub108\ubb34\ub098\ub3c4 \ud070 \ubd80\ud0c1\uc774\uaca0\uc9c0... \ud558\uc9c0\ub9cc \ub09c \ub124 \ub3c4\uc6c0\uc774 \ud544\uc694\ud558\ub2e8\ub2e4."}, {"op": "replace", "path": "/panels/26/text", "value": "\uc774 \ubd09\uc778\uc740 \uc57d\ud574\uc9c0\uace0 \uc788\ub2e8\ub2e4. \uc6b0\ub9b0 \ubc18\ub4dc\uc2dc \uc774 \uc720\ubb3c\uc744 \ubaa8\uc544\uc11c, \ud30c\uba78\uc5d0 \ub3c4\ub2ec\ud558\uace0, \uadf8\uac83\uc744 \ud30c\uad34\ud574\uc57c\ub9cc \ud574."}, {"op": "replace", "path": "/panels/25/text", "value": "\uc774 \uc5ec\uc12f \uc720\ubb3c\uc774 \ud55c\ub370 \ubb49\uce58\uac8c \ub418\uba74, \ud30c\uba78\ub85c \ud5a5\ud558\ub294 \ubb38\uc744 \uc5ec\ub294 \uc5f4\uc1e0\uac00 \ub41c\ub2e8\ub2e4."}, {"op": "replace", "path": "/panels/23/text", "value": "\ub9c8\uc9c0\ub9c9 \uc5d0\ub108\uc9c0\uac00 \uc0ac\ub77c\uc9c0\uae30 \uc804\uc5d0, \uad6c\ucd95\uc790\ub294 \uc5ec\uc12f \uac1c\uc758 \uc885\uc871\uc5d0 \uc18c\uc911\ud55c \uc720\ubb3c\uc744 \uc120\ubb3c\ud588\uc9c0."}, {"op": "replace", "path": "/panels/21/text", "value": "\ud558\uc9c0\ub9cc \uc774 \uac83\uc73c\ub85c \uc778\ud574, \uad6c\ucd95\uc790\ub294 \ud798\uc744 \ub2e4 \uc368\ubc84\ub838\ub2e8\ub2e4."}, {"op": "replace", "path": "/panels/20/text", "value": "\uad6c\ucd95\uc790\ub294 \ud30c\uba78\uc5d0 \ub9de\uc11c \uc2f8\uc6e0\uace0, \ud30c\uba78\uc744 \ubd09\uc778\ud558\ub294\ub370 \uc131\uacf5\ud588\uc9c0."}, {"op": "replace", "path": "/panels/18/text", "value": "\ud30c\uba78\uc740 \ubaa8\ub4e0 \uc0dd\uba85\uc744 \uc5c6\uc560\ubc84\ub9ac\ub824\uace0 \ud588\uace0, \uc131\uacf5\ud560 \uc218\ub3c4 \uc788\uc5c8\uc9c0\ub9cc, \uad6c\ucd95\uc790\uc758 \ud76c\uc0dd\uc73c\ub85c \ub05d\ub0b4 \uc2e4\ud328\ud588\uc5b4."}, {"op": "replace", "path": "/panels/17/text", "value": "\ud30c\uba78\uc740 \uc6b0\uc8fc\uc5d0 \uc5b4\ub5a0\ud55c \uc0dd\uba85\uccb4\ub3c4 \uc874\uc7ac\ud558\ub294 \uac83\uc744 \uc6d0\uce58 \uc54a\uc558\uace0, \uc804 \uc6b0\uc8fc\ub97c \ud30c\uad34\ud558\uae38 \uc6d0\ud588\ub2e8\ub2e4."}, {"op": "replace", "path": "/panels/16/text", "value": "\uadf8\ub7ec\ub098 \uc774 \ud3c9\uc628\ud568\uc740 \ud558\ub098\uc758 \uc99d\uc624\uc640 \ud30c\uad34\uc758 \uc874\uc7ac\uac00 \ub098\ud0c0\ub098\uba70 \uc720\ud55c\ud568\uc774 \uc785\uc99d\ub418\uc5c8\uc9c0. \uadf8\uac83\uc740 \ubc14\ub85c \ud30c\uba78\uc774\uc600\ub2e8\ub2e4."}, {"op": "replace", "path": "/panels/14/text", "value": "\uadf8\ub4e4\uc758 \uc601\ud5a5\uc73c\ub85c, \uc870\ud654\ub97c \uc774\ub8e8\uc5b4 \uc6b0\uc8fc\ub97c \ub2e4\uc2a4\ub838\uace0, \uc218\ubc31\uc5b5\ub144\uac04 \uc9c0\uc18d\ub418\uc5c8\ub2e8\ub2e4."}, {"op": "replace", "path": "/panels/13/text", "value": "\uc624\ub798\uc804\uc5d0, \uad6c\ucd95\uc790\ub77c\uace0 \ubd88\ub9ac\uc6b0\ub294 \uc54c \uc218 \uc5c6\ub294 \uc138\ub825\uc774 \uc788\uc5c8\uc9c0, \uadf8\ub4e4\uc740 \uc628 \uc6b0\uc8fc\uc758 \uade0\ud615\uc744 \uc720\uc9c0\ud588\ub2e8\ub2e4."}, {"op": "replace", "path": "/panels/10/text", "value": "\uacfc\uac70\uc5d0 \ubb34\uac70\uc6b4 \uc9d0\uc744 \uc9c0\uace0 \uc788\uc744\ub54c\ub294 \uc78a\uace0 \uc788\uc5c8\ub358 \uac83\uc744 \ubc1c\uacac\ud574\ub0c8\uc9c0."}, {"op": "replace", "path": "/panels/9/text", "value": "\ub098\ub294 \uba87\ub144\uc804\uc5d0 \uc5f0\ud569\uc744 \ud0c8\ud1f4\ud558\ub294 \ud798\ub4e0 \uacb0\uc815\uc744 \ub0b4\ub838\uc9c0; \ud558\uc9c0\ub9cc \ub0b4 \uc5f0\uad6c\uc5d0 \uc18c\ud640\ud558\uc9c4 \uc54a\uc558\ub2e8\ub2e4."}, {"op": "replace", "path": "/panels/8/text", "value": "\ub0b4 \uc774\ub984\uc740 ^orange;\uc5d0\uc2a4\ub354 \ube0c\ub77c\uc774\ud2b8^white;\ub780\ub2e4. \uc9c0\uae08\uc740 \uc740\ud1f4\ud588\uc9c0\ub9cc \ub300 \uc218\ud638\uc790\uc600\uc9c0."}, {"op": "replace", "path": "/panels/7/text", "value": "\ubd88\ud589\ud788\ub3c4 \uadf8 \uc0ac\uac74\uc73c\ub85c \uc778\ud574 \uc6b0\ub9ac\uac00 \uc774\ub807\uac8c \ub9cc\ub098\uac8c \ub418\uc5c8\uc9c0..."}, {"op": "replace", "path": "/panels/6/text", "value": "\ub9d0\uc744 \uaebc\ub0b4\uc11c \ubbf8\uc548\ud558\uc9c0\ub9cc, \uc9c0\ub09c\ubc88\uc5d0 \uc77c\uc5b4\ub09c... \uc9c0\uad6c\uac00 \ub054\ucc0d\ud558\uac8c \ud30c\uad34\ub41c \uac83..."}, {"op": "replace", "path": "/panels/5/text", "value": "\ubc18\uac11\uad6c\ub098, \uc598\uc57c! \ud639\uc2dc \ub124\uac00 \ub4e4\uace0 \uc788\ub294 \uadf8\uac83\uc740 \ubb3c\uc9c8 \uc870\uc791\uae30\uc774\ub2c8? \ud6cc\ub96d\ud574. \uadf8\ub807\ub2e4\uba74 \ub10c \uc218\ud638\uc790\ub85c\uad6c\ub098."}]