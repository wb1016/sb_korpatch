[{"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/0", "value": "\uacbd\uace0 \ud558\ub098\ub9cc \ud574\uc904\uac8c. \ub2e4\ub978 \uc0e4\ub0e5\uafbc\ub4e4\ub3c4 \uc774 \uc0e4\ub0e5 \uc758\uc2dd\uc5d0\uc11c \uc774\uae30\uace0 \uc2f6\uc5b4\uc154 \ub110 \ub9c9\uc73c\ub824 \ub4e4 \uac70\uc57c."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/2", "value": "\ub09c \ub2e4\ub978 \uc0e4\ub0e5\uafbc\ub4e4\uc774 \uba3c\uc800 \uc154\ub450\ub974\ub294 \ub3d9\uc548 \uc5ec\uae30\uc154 \uac04\uc2dd\uc774\ub098 \uba39\uace0 \uc788\uc73c\ub824\uace0. \uc7e4\ub4e4\uc740 \uc154\ub85c \uc330\uc6b0\ub2e4 \ub098\uac00\ub5a8\uc5b4\uc9c0\uaca0\uc9c0."}, {"op": "replace", "path": "/scriptConfig/dialog/boss/finish/default/default/0", "value": "\uac70\uc758 \ub2e4 \uc7a1\uc558\uc5b4. \ub05d\ub0b4\ubc84\ub824!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/13", "value": "\ud50c\ub85c\ub780\uc740 \ub098\uc058\uc9c0 \uc54a\uc544. \uc6b0\ub9ac \ubd80\uc871 \uc6d0\uc608\uac00\uac00 \uc115\uba85\uc744 \uc874\uc911\ud558\ub77c\uace0 \uadf8\ub7ac\uc5b4. \uc218\ub9ce\uc740 \uc774\ub4e4\uc774 \ubc30\uc6b0\uc9c0 \ubabb \ud55c \uad50\ud6c8\uc774\uc9c0."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/12", "value": "\ub098\ub3c4 \uc0e4\ub0e5\uc774 \uc88b\uc9c0\ub9cc, \uadf8\uac83\ub9cc\uc73c\ub860 \ucda9\ubd84\uce58 \uc54a\uc744 \ub54c\uac00 \uc788\uc5b4."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/9", "value": "\ub09c \ud50c\ub85c\ub780\ub4e4\uc5d0\uac8c \uc11c\ub85c \uc330\uc6b0\ub294 \uac8c \ucd5c\uc120\uc758 \uc218\ub294 \uc544\ub2c8\ub77c\ub294 \uac78 \uc54c\ub824\uc8fc\ub824\uace0 \uc0e4\ub0e5 \uc758\uc2dd\uc5d0 \ucc38\uac00\ud558\ub294 \uac70\uc57c."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/6", "value": "\ucd5c\uace0\uc758 \uc0e4\ub0e5\uafbc\uc774 \ub418\uba74 \uc0ac\ub78c\ub4e4\uc774 \ub124\uac8c \ubc14\ub77c\ub294 \uac8c \ub9ce\uc544\uc9c4\ub2e4\ub2c8\uae4c..."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/5", "value": "\ud50c\ub85c\ub780\ub4e4\uc740 \uac00\ub054 \ubcc4 \uc774\uc720 \uc5c6\uc774 \uc0ac\ub098\uc6cc\uc9c8 \ub54c\uac00 \uc788\uc5b4."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/3", "value": "\ub09c \ud56d\uc0c1 \uc6b0\uc8fc\ub97c \uaf2d \ubcf4\uace0 \uc2f6\uc5c8\ub294\ub370, \ub10c \uc5b4\ub514\uc11c \uc654\uc5b4?"}, {"op": "replace", "path": "/scriptConfig/dialog/endscene/smalltalk/default/default/0", "value": "\uc788\uc796\uc544, \uc544\uae4c \uadf8\uac70 \uc9c4\uc9dc \uc778\uc0f9\uae4a\uc5c8\uc5b4."}, {"op": "replace", "path": "/scriptConfig/dialog/endscene/follow/default/default/0", "value": "\ub9cc\uc871\uc2a4\ub7ec\uc6e0\uc5b4! \uac00\uc790."}, {"op": "replace", "path": "/scriptConfig/dialog/endscene/end/default/default/1", "value": "\uc0ac\uc2e4\uc740... \ucd95\ud558\ud574! \ub124 \uc2b9\ub9ac \ud2b8\ub85c\ud53c\uc5d0 \uacf5\uc9dc \ub204\ub8e8\uac00 \uc0e4\uc740\ud488\uc73c\ub85c \ub530\ub77c\uc624\uac8c \ub410\uc9c0 \ubb50\uc57c!"}, {"op": "replace", "path": "/scriptConfig/dialog/endscene/end/default/default/0", "value": "\uc5ec\ud0dc\uaecf \ud574\uc628 \uc77c \uc911\uc5d0\uc154 \uc81c\uc77c \uc7ac\ubc0c\ub294 \uc77c\uc774\uc5c8\uc5b4. \uc790\uae30\uad00\ub9ac \uc81c\ub300\ub85c \ud560 \uc904 \uc544\ub294 \uc0e4\ub78c\uc774 \uc606\uc5d0 \uc788\uc5b4\uc8fc\uba74 \ucc38 \uc548\uc2ec\ub41c\ub2e4\ub2c8\uae4c."}, {"op": "replace", "path": "/scriptConfig/dialog/boss/shellbroken/default/default/0", "value": "\uc678\ud53c\uac00 \ubd80\uc154\uc84c\uc5b4! \uc9c0\uae08\uc774 \uacf5\uaca9\ud560 \uae30\ud68c\uc57c!"}, {"op": "replace", "path": "/scriptConfig/dialog/boss/fleshy/default/default/0", "value": "\ub54c\ub824! \uc0ec\uc774 \ub098\uc640\uc788\uc744 \ub54c \ub54c\ub824!"}, {"op": "replace", "path": "/scriptConfig/dialog/startboss/start/default/default/0", "value": "\uc774\uac70 \uc7ac\ubc0c\uaca0\ub294\ub370."}, {"op": "replace", "path": "/scriptConfig/dialog/startboss/intro/default/default/0", "value": "\uc774\uac70 \uc791\ub144 \uc0e4\ub0e5\uac10\ubcf4\ub2e4 \ud6e8\uc52c \ud06c\uc796\uc544. \ub4dc\ub514\uc5b4! \uc81c\ub300\ub85c \ub41c \ub3c4\uc804\uc774\uc57c!"}, {"op": "replace", "path": "/scriptConfig/dialog/floranmissionstart/introduction/default/default/4", "value": "\uadf8\ub7ec\ub2e4 \ub300\ubd80\ubd84 \ub2e4 \uc8fd\uc5b4... \uc5b4\uca0c\ub4e0, \ud589\uc6b4\uc744 \ube4c\uc5b4!"}, {"op": "replace", "path": "/scriptConfig/dialog/floranmissionstart/introduction/default/default/3", "value": "\uc5ec\uae30, \ud50c\ub85c\ub780\uc740 \uc288 \ub144\ub3d9\uc548 \uae30\ub7c9\uc744 \ub2e6\uc544. \uc289\ub828\ub41c \ub300\uc7a5\uc7a5\uc774\uac00 \ucc3d\ub0a0\uc744 \uac08\uc544\ub0b4\ub294 \uac83\ucc98\ub7fc."}, {"op": "replace", "path": "/scriptConfig/dialog/floranmissionstart/introduction/default/default/2", "value": "\ud55c \ub9c8\ub514 \ud574\uc57c\uaca0\ub2e4. \uc774\uac70 \uc9c4\uc9dc \uc778\uc0f9\uae4a\uc5c8\uc5b4!  \ub124\uac00 \uc870\uac01\uc870\uac01 \ub098\uac70\ub098 \ud588\uc73c\uba74 \ub35c \uc778\uc0f9\uae4a\uc5c8\uaca0\uc9c0."}, {"op": "replace", "path": "/scriptConfig/dialog/floranmissionstart/introduction/default/default/1", "value": "\uc544, \ub098? \ub09c ^orange;\ub204\ub8e8^white;\uc57c. \uc0e4\ub0e5 \uc758\uc2dd \uc5ed\uc0e4\uc0f9 \ucd5c\uc5f0\uc1fc \ucc54\ud53c\uc5b8."}, {"op": "replace", "path": "/scriptConfig/dialog/floranmissionstart/introduction/default/default/0", "value": "\uc640! \uc678\uc9c0\uc778\uc774\ub2e4! \uc9c4\uc9dc \uc5ec\uae30\uc5d0 \uc0e4\ub0e5\ud558\ub7ec \uc628 \uac70\uc57c? \uc880 \uc774\uc0f9\ud558\uae34 \ud574\ub3c4 \uc6a9\uac10\ubb34\uc345\ud558\ub124."}, {"op": "replace", "path": "/scriptConfig/dialog/floranmissionstart/end/default/default/0", "value": "\ub2e4\ub978 \uc0e4\ub0e5\uafbc\ub4e4\uc744 \uc870\uc2ec\ud558\uace0!"}, {"op": "replace", "path": "/scriptConfig/dialog/floranmissionswitch/hello/default/default/0", "value": "\uc774\ubd10! \uc5ec\uae30\uc57c! \uc5ec\uae30\uae4c\uc9c0 \uc624\ub2e4\ub2c8 \ubbff\uae30\uc9c0\uac00 \uc54a\ub124. \uc798 \ud588\uc5b4."}, {"op": "replace", "path": "/scriptConfig/dialog/floranmissionswitch/goodbye/default/default/0", "value": "\uac70\uc758 \ub2e4 \uc628 \uac83 \uac19\uc544. \ub124\uac00 \uc785\ub9db\uc744 \ub3cb\uad88\uc8fc\uc5c8\uc74c \uc88b\uaca0\ub2e4!"}, {"op": "replace", "path": "/scriptConfig/dialog/floranmissionswitch/stuck/default/default/0", "value": "\uadf8\uc73c\ub7ec\uc5b8\ub370\uc5d0 \ub108 \uc9c0\uae08 \uac07\ud600\ubc84\ub838\ub124. \ube44\uaf2c\ub4ef\uc774 '\ucc9c\ucc9c\ud788 \ubc15\uc288'\uce58\uace0 \uc2f6\uc5b4\uc9c0\ub294\uac78. \uc790, \ub0b4\uac00 \uc5f4\uc5b4\uc904\uac8c."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/11", "value": "\ub09c \uc5f4\uc77c\uacf1 \uc5ec\ub984\uc744 \ubcf4\ub0c8\uc5b4."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/10", "value": "\uc6ec\ub9cc\ud558\uba74 \uc774 \uc0e4\uc544\uc544 \ud558\ub294 \uc1fc\ub9ac \uc548 \ub0b4\uace0 \uc2f6\uc740\ub370, \uac00\ub054\uc740 \ub098\ub3c4 \uc5b4\uca54 \uc288 \uc5c6\uc5b4."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/8", "value": "\ud50c\ub85c\ub780\uc774 \uc7a1\uc544\ubcf8 \uc9d0\uc2b9\ub4e4\ubcf4\ub2e4 \ud6e8\uc52c \ub300\ub2e8\ud55c \ub188\uc774 \uc788\ub2e4\ub294 \uc1fc\ubb38\uc740 \ub4e4\uc5c8\uc5b4."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/7", "value": "\uc800 \ubc16\uc5d4 \uc0e4\ub0ad\uac10\uc774 \ub354 \ub9ce\uc744 \uac70\uc57c."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/4", "value": "\uc5b8\uc820\uac00 \uc5ec\uae30\uc154 \ube60\uc838\ub098\uac08 \ubc29\ubc95\uc744 \ucc3e\uc544\uc57c\uaca0\uc5b4."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/1", "value": "\ub09c \uc100\uc331\ub54c\ubd80\ud130 \uc0e4\ub0e5\uc744 \uc2dc\uc791\ud574 \ubd09\uc624\ub9ac \uc2dc\uc808\ubd80\ud130 \uc2b9\uc2b9\uc7a5\uad6c\uc600\uc9c0."}]