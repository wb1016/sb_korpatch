[{"op": "replace", "path": "/scriptConfig/dialog/tonauac/giveRelic/default/default/1", "value": "\ud074\ub8e8\uc5d1\uc2a4\ub97c \uc12c\uae30\uae30 \uc704\ud574 \uc624\uc2e0 \uac70\uad70\uc694! \uadf8\uc758 \ud638\uc758\uc640 \uc9c0\ub3c4 \uc5c6\uc774 \uc5ec\uae30 \uc624\ub294 \uac74 \ubd88\uac00\ub2a5\ud588\uc744 \ud14c\ub2c8\uae4c\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/continue/default/default/0", "value": "\ub4e4\uc5b4\uc624\uc138\uc694! \uba38\ub9ac, \ub2e4\ub9ac \uadf8\ub9ac\uace0 \uc5d0.... \uadf8\ub0e5 \uc804\ubd80 \uc870\uc2ec\ud558\uc2dc\uace0\uc694. \uc5ec\uae34 \uc880 \uc704\ud5d8\ud560 \uc218\ub3c4 \uc788\ub2f5\ub2c8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/endTour/default/default/2", "value": "\uc704\uc5d0 \uc788\ub294 \ubc29\uc5d0\ub294 \uba4b\uc9c4 \ubb3c\uac74\ub4e4\uc774 \uc788\ub2f5\ub2c8\ub2e4, \ubcf4\uace0 \uc5ec\uae30\ub85c \ub2e4\uc2dc \ub098\uac00\uc2dc\uba74 \ub3fc\uc694. \uc81c\uac00 \ubb38\uc744 \uc5f4\uc5b4\ub4dc\ub9ac\uc8e0."}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/endTour/default/default/1", "value": "\uc77c\ub2e8 \ubc29\ubb38\uac1d\uc774 \ubcfc \uc218 \uc788\ub294 \uac74 \uc5ec\uae30\uae4c\uc9c0\ub78d\ub2c8\ub2e4. \uadf8 \uc774\ud6c4\ub294 \uad00\ub9ac\uc778\uacfc \uc704\ub300\ud55c \uc2e0\ub4e4\uaed8\uc11c\ub9cc \ub4dc\ub098\ub4e4 \uc218 \uc788\uc9c0\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/endTour/default/default/0", "value": "\uc790, \uc774\ubc88 \ud22c\uc5b4\uac00 \uc990\uac70\uc6e0\uae38 \ubc14\ub78d\ub2c8\ub2e4! \uc6d0\ud558\ub294 \ub9cc\ud07c \ub3cc\uc544\ub2e4\ub2c8\uc154\ub3c4 \ub418\uace0\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/giveRelic/default/default/3", "value": "\uc81c\uac00 \ub2f9\uc2e0\uacfc \ud568\uaed8\ud574\ub3c4 \ub420\uae4c\uc694? \uc804 \ud074\ub8e8\uc5d1\uc2a4\uc5d0\uac8c \uc77c\uc2e0\uc744 \ubc14\uce5c \uc5f4\uad11\uc801\uc778 \uc885\uc73c\ub85c\uc11c, \uc81c\uac00 \uac00\uce58 \uc788\ub2e4\ub294 \uac78 \ubcf4\uc5ec\ub4dc\ub9b4\uac8c\uc694!"}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/giveRelic/default/default/2", "value": "\uc774 \uc720\ubb3c\uc774 \ub2f9\uc2e0\uc744 \uc704\ud574 \uc788\uc5c8\ub2e4\ub294 \uc804\uc870\uaca0\uc9c0\uc694: \uc774 \uc544\ubcfc\ub77c\uc774\ud2b8 \uc218\uc815\uc740 \ud074\ub8e8\uc5d1\uc2a4\uc758 \uc758\uc9c0\ub97c \ub2f4\uace0 \uc788\ub2e4\uace0 \ud574\uc694!"}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/giveRelic/default/default/0", "value": "...\ubb34\uc5c7\uc744 \ud558\ub824\uace0 \ud558\uc168\ub294\uc9c0 \ub4dc\ub514\uc5b4 \uc54c\uaca0\uc2b5\ub2c8\ub2e4!"}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/feelingTired/default/default/1", "value": "\uc74c, \uc0c8\ub85c\uc6b4 \uce5c\uad6c\ub97c \ub9cc\ub4e4\uac8c \ub3fc\uc11c \uc815\ub9d0 \uc88b\uc558\uc5b4\uc694. \ud558\uc9c0\ub9cc \uc774 \uc5ec\ud589\uc740... \uc870\uae08... \ud53c\uace4\ud558\ub124\uc694..."}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/feelingTired/default/default/0", "value": "\uc81c.\ubc1c. \uae4a\uc219\ud788 \ub4e4\uc5b4\uac00\uc9c0 \ub9d0\uc544 \uc8fc\uc138\uc694. \uac70\uae34 \ub108\ubb34 \uc704\ud5d8\ud574\uc694..."}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/statueDescription/default/default/1", "value": "\uadf8 \uc0ac\ub78c\uc774 \uc774\uac78 \uc4f8 \uc77c\uc740 \ub354 \uc5c6\uaca0\ub124\uc694. \uc65c\ub0d0\uba74, \uc74c, \uc8fd\uc5c8\uc73c\ub2c8\uae4c\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/statueDescription/default/default/0", "value": "\uc774\uac74 \uc624\ub798\ub41c \uc544\ube44\uc548 \uc804\uc0ac\uc758 \uc870\uac01\uc0c1\uc774\ub124\uc694! \uc774\uac83\ub4e4\uc740 \uadf8\uc758 \ucc3d\uc774\uace0\uc694..."}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/doorOpened/default/default/1", "value": "\ub2f9\uc2e0\uc740 \uadf8\ucabd\uc744 \ud655\uc778\ud574 \uc8fc\uc138\uc694. \uc800\ub294 \uc774\ucabd\uc5d0\uc11c \ub098\uac00\ub294 \ubb38\uc744 \ucc3e\uc544\ubcfc\uac8c\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/doorOpened/default/default/0", "value": "\uc774 \ubb38\uc740 \uc81c\uac8c\ub294 \uc880 \ub108\ubb34 \uc791\ub124\uc694. \uadf8\ub798\ub3c4 \ubd84\uba85 \ub2f9\uc2e0\uc5d0\uac8c \ub531 \ub9de\uaca0\uc9c0\ub9cc\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/altarDescription/default/default/2", "value": "\uc138\uc0c1\uc5d0! \uac15\ud558\uace0\ub3c4 \uacbd\uc774\ub85c\uc6e0\ub124\uc694. \uadf8\ub807\uc9c0 \uc54a\ub098\uc694?"}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/altarDescription/default/default/1", "value": "\uc774 \uc0ac\uc6d0\uc740 \ud074\ub8e8\uc5d1\uc2a4 \uc2a4\uc2a4\ub85c\uaed8\uc11c \ub9cc\ub4e0 \uac83 \uc911 \ud558\ub098\uc600\uc5b4\uc694"}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/altarDescription/default/default/0", "value": "\uc5ec\uae34 \uc6b0\ub9ac\uc758 \ud76c\uc0dd\uc758 \uc81c\ub2e8\uc785\ub2c8\ub2e4. \uba4b\uc9c4 \uc608\uad70\uc694. \uc774 \ud648\ub4e4\uc740 \ud53c\ub97c \ub05d\uae4c\uc9c0 \uc2e4\uc5b4\ub098\ub974\uc8e0."}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/doorGreeting/default/default/2", "value": "\uc194\uc9c1\ud788 \ub9d0\ud574\uc11c, \uc5b4.. \uc800\ub3c4 \uae38\uc744 \uc783\uc5c8\uc5b4\uc694... \ud558\uc9c0\ub9cc \uc800\ub294 \ubd84\uba85 \uc6b0\ub9ac \ub458\uc774\ub77c\uba74 \ub3cc\uc544\uac00\ub294 \uae38\uc744 \ucc3e\uc744 \uc218 \uc788\uc744 \uac70\uc608\uc694!"}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/doorGreeting/default/default/1", "value": "\uc0ac\uc2e4, \uc800\ub3c4 \uc5ec\uae30 \uc788\uc73c\uba74 \uc548 \ub429\ub2c8\ub2e4, \uc774\ub807\uac8c \uae4a\uc219\uc774 \ub4e4\uc5b4\uc628 \uc801\uc740 \uc5c6\uc5c8\uac70\ub4e0\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/doorGreeting/default/default/0", "value": "\uc624, \uc5ec\uae30 \uacc4\uc168\uad70\uc694! \ub2f9\uc2e0\uc774 \uc548\uc804\ud558\uc2dc\ub2c8 \ub2e4\ud589\uc774\ub124\uc694! \uae38\uc744 \uc783\uc5b4\ubc84\ub9ac\uc2e0 \uac83 \uac19\ub124\uc694...\uc815\ub9d0 \uc5ec\uae30 \uacc4\uc2dc\uba74 \uc548 \ub3fc\uc694..."}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/maskDescription/default/default/1", "value": "\uc774\uac83\ub4e4\uc740 \ub9e4\uc6b0 \uc624\ub798\ub3fc\uc11c \uc800\ud76c\ub3c4 \uc5b4\ub290 \uc815\ub3c4 \ud754\uc801\uc744 \ub193\ucce4\uc5b4\uc694... \ud558\uc9c0\ub9cc \uc800\ud76c\ub294 \ud074\ub8e8\uc5d1\uc2a4\ub2d8\uc774 \uc758\uc2dd\uc6a9 \uac00\uba74\uc758 \uc5f4\ub82c\ud55c \ud32c\uc774\ub77c\ub294 \uac83\uc740 \ud655\uc2e4\ud788 \uc54c\uace0 \uc788\uc9c0\uc694!"}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/maskDescription/default/default/0", "value": "\uc774 \uc758\uc2dd\uc6a9 \uac00\uba74\uc740 \ub9e4\uc6b0 \uace0\ub300\uc758 \uac83\uc774\uace0, \ud074\ub8e8\uc5d1\uc2a4\ub2d8\uc758 \uc6b0\uc8fc\uc758 \ud798\uc774 \uae43\ub4e4\uc5c8\uc5b4\uc694. \uc6b0\ub9b0 \uadf8\ub807\uac8c \uc0dd\uac01\ud574\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/start/default/default/4", "value": "\uc800\ub294 \ub2f9\uc2e0\uc774 \ubb34\uc5b8\uac00\ub97c \uac00\uc838\uac08 \uc0dd\uac01\uc744 \ud558\uc9c0 \uc54a\uc744 \uac70\ub77c\ub294 \uac83\uc744 \uc54c\uc9c0\ub9cc, \ubaa8\ub4e0 \uc0ac\ub78c\uc774 \uc2e0\ub8b0\ud560 \ub9cc\ud55c \uc0ac\ub78c\ub4e4\uc740 \uc544\ub2c8\uc5d0\uc694. \ub9e4\uc6b0 \ud654\ub098\ub294 \uc77c\uc774\uc8e0..."}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/start/default/default/3", "value": "\ud55c \uac00\uc9c0 \uc77c\ub7ec\ub450\uc790\uba74, \uc81c\ubc1c \uc544\ubb34 \uac83\ub3c4 \ub9cc\uc9c0\uc9c0 \ub9c8\uc138\uc694. \uac10\uc774 \uc7a1\ud788\uc2dc\uaca0\uc9c0\ub9cc, \uc6b0\ub9ac\ub294 \uc608\uc804\uc5d0 \ub3c4\ub09c \ubb38\uc81c\ub85c \uace8\uce58\uac00 \uc544\ud508 \uc801\uc774 \uc788\uc5c8\uc5b4\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/start/default/default/2", "value": "\ud639\uc2dc \uc6b0\ub9ac\uc758 \ub180\ub77c\uc6b4 \uc2e0 \ud074\ub8e8\uc5d1\uc2a4\uc5d0 \ub300\ud574 \ubc30\uc6b0\uae30 \uc704\ud574 \uc624\uc168\ub098\uc694? \ub3c4\uc6c0\uc744 \ub4dc\ub9b4 \uc218 \uc788\uc5b4 \ub9e4\uc6b0 \uae30\uc058\ub124\uc694! \uc81c\uac00 \uc5ec\uae38 \ubcf4\uc5ec\ub4dc\ub9b4\uac8c\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/start/default/default/1", "value": "\uc2e0\uc804\uc5d0 \uc624\uc2e0\uac78 \ud658\uc601\ud569\ub2c8\ub2e4! \uc5ec\uae30\uc5d0 \uc624\uc2e0\uac78 \ud6c4\ud68c\ud558\uc9c0 \uc54a\uc744\uac70\uc5d0\uc694. \uc800\ub294^orange;\ud1a0\ub098\uc6b0\uc545^white;, \uc774 \uc2e0\uc804\uc758 \uad00\ub9ac\uc778\uc785\ub2c8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/start/default/default/0", "value": "\uc624!... \ubc29\ubb38\uac1d\uc774\ub2e4!"}, {"op": "replace", "path": "/scriptConfig/dialog/tonauac/end/default/default/0", "value": "\ud154\ub808\ud3ec\ud130\ub2e4! \uac10\uc0ac\ud569\ub2c8\ub2e4. \ud074\ub8e8\uc5d1\uc2a4\uc2dc\uc5ec! \uc624, \uadf8\ubd84\uc740 \uc815\ub9d0 \ub180\ub77c\uc6b0\uc154."}]