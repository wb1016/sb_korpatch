[{"op": "replace", "path": "/scriptConfig/dialog/koichihologram/6/default/default/0", "value": "\uc774 \uc55e\uc758 \ubc29\uc740 \uce68\uc218\ub410\uc2b5\ub2c8\ub2e4... \uc800\ub294 \uc218\uc601\uc744 \ud574\uc11c \uac74\ub108\uac14\uc2b5\ub2c8\ub2e4. \ub2f9\uc2e0\ub3c4 \uadf8\ub9ac \ud574\uc57c\ub420 \uac83 \uac19\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/dialog/koichihologram/7/default/default/0", "value": "\uc774\uac8c \ub9c8\uc9c0\ub9c9 \ud640\ub85c\uadf8\ub7a8 \ub514\uc2a4\ud50c\ub808\uc774\uc785\ub2c8\ub2e4. \uc870\uc2ec\ud558\uc2dc\uace0, \uace7 \ubcfc \uc218 \uc788\uac8c\ub418\uba74 \uc88b\uaca0\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/dialog/koichihologram/3/default/default/0", "value": "\ud639\uc2dc \uc5b4\ub460\uc18d\uc5d0\uc11c \uac78\ub824 \ub118\uc5b4\uc9c0\uc168\ub2e4\uba74 \uc8c4\uc1a1\ud569\ub2c8\ub2e4. \uc774 \ubc29\uc758 \ucc28\ub2e8\uae30\uac00 \uadfc\ucc98\uc5d0 \uc788\uc744\uac81\ub2c8\ub2e4..."}, {"op": "replace", "path": "/scriptConfig/dialog/koichihologram/5/default/default/1", "value": "\uadf8 \uc544\ub798\uc5d0 \ub0b4\ub824\uac08\ub54c \uc870\uc2ec\ud558\uc2ed\uc2dc\uc624. \uadf8 \uc0ac\ub78c\ub4e4 \ub09c\ud3ed\ud55c \uae61\ud328\ub4e4\uc785\ub2c8\ub2e4!"}, {"op": "replace", "path": "/scriptConfig/dialog/koichihologram/5/default/default/0", "value": "\ub3c4\uc11c\uad00\uc758 \ub3d9\uc791 \uc13c\uc11c\uac00 \ub2f9\uc2e0\uc758 \uc704\uce58 \ubc11\uc5d0\uc11c \uc2e0\ud638\ub97c \uac10\uc9c0\ud588\uc2b5\ub2c8\ub2e4..."}, {"op": "replace", "path": "/scriptConfig/dialog/koichihologram/2/default/default/1", "value": "\ud558\uc9c0\ub9cc \uc81c\uac00 \ub2f9\uc2e0\uc744 \ub3c4\uc640\ub4dc\ub9b4 \uc218 \uc788\uc2b5\ub2c8\ub2e4! \uc81c\uac00 \uc774 \uc624\ub798\ub41c \ud640\ub85c\uadf8\ub7a8 \ud504\ub85c\uc81d\uc158 \ub514\uc2a4\ud50c\ub808\uc774\ub85c \ub3c4\uc11c\uad00\uc744 \uac10\uc2dc\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/dialog/koichihologram/2/default/default/0", "value": "\uc774 \ub3c4\uc11c\uad00\uc740 \uc624\ub7ab\ub3d9\uc548 \ubc29\uce58\ub418\uc11c \uace8\uba38\ub9ac\ub97c \uc553\uace0\uc788\uc2b5\ub2c8\ub2e4. \uc720\uac10\uc2a4\ub7fd\uac8c\ub3c4 \uc880 \uba40\ub9ac \ub3cc\uc544\uc11c \uac00\uc154\uc57c \ud560 \uac83 \uac19\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/dialog/koichihologram/1/default/default/3", "value": "\uac15\uc694\ud558\uae34 \uc8c4\uc1a1\ud558\uc9c0\ub9cc, \uc81c\ubc1c \uc800\ub97c \uad6c\ud574\uc8fc\uc2ed\uc2dc\uc624! \uc81c\uac00 \uc774 \uc785\uad6c\ub97c \uc5f4\uaca0\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/dialog/koichihologram/1/default/default/2", "value": "\ub09c\ud3ed\ud55c \ubc94\uc8c4\uc790\ub188\ub4e4! \uc800\ub294 \uc9c0\ud558\uc2e4\ub85c \uc7ac\ube68\ub9ac \ub3c4\ub9dd\ucce4\uc2b5\ub2c8\ub2e4! \uc804 \uc9c0\uae08 \uadf8\ub4e4\uc774 \ud6d4\uce58\ub824\uace0 \ud558\ub294 \uc720\ubb3c\uacfc \ud568\uaed8 \uac07\ud788\uace0 \ub9d0\uc558\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/dialog/koichihologram/1/default/default/1", "value": "\uc8c4\uc1a1\ud569\ub2c8\ub2e4.. \uc81c \uc774\ub984\uc740 ^orange;\ucf54\uc774\uce58^White;;\uc785\ub2c8\ub2e4. \uc800\ub294 \uc704\ub300\ud55c \ud558\uc774\ub85c\ud2c0 \uc0ac\uc0c1\uac00\ubd84\ub4e4\uc744 \uc5f0\uad6c\ud558\ub824 \uc654\uc2b5\ub2c8\ub2e4. \uadf8\ub7f0\ub370... \uc774 \uc545\ub2f9\ub4e4\uc774 \ub098\ud0c0\ub0ac\uc9c0 \ubb61\ub2c8\uae4c!"}, {"op": "replace", "path": "/scriptConfig/dialog/koichihologram/1/default/default/0", "value": "\ud639\uc2dc \uac70\uae30 \ub204\uad6c \uc788\uc2b5\ub2c8\uae4c?! \ub3c4\uc11c\uad00\uc758 \ub3d9\uc791 \uc13c\uc11c\uc5d0 \ubb34\uc5b8\uac00 \uc7a1\ud614\uc2b5\ub2c8\ub2e4. \uc800\uc880 \ub3c4\uc640\uc8fc\uc2ed\uc2dc\uc624!"}, {"op": "replace", "path": "/scriptConfig/dialog/koichihologram/4/default/default/2", "value": "\ud639\uc2dc \uc6b0\ub9ac \uc885\uc871\uc774 \ud50c\ub85c\ub780 \ubb34\ub9ac\ub4e4 \uc190\uc5d0 \uc758\ud574 \uac70\uc758 \uba78\ub9dd \uc9c1\uc804\uae4c\uc9c0 \uac14\ub2e4\ub294 \uc0ac\uc2e4\uc744 \uc54c\uace0 \uacc4\uc168\uc2b5\ub2c8\uae4c?"}, {"op": "replace", "path": "/scriptConfig/dialog/koichihologram/4/default/default/1", "value": "\uc218\ucc9c\ub144\uac04\uc758 \ubb38\ud559, \uc608\uc220 \uadf8\ub9ac\uace0 \uc5d0\uc138\uc774."}, {"op": "replace", "path": "/scriptConfig/dialog/koichihologram/4/default/default/0", "value": "\uc774 \ub3c4\uc11c\uad00\uc774 \ud30c\uc190\ub41c \uac74 \uc54c\uace0\uc788\uc9c0\ub9cc, \uc5ec\uc804\ud788 \ub180\ub78d\uc9c0 \uc54a\uc2b5\ub2c8\uae4c? \uc790\uc6d0\uc774 \ub108\ubb34\ub098\ub3c4 \ub9ce\uc2b5\ub2c8\ub2e4!"}]