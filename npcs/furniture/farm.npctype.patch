[{"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/4", "value": "\uc2e0\uc120\ud55c \uc74c\uc2dd\uc740 \ud074\ub8e8\uc5d1\uc2a4\uac00 \uc900 \uc120\ubb3c\uc774\uc57c!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/1", "value": "\ud0dc\uc591\uc744 \ud1b5\ud574\uc11c \ud074\ub8e8\uc5d1\uc2a4\uc758 \ube5b\ub0a8\uc774 \uc6b0\ub9ac\uc758 \ub18d\uc791\ubb3c \uc704\uc5d0 \uc788\uc5b4! \uc2dd\ubb3c\ub4e4\uc774 \uc798 \uc790\ub77c\uac8c \ub3c4\uc640\uc904\uac70\uc57c."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/9", "value": "\ub354 \ub9ce\uc740 \uce90\ube44\ub2db\uc774 \uc788\uc744\uc218\ub85d, \ub354 \ub9ce\uc740 \uc790\uc5f0 \uc0dd\uc0b0\ubb3c\uc744 \uc9c0\ud0ac \uc218 \uc788\uc5b4!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/8", "value": "\ub0b4 \uc0dd\ubb3c\ubcf5\uc7a5\uc740 \uc790\uc5f0\uc5d0 \uac00\uae4c\uc774 \uac08 \uc218 \uc788\ub3c4\ub85d \ub3c4\uc640\uc918, \uadf8\ub9ac\uace0 \ud074\ub8e8\uc5d1\uc2a4\uc5d0\uac8c\ub3c4."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/7", "value": "\ub09c \uc790\uc5f0\uc5d0 \uac00\uae4c\uc774 \ub2e4\uac00\uac00\uc11c \uc0b4 \uc218 \uc788\ub3c4\ub85d \ud5cc\uc2e0\uc801\uc774\uc57c, \ub0b4 \uc637\uc870\ucc28\ub3c4 \uc0dd\ubb3c\uc774\uc9c0!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/6", "value": "\ud0dc\uc591\uc740 \uc2dd\ubb3c\uc5d0\uac8c \uc0b6\uc744 \uc8fc\uace0, \uc6b0\ub9ac\uc5d0\uac8c\ub3c4 \uc0b6\uc744 \uc918... \ud074\ub8e8\uc5d1\uc2a4\uac00 \ub180\ub78d\uc9c0 \uc54a\ub2c8?"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/5", "value": "\ubc84\uc12f\uacfc \uc528\uc557, \uc751 \uc918! \ub10c \ub354 \uac74\uac15\ud558\uac8c \uba39\uc5b4\uc57c\ud574."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/3", "value": "\ub098\ucc98\ub7fc \uc0ac\ub294 \uc0ac\ub78c\uc774 \ub9ce\uc544, \uac00\ub2a5\ud55c \ud55c \uc790\uc5f0\uc5d0 \uac00\uae4c\uc774 \uc0ac\ub824\uace0 \ud558\ub294 \uc0ac\ub78c\ub4e4\uc774\uc9c0."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/2", "value": "\ubaa8\ub4e0 \ub098\uc758 \uc790\uc5f0\uc2dd\ud488\uc744 \uc704\ud55c \ubd80\uc5cc\uc740 \ubaa8\ub450 \ub0b4\uac00 \ubd80\ud0c1\ud55c\uac70\uc57c."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/0", "value": "\ub10c \uc790\uc5f0\uc0dd\uc0b0\ubb3c\ub85c \ub108\uc758 \uc74c\uc2dd\uc744 \ubaa8\ub450 \ub9cc\ub4e4\uc5b4\uc57c\ud574!"}]