[{"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/8", "value": "\ucc3d\uc758\uc801. \uc774 \ud30c\uc774\ud504\ub97c \uc880\ub354 \ud655\uc7a5\ud558\uba74 \uc190\ub2d8\ub4e4\uc744 \uc704\ud55c \uc774\uce35 \uce68\ub300\ub97c \ub9cc\ub4e4 \uc218 \uc788\uaca0\uad70!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/7", "value": "\uac71\uc815. \uc77c\ubd80 \uc555\ub825 \uce21\uc815\uc774 \ud2c0\ub9b0\uac83 \uac19\uc740\ub370..."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/6", "value": "\uc9c0\uce68. \uc774 \ud30c\uc774\ud504 \ub4e4\uc744 \ud558\ub8e8\uc885\uc77c \ub2e6\uace0 \uc788\uc5c8\uc9c0\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/4", "value": "\uc2e4\ub9dd, \ub098 \uac19\uc774 \uc0c8\ub048\ud558\uac8c \uc785\uc740 \uc0ac\ub78c\uc740 \uc5c6\ub294\uac74\uac00?"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/5", "value": "\ucf8c\ud65c. \uc99d\uae30 \uae30\uacc4\uc758 \ubcf5\uc7a1\ud568\uc774 \ub108\ubb34 \uc0ac\ub791\uc2a4\ub7fd\uc9c0 \uc54a\ub098?"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/3", "value": "\uc0dd\uac01. \uc774 \uac15\ub825\ud55c \uc99d\uae30 \uae30\uc220\uc774\ub77c\uba74, \ud0c0\uc784 \uba38\uc2e0\uc744 \ub9cc\ub4e4 \uc218 \uc788\uc744\uc9c0\ub3c4 \ubab0\ub77c!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/2", "value": "\uc790\ub791. \uc774 \ub180\ub77c\uc6b4 \ubb3c\uac74\ub4e4\uc744 \uc880 \ubcf4\uc2dc\uc8e0!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/0", "value": "\ub9cc\uc871. \uc774 \ub18b\uc1e0 \uae30\uc220\uc740 \uadf8\ub807\uac8c \ubc1c\uc804\ud558\uc9c4 \uc54a\uc558\uc9c0\ub9cc, \uba4b\uc9c4 \ud488\uc0c8\ub97c \uac00\uc9c0\uace0 \uc788\uc73c\ub2c8\uae4c!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/9", "value": "\uac71\uc815. \uc0ac\uc2e4 \ub098\ub3c4 \uc774 \ud30c\uc774\u