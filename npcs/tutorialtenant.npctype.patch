[{"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/1", "value": "\uc0c1\uc778\uc774 \ub4e4\uc5b4\uc62c\ub9cc\ud55c \uc0c1\uc810\uc744 \ub9cc\ub4dc\uc2dc\uace0 \uc2f6\ub2e4\uba74, \uadf8\ub4e4\uc774 \uc4f8\ub9cc\ud55c \ucc3d\uace0\uac00 \ud544\uc694\ud569\ub2c8\ub2e4!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/3", "value": "\ub18d\uc791\ubb3c\uc740 \uc2a4\uc2a4\ub85c \ud0a4\uc6b0\uc2dc\ub294\uc9c0\uc694? \uad2d\uc774\ub97c \ub9cc\ub4dc\uc2dc\uba74 \ub2f9\uc2e0\ub3c4 \ud560 \uc218 \uc788\uc744\uac81\ub2c8\ub2e4!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/16", "value": "\uc9c0\uad6c\uc5d0 \uc548\uac00\ubcf8\uc9c0\uac00 \ud55c\ucc38 \ub42c\uad70\uc694! \uc694\uc998\uc5d4 \uc5b4\ub5bb\uac8c \ub3cc\uc544\uac00\ub098\uc694?"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/5", "value": "\uaf43\uc774\ub098 \uc794\ub514\ub97c \ud0a4\uc6b8\ub824\uba74 \ubb3c\ubfcc\ub9ac\uac1c\ub97c \uad6c\ud558\uc138\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/2", "value": "\uc0ac\ub78c\ub4e4\uc774 \ucda9\ubd84\ud788 \uc624\ub79c \uc2dc\uac04\ub3d9\uc548 \ub2f9\uc2e0\uc758 \uc9d1\uc5d0\uc11c \uc0b0\ub2e4\uba74, \uc544\ub9c8 \uc120\ubb3c\uc744 \uc904\uac81\ub2c8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/0", "value": "\uba4b\uc9c4 \uacf3 \uc774\uad70\uc694! \ub9c8\uce58 \uace0\ud5a5 \uac19\uc740\ub370\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/0", "value": "\uadf8\ub0e5 \uc5ec\ud589\uae38\uc5d0 \uc788\uc2b5\ub2c8\ub2e4, \uc6d0\ud558\ub294\uac78 \ud558\uba74\uc11c\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/15", "value": "\uc694\uc998\uc5d4 \ub300\ucc44 \ucee4\ud53c \ud55c\uc794\uc744 \ub9cc\ub4dc\ub294\uac8c \uc65c\uc774\ub9ac \ud798\ub4e0\uac74\uc9c0..."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/14", "value": "\ud070 \uacf5\ub3d9\uc73c\ub85c \ub6f0\uc5b4\ub0b4\ub9ac\uace0 \uc2f6\uc9c0 \uc54a\ub2e4\uba74, \ub85c\ud504\ub97c \uc368\uc11c \ucc9c\ucc9c\ud788 \ub0b4\ub824\uac00\ub294\uac74 \uc5b4\ub5a4\uac00\uc694?"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/13", "value": "\ub204\uac00 \ubb34\uae30 \uc0c1\uc790\ub97c \uc628\uacf3\uc5d0 \uc124\uce58\ud574\ub450\ub294 \uac78\uae4c\uc694? \ubb34\ub2e8 \ud22c\uae30 \uc218\uc900\uc778\uac83 \uac19\uc740\ub370!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/12", "value": "\ub9cc\uc57d \ubaac\uc2a4\ud130\ub4e4\uc774 \uc785\uc8fc\uc790 \ub4e4\uc744 \uacf5\uaca9\ud55c\ub2e4\uba74, \uacbd\ube44\ubcd1\uc744 \ubd88\ub7ec\uc624\ub294\uac8c \uc88b\uc744\uac81\ub2c8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/11", "value": "\ud3ed\ud0c4\uc744 \ub2e4\ub8f0\ub54c\ub294 \uc870\uc2ec\ud558\uc138\uc694! \ub784\uae4c, \uadf8\ub7f0\ub9d0\uc744 \ud560 \ud544\uc694\uac00 \uc788\uae34 \ud560\ub824\ub098\uc694?"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/10", "value": "\uc77c\ubd80 \uc785\uc8fc\uc790\ub294 \uc9d1\uc5d0 \ub458 \uc7a5\uc2dd\ubb3c\uc744 \uad49\uc7a5\ud788 \uac00\ub9b0\ub2f5\ub2c8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/9", "value": "\uc544\ub9c8 \ub300\ubd80\ubd84\uc758 \ubc29\ub791 \ubaa8\ud5d8\uac00\ub4e4\uc740 \ud3c9\ud654\ub85c\uc6b4 \ud589\uc131\uc744 \uc704\uc8fc\ub85c \ub2e4\ub2d0\uac81\ub2c8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/8", "value": "\ud0e4\ub798\ud3ec\ud130\ub4e4\uc740 \ud589\uc131\uac04 \uc774\ub3d9\uc5d0\ub294 \uad49\uc7a5\ud788 \uc720\uc6a9\ud558\uc9c0\ub9cc, \uc5ed\uc2dc \ub9cc\ub4e4\uae30 \uc704\ud55c \ud0e4\ub798\ud3ec\ud130 \ucf54\uc5b4\ub97c \uad6c\ud558\uae30\uac00 \ud798\ub4e4\uc8e0."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/7", "value": "\uc785\uc8fc\uc790\uac00 \ubd88\ub9cc\uc774 \uc788\uc73c\uba74 \ubc29 \ube7c\uace0 \ub098\uac00\uae30 \uc804\uc5d0 \ub3c4\uc640\uc8fc\ub294\uac8c \uc88b\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/6", "value": "\uadf8 \uc774\uc0c1\ud55c \uad00\ubb38\ub4e4\uc740 \ub610 \ubb61\ub2c8\uae4c? \ud56d\uc0c1 \uac70\uae30\uc5d0 \uc788\uc5c8\ub358 \uac74\uac00\uc694?"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/4", "value": "\ub300\ubd80\ubd84\uc758 \uc785\uc8fc\uc790\ub294 \ubb38\uc774\ub098 \uad11\uc6d0\ucc44\uac00 \uc5c6\ub294 \uc9d1\uc5d0\ub294 \uc548\ub4e4\uc5b4 \uc62c\uac81\ub2c8\ub2e4!"}]