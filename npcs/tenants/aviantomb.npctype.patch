[{"op": "replace", "path": "/dialog/converse/avian/default/4", "value": "\uc6b0\ub9ac \uc544\ube44\uc548\ub4e4\uc740 \ud544\uba78\uc758 \uc138\uacc4 \ub108\uba38\uc5d0 \uc788\ub294 \ucc9c\uad6d\uc5d0 \uc0b4\uc558\ub2e4\uace0 \ud574\uc694. \uc5b8\uc820\uac00 \uc6b0\ub9ac\ub4e4\uc740 \ub3cc\uc544\uac00\uace0 \ub9d0 \uac70\uc5d0\uc694!"}, {"op": "replace", "path": "/dialog/converse/avian/default/3", "value": "\ub0a0\uae30\ub97c \uc6d0\ud55c\ub2e4\uba74, \ubaa8\ub450 \ud074\ub8e8\uc5d1\uc2a4\uc758 \ube5b\uc5d0\uc11c \uc0b4\uc544\uc57c \ud574\uc694."}, {"op": "replace", "path": "/dialog/converse/avian/default/2", "value": "\ubaa8\ub4e0 \ubcc4\uc774 \ud074\ub8e8\uc5d1\uc2a4\uc758 \ub208\uc774\ub77c\ub294 \uac78 \uc544\uc2dc\ub098\uc694? \uadf8\ub294 \ubaa8\ub4e0 \uac78 \uc9c0\ucf1c\ubcf8\ub2f5\ub2c8\ub2e4!"}, {"op": "replace", "path": "/dialog/converse/avian/default/1", "value": "\ub2f9\uc2e0\uc5d0\uac8c \uc77c\uc5b4\ub098\ub294 \ubaa8\ub4e0 \uc88b\uc740 \uc77c\uc740 \ud074\ub8e8\uc5d1\uc2a4\uc758 \ub355\uc774\uc5d0\uc694. \ub098\uc05c \uc77c\uc740 \ub2f9\uc2e0\uc758 \ud0d3\uc774\uace0\uc694."}, {"op": "replace", "path": "/dialog/converse/avian/default/0", "value": "\ud074\ub8e8\uc5d1\uc2a4\uc758 \uac00\ub974\uce68\uc744 \uc124\ud30c\ud558\ub7ec \uc654\uc5b4\uc694! \uac00\ub974\uce68\uc744 \ubc1b\uace0 \ub2f9\uc2e0\uc758 \ub0a0\uac1c\ub97c \uad6c\ud558\uc2dc\uae38!"}, {"op": "replace", "path": "/dialog/converse/avian/avian/4", "value": "\uc800 \ud558\ub298\ub85c \ub0a0\uae30\ub97c \ubc14\ub780\ub2e4\uba74, \ud074\ub8e8\uc5d1\uc2a4\uc758 \ub73b\uc744 \ub530\ub974\uc138\uc694!"}, {"op": "replace", "path": "/dialog/converse/avian/avian/3", "value": "\ubaa8\ub4e0 \ud0dc\uc591\uc774 \ud074\ub8e8\uc5d1\uc2a4\uc758 \ub208\uc774\ub77c\uace0 \ud558\uc8e0. \uadf8\ub294 \ubaa8\ub4e0 \uac78 \ubcf4\uc2dc\uc8e0!"}, {"op": "replace", "path": "/dialog/converse/avian/avian/2", "value": "\ub2f9\uc2e0\uc5d0\uac8c \uc77c\uc5b4\ub098\ub294 \ubaa8\ub4e0 \uc88b\uc740 \uc77c\uc740 \ud074\ub8e8\u