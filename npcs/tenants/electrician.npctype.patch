[{"op": "replace", "path": "/scriptConfig/dialog/converse/apex/default/1", "value": "\uc774 \ud68c\ub85c\ub294 \uc815\ub9d0 \uc798 \ub9cc\ub4e4\uc5b4\uc84c\uad70\uc694. \ud55c\ucc38 \ub3d9\uc548 \uc190\uc0c1\ub420 \uac83 \uac19\uc9c4 \uc54a\uad70\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/apex/default/0", "value": "\uc804 \uc5ec\uae30\uc11c \uc774 \ubc30\uc120\ub4e4\uc774 \uc190\uc0c1\ub418\uc9c0 \uc54a\ub3c4\ub85d \uc77c\ud558\uc9c0\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/glitch/default/2", "value": "\ub9cc\uc871. \uc804\uae30\uacf5\ud559\uc744 \ubc30\uc6cc\uc11c \ub2e4\ud589\uc774\ub77c\uace0 \uc0dd\uac01\ud558\uc624, \ud68c\ub85c\ub780 \uac74 \uc815\ub9d0 \ub9e4\ud639\uc801\uc774\ub77c\uc624."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/glitch/default/0", "value": "\ub180\ub78c. \ubd84\uba85 \ud68c\ub85c\ub97c \uace0\uce58\ub7ec \uc654\ub294\ub370, \ubaa8\uc870\ub9ac \uba40\uca61\ud558\uad6c\ub824."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/hylotl/default/1", "value": "\uc804\uae30 \ud68c\ub85c\ub294 \uc815\ub9d0\ub85c \ub9e4\ub825\uc801\uc774\uc9c0 \uc54a\uc544?"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/hylotl/default/0", "value": "\uc804\uae30 \uae30\uae30\ub97c \uace0\uce58\ub294 \uac74 \uc815\ub9d0 \uc608\uc220\uacfc\ub3c4 \uac19\uc740 \ubcf5\uc7a1\ud55c \uae30\uc220\uc774 \ud544\uc694\ud558\uc9c0. \ub2e4\ud589\ud788\ub3c4, \uc774 \ud68c\ub85c\ub4e4\uc740 \uba40\uca61\ud574 \ubcf4\uc774\ub294\uad70."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/floran/default/1", "value": "\ud50c\ub85c\ub780\uc774 \uc5b4\ub5bb\uac8c \uc804\uae30\uacf5\ud559\uc790\ub97c \ud558\uac8c \ub410\uc744\uae4c? \ucc28\ub77c\ub9ac \ucc0c\ub974\uace0 \ub2e4\ub2c8\uace0 \uc2f6\uc740\ub370."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/floran/default/0", "value": "\ud50c\ub85c\ub780\uc774 \uc804\uae30\uacf5\ud559\uc790\uae34 \ud55c\ub370, \ud50c\ub85c\ub780\uc740 \uc804\uae30\uac00 \uc5b4\ub5bb\uac8c \uc791\ub3d9\ud558\ub294\uc9c4 \ubab0\ub77c."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/apex/default/2", "value": "\ud68c\ub85c\uac00 \ub118\uccd0\ub098\ub294 \uacf3\uc774\ub77c\uba74 \uc5b4\ub514\ub4e0 \uac00\ubcf4\uace0 \uc2f6\uc9c0\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/glitch/default/1", "value": "\uc5f4\ub9dd. \ud68c\ub85c\uc640 \uc804\uc790 \uae30\uae30\ub97c \uc720\uc9c0 \ubcf4\uc218\ud558\ub294\uac78 \uc88b\uc544\ud558\uc9c0! \uc9c1\uc5c5\uc774\uae30\ub3c4 \ud558\uace0, \ucde8\ubbf8\uc774\uae30\ub3c4 \ud558\ub124."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/hylotl/default/2", "value": "\uc804\uae30\uacf5\ud559\uc790\ub294 \uc608\uc220\uac00\uc640 \uac19\uc774 \uace0\uacb0\ud55c \uc0ac\ub78c\ub4e4\uc774\ub77c\uace0. \ub18d\ub2f4\ud558\ub294 \uac70 \uc544\ub2c8\uc57c!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/floran/default/2", "value": "\ud50c\ub85c\ub780\uc740 \uc774 \uc804\uae30\uacf5\ud559\uc790 \ubaa8\uc790\uac00 \uc330\uc6c0\uc5d0 \ud6a8\uacfc\uc801\uc77c\uc9c0 \uad81\uae08\ud574."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/2", "value": "\uc774 \ud68c\ub85c\ub4e4\uc744 \ucd5c\uace0\uc758 \uc0c1\ud0dc\ub85c \uc720\uc9c0\ud558\uae30 \uc704\ud574\uc11c \uc77c\ud558\uace0 \uc788\uc9c0\uc694!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/1", "value": "\uc77c\ud558\ub294 \uc774\uc720\uc694? \ud68c\ub85c\ub4e4\uc774 \uc54c\uc544\uc11c \uc790\uc2e0\ub4e4\uc744 \uace0\uce58\uc9c4 \uc54a\uc73c\ub2c8\uae4c\uc694!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/0", "value": "\uc804 \uc804\uae30\uacf5\ud559\uc790\ub78d\ub2c8\ub2e4! \uc5ec\uae30 \uc788\ub294 \ubaa8\ub4e0 \ud68c\ub85c\uac00 \uc815\uc0c1\uc801\uc73c\ub85c \uc791\ub3d9\ud558\ub3c4\ub85d \uc720\uc9c0\ud558\uace0 \uc788\uc5b4\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/avian/default/2", "value": "\ubd84\uba85 \ud68c\ub85c\ub97c \uace0\uce58\ub7ec \uc654\ub294\ub370, \ubaa8\ub450 \uba40\uca61\ud574 \ubcf4\uc774\ub294 \uac83 \uac19\uc740\ub370."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/avian/default/1", "value": "\uc774 \uc810\ud504\uc288\ud2b8\ub294 \ubcc4\ub85c \ubcf4\uae30 \uc548 \uc88b\uad70\uc694. \uc544\ube44\uc548\uc740 \uc774\ub807\uac8c \ubab8\uc744 \ub9ce\uc774 \uac00\ub9ac\uba74 \uc548 \ub418\ub294\ub370!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/avian/default/0", "value": "\uc81c \ubaa8\uc790\uac00 \uae43\ud138\uc744 \uacc4\uc18d \uac04\uc9c0\ub7fd\ud788\ub294\uad70\uc694."}]