[{"op": "replace", "path": "/scriptConfig/dialog/converse/apex/default/2", "value": "\uc2e4\ud5d8\uc744 \uc880 \ud558\uace0 \uc788\uc9c0, \ub9c8\uce68 \uc774\uacf3\uc774 \uadf8\ub7ec\uae30 \ub531 \uc88b\ub354\uad70."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/apex/default/1", "value": "\ud544\ub4dc \ud14c\uc2a4\ud2b8\uc911 \uc785\ub2c8\ub2e4, \ub9c8\uce68 \uc774\uacf3\uc774 \uc801\uc808\ud55c \ud658\uacbd\uc744 \uac00\uc9c0\uace0 \uc788\uc5b4\uc11c\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/hylotl/default/2", "value": "\uc7ac\ub300\ub85c \ub41c \uc548\uacbd \uc5c6\uc774 \uc5f0\uad6c\ub97c \ud558\ub2c8 \uc774\uc0c1\ud55c \ub290\ub08c\uc774 \ub4dc\ub294\uad70\uc694. \ubb50, \uadf8\uac8c \ud558\uc774\ub85c\ud2c0\uc758 \uc0b6\uc774\uc9c0\ub9cc\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/2", "value": "\uc0dd\ubb3c \ub2e4\uc591\uc131\uc5d0 \ub300\ud55c \uc5f0\uad6c \ubcf4\uace0\uc11c\ub97c \uc4f0\uace0\uc788\uc2b5\ub2c8\ub2e4. \ub9e4\uc6b0 \uc911\uc694\ud55c \ub140\uc11d\uc774\uc8e0."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/1", "value": "\ud658\uc0c1\uc801\uc73c\ub85c \ub9ce\uc740 \ub370\uc774\ud130\uac00 \uc788\uad70\uc694, \uc5b8\uc81c \ub2e4 \uae30\ub85d\ud560\uc9c0."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/apex/default/0", "value": "\uac71\uc815\ub9d0\uac8c, \ub0b4 \uc5f0\uad6c\ub294 \uac1c\uc778\uc6a9\uc774\uc57c, \uc9c0\uc815\ubd80\uc640\ub294 \uad00\ub828 \uc5c6\uc9c0."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/hylotl/default/1", "value": "\uc774 \uc548\uacbd\ub4e4\uc774 \uc7ac\ub300\ub85c \ub9de\uc558\uc73c\uba74 \uc88b\uc558\uc744\ud150\ub370."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/hylotl/default/0", "value": "\uc694\uc998 \uad1c\ucc2e\uc740 \ud558\uc774\ub85c\ud2c0\uc6a9 \uc548\uacbd\uc744 \ucc3e\uae30\uac00 \ucc38 \ud798\ub4e4\ub2e8 \ub9d0\uc774\uc9c0."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/0", "value": "\ucc38\uace0\ub85c \uc9c0\uae08 \ub300\ud654\ub294 \ub179\ud654\ud558\uace0 \uc788\uc2b5\ub2c8\ub2e4, \ub370\uc774\ud130\ub85c \ud544\uc694\ud558\uac70\ub4e0\uc694. \uc2e0\uacbd\uc4f0\uc9c0 \uc54a\uae38 \ubc14\ub78d\ub2c8\ub2e4."}]