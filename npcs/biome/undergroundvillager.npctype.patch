[{"op": "replace", "path": "/scriptConfig/dialog/converse/avian/default/2", "value": "\ud658\uc601\ud574\uc694! \uc774 \uacf3\uc5d0 \uc0b4\uae30\uc704\ud574 \ub0b4\ub824\uc624\uc2e0 \uac74\uac00\uc694? \uc81c\uac00 \uc774\uc6c3\uc774 \ub418\uba74 \uc88b\uaca0\ub124\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/apex/default/1", "value": "\uc5b4\ub460\uc740 \uc774\uacf3\uc5d0 \uc788\ub294 \uc5ec\ub7ec\uac00\uc9c0\ub97c \uc228\uae30\uc8e0. \ub2f9\uc2e0\uc740 \ubcf4\ubb3c \uc0ac\ub0e5\uafbc\uc778\uac00\uc694?"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/avian/default/1", "value": "\uc774 \ud070 \ub3d9\uad74\uc740 \uc57d\uac04 \uc704\ud5d8\ud560 \uc218 \uc788\uc5b4\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/avian/default/0", "value": "\ub2f9\uc2e0\uc744 \ubcf4\ub294 \uac83\ub9cc\uc73c\ub85c\ub3c4 \uc81c \uae43\ud138\uc744 \ud1b5\ud574 \ucda9\uaca9\uc774 \uc804\ud574\uc838 \uc624\ub294\uad70\uc694! \ub2e4\ub978 \uc0ac\ub78c\uc744 \ubcf4\ub294 \uacbd\uc6b0\ub294 \uac70\uc758 \uc5c6\uac70\ub4e0\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/apex/default/2", "value": "\uc774\uacf3\uc5d0 \uaf2d\uaf2d \uc228\uae30\uc704\ud574 \ub0b4\ub824\uc628\uac74\uac00, \ub0b4 \uce5c\uad6c\uc5ec?"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/apex/default/0", "value": "\ube45 \uc5d0\uc774\ud504\uc640 \uadf8\uc758 \uc9c0\uc815\ubd80 \uc870\ucc28 \uc5ec\uae30 \uc544\ub798\uc788\ub294 \ub0a0 \ucc3e\uc9c0 \ubabb\ud558\uaca0\uc9c0... \uadf8\uac8c \ub0b4 \ud76c\ub9dd\uc774\ub2e4."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/glitch/default/3", "value": "\uac10\uba85. \uc5b4\ucc0c \uadf8\ub9ac \uc6a9\ucf00\ub3c4 \uc5ec\uae30\uae4c\uc9c0 \ub0b4\ub824\uc628 \uac70\uc694? \uc790\ub124\ub294 \ubd84\uba85 \uc6a9\uac10\ud55c\uac00 \ubcf4\uad6c\ub9cc."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/hylotl/default/3", "value": "\ubc18\uac11\uc2b5\ub2c8\ub2e4, \uc774\ubc29\uc778. \ub2f9\uc2e0\uc740 \uc774\uacf3\uc5d0 \uac1c\uc778\uc801\uc778 \uae68\ub2ec\uc74c\uc744 \ucc3e\uc73c\ub7ec \uc624\uc2e0 \uac81\ub2c8\uae4c?"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/floran/default/2", "value": "\ud50c\ub85c\ub780 \uc5ec\uae30\uc11c \ub0af\uc158 \uc0e4\ub78c \ub9cc\ub098\uc154 \ub180\ub790\ub2e4! \ud50c\ub85c\ub780 \uac70\uc758 \ud63c\uc790\ub2e4..."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/floran/default/1", "value": "\ud50c\ub85c\ub780 \uc774 \uc544\ub798\uc5d0\uc11c \uc0e4\ub78c \uc798 \ubabb \ubcf8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/floran/default/0", "value": "\ud50c\ub85c\ub780 \uc9c0\ud558\uc5d0\uc11c \uc0e4\ub0e5\ud558\ub294 \uac8c \ub354 \uc88b\ub2e4. \uc298\uc5b4\ub2e4\ub2c8\uae30 \ub354 \uc88b\ub2e4."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/apex/default/3", "value": "\uc9c0\uc815\ubd80 \uc815\ucc30\ubcd1\ub4e4\uc774 \uc124\ub9c8 \uc774\ub7f0 \ub3d9\uad74\uae4c\uc9c0 \ub0b4\ub824\uc624\uaca0\uc5b4..."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/glitch/default/2", "value": "\ud658\uc601. \ubc18\uac11\ub124. \uc5ec\uae30\uc11c \uba38\ubb34\ub974\ub2e4 \uac08 \uc0dd\uac01\uc778\uac00?"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/glitch/default/1", "value": "\ucda9\uaca9. \uc790\ub124\uac00 \uadfc\ub798 \ub4e4\uc5b4\uc11c \ub0b4 \ucc98\uc74c \ubcf8 \uc0ac\ub78c\uc774\ub77c\ub124."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/glitch/default/0", "value": "\uacbd\ud0c4. \ub204\uac00 \uc774\ub9ac\ub85c \ub0b4\ub824\uc62c \uac70\ub780 \uc0dd\uac01\uc740 \ubabb \ud588\ub2e4\ub9cc."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/floran/default/3", "value": "\ub0af\uc158 \uc0e4\ub78c! \ud50c\ub85c\ub780 \ub204\uac00 \ub0b4\ub824\uc62c \uac70\ub77c\uace0 \uae30\ub300\ub3c4 \uc548 \ud558\uace0 \uc788\uc5c8\ub2e4!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/hylotl/default/2", "value": "\ud658\uc601\ud560 \ub9cc\ud55c \uc190\ub2d8\uc774\uc2dc\uad70\uc694! \uc774\uacf3\uc5d0\uc11c \ud55c\ub3d9\uc548 \ud63c\uc790 \uc0b4\uc544\ub0a8\uc558\ub2f5\ub2c8\ub2e4."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/hylotl/default/1", "value": "\ud658\uc601\ud569\ub2c8\ub2e4, \uc5ec\ud589\uc790\uc5ec! \ub2f9\uc2e0\uc740 \ubcf4\ubb3c\uc744 \ucc3e\uae30\uc704\ud574 \uc774 \ub3d9\uad74\ub85c \uc628\uac74\uac00\uc694?"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/hylotl/default/0", "value": "\uba85\uc0c1\uc774\ub77c\ub3c4 \ud558\ub7ec \uc624\uc2e0 \uac81\ub2c8\uae4c? \uc544\ub2c8\uba74 \uae38\uc744 \uc783\uc73c\uc168\uc2b5\ub2c8\uae4c?"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/0", "value": "\ub09c \uc9c0\ud558\uc5d0\uc11c \uc0b4\uc544\uc694! \ucd5c\uace0\uc758 \uc0dd\ud65c \uc591\uc2dd\uc774\uc8e0."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/3", "value": "\uc5ec\uae30 \ub0b4\ub824\uc628 \uc9c0 \uaf64 \ub410\ub294\ub370. \ubcf4\ud638\uad6d\uc740 \uc694\uc998 \uc5b4\ub5bb\ub300\uc694?"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/2", "value": "\ucc44\uad11\ud558\ub7ec \uc624\uc2e0 \uac74\uac00\uc694? \uc544\ub2c8\uba74 \uadf8 \uc218\uc0c1\ud55c \ubb38\uc744 \ucc3e\uc544\uc628 \uac83\uc774\uaca0\ub124\uc694..."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/1", "value": "\uc774\ubc29\uc778 \uc774\uc2dc\ub124\uc694! \uc800\ub294 \uc5ec\uae30\ub85c \ub0b4\ub824\uc628 \uc190\ub2d8\uc774 \uc775\uc219\uce58 \ubabb\ud574\uc11c\uc694. \ud3c9\ud654\uc801\uc73c\ub85c \uc624\uc2e0\uac74\uac00\uc694?"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/human/default/0", "value": "\ub9cc\uc57d \ub0b4\uac00 \uc774\uacf3 \uc544\ub798\uc5d0 \uc228\uc5b4\uc9c0\ub0b8\ub2e4\uba74, \ub0b4 \uc801\ub4e4\uc740 \ub0a0 \ubabb \ucc3e\uc744\ud14c\uc9c0..."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/avian/default/3", "value": "\ub2f9\uc2e0 \uac19\uc740 \uc5ec\ud589\uc790\ub4e4\uc774 \uac00\ub054 \uc774\uacf3\uc5d0 \ub4e4\uc5b4\uc640\uc694. \ub09c \uadf8\ub4e4\uc744 \ub2e4\uc2dc \ubcfc \uc218 \uc5c6\uc5c8\uc8e0..."}]