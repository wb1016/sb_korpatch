[{"op": "replace", "path": "/scriptConfig/dialog/converse/avian/default/0", "value": "\uae43\ud138\uc744 \ub118\uc5b4\uc11c \ub540\uc744 \ud758\ub9ac\uace0 \uc788\uc5b4!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/glitch/default/1", "value": "\uac71\uc815. \ub0b4\uac00 \ub540\uc744 \ud758\ub9ac\ub294 \uac83 \ucc98\ub7fc \ubcf4\uc774\ub098? \uc0ac\uc2e4 \uc99d\uae30\uac00 \uace0\uc778\uac70\ub77c\ub124... \uc9c4\uc9dc\uc57c!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/apex/default/0", "value": "\ube45 \uc5d0\uc774\ud504\ub77c\ub3c4 \ud314\uc528\ub984\uc5d0\uc11c \uc774\uae38 \uc815\ub3c4\ub85c \uac15\ud574\uc84c\ub2e4\uace0!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/avian/default/2", "value": "\uac70\ub300\ud55c \ub9dd\uce58\ub97c \ud718\ub450\ub974\ub294\uac83\ub3c4 \uadf8\ub807\uac8c \uc5b4\ub835\uc9c0\ub9cc\uc740 \uc54a\uc544."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/avian/default/1", "value": "\ubaa8\ud589\uc131\uc5d0\uc11c \ud0dc\uc5b4\ub0ac\uc744 \ub54c\ubd80\ud130 \uc804\uc0ac\ub85c \uc790\ub77c\ub0ac\uc9c0. \uc774\ub7f0 \uacf3\uc744 \uc9c0\ud0a4\ub294 \uac74 \uc2dd\uc740 \uc8fd \uba39\uae30\uc57c!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/apex/default/2", "value": "\uc5ec\uae38 \ub540 \ud758\ub9b4 \uc815\ub3c4\ub85c \uc9c0\ucf30\ub2e4\uace0! \ubcf4\uc5ec?"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/apex/default/1", "value": "\uacc4\uc18d \ubc14\uc058\uac8c \uc77c\ud558\ub2e4 \ubcf4\ub2c8 \ub540\uc774 \uba48\ucd94\uc9c8 \uc54a\uc544. \uc0c1\ub2f9\ud788 \ubd88\ucf8c\ud558\uad70."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/glitch/default/2", "value": "\uc9c0\ub8e8\ud568. \uc5ec\uae30\ub97c \uc9c0\ud0a4\uace0 \uc788\ub294 \uac74 \ucc38 \uc9c0\ub8e8\ud558\uad70. \ub204\uad70\uac00 \ub300\ub828 \uc0c1\ub300\ub77c\ub3c4 \ucc3e\uc544\uc57c \ud558\ub294 \uac74\uac00..."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/glitch/default/0", "value": "\uc6a9\uac10. \uc774 \ud589\uc131\uc758 \uc5b4\ub5a0\ud55c \uc704\ud5d8\uc774\ub77c\ub3c4 \ub9de\uc124 \uc218 \uc788\uc18c!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/2", "value": "\ub0b4 \ub9dd\uce58\ub294 \uc774\uacf3\uc758 \ud3c9\ud654\ub97c \uc9c0\ud0a4\uae30 \uc704\ud574 \uc788\uc9c0. \ub9dd\ub098\ub2c8\ucc98\ub7fc \ub2e4\ub2c8\uc9c0 \uc54a\uc73c\uba74 \uc774\uac78 \uc4f0\ub294 \uac78 \ubcfc\uc77c\uc740 \uc5c6\uc744 \uac70\uc57c."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/1", "value": "\uc5ec\uae30\uc11c \uc77c\ud558\ub294 \uac74 \ub098\uc5d0\uac8c \uac15\ucca0\uc758 \ud314\uc744 \uc92c\uc9c0! \ub098\ub294 \ud568\ubd80\ub85c \uc2dc\ube44 \uac78\uace0 \ub2e4\ub2d0 \uc0ac\ub78c\uc774 \uc544\ub2c8\ub77c\uace0."}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/0", "value": "\ub09c \uc774\uacf3\uc758 \ud3c9\ud654\ub97c \uc9c0\ud0a4\uae30 \uc704\ud574 \uc788\uc5b4. \ub3cc\uc544\ub2e4\ub2c8\ub294 \uac74 \uc790\uc720\uc9c0\ub9cc, \ubb38\uc81c\ub97c \uc77c\uc73c\ud0a4\uc9c4 \ub9d0\uc544\uc918."}]