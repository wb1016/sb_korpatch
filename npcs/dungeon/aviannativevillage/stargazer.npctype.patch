[{"op": "replace", "path": "/dialog/converse/avian/default/5", "value": "\uc6b0\ub9ac \uc544\ube44\uc548 \ub4e4\uc740 \uace7 \ucc9c\uad6d\uc5d0 \uc2b9\ucc9c\ud574 \uc6b0\ub9ac\ub4e4\uc758 \uc790\ub9ac\ub85c \ub3cc\uc544\uac08\uac83\uc774\ub2e4. \uc6b0\ub9ac\uac00 \uc0ac\ub77c\uc9c0\ub354\ub77c\ub3c4 \uc6b0\ub9ac\ub97c \uadf8\ub9ac\uc9c0 \ub9d0\ub3c4\ub85d, \uc774\ubc29\uc778\uc774\uc5ec."}, {"op": "replace", "path": "/dialog/converse/avian/default/4", "value": "\uc774\uacf3\uc758 \uc0ac\ub78c\uc740 \uc544\ub2c8\uad70. \ud074\ub8e8\uc5d1\uc2a4\uc758 \ubc95\uc744 \uc874\uc911\ud55c\ub2e4\uba74 \ub530\uc9c0\uc9c4 \uc54a\ub3c4\ub85d \ud558\uc9c0."}, {"op": "replace", "path": "/dialog/converse/avian/default/3", "value": "\uc9c4\uc815\ud55c \ud074\ub8e8\uc5d1\uc2a4\uc758 \uc2e0\ubd09\uc790\uac00 \uc544\ub2cc\uac83 \uac19\uad70. \uacf5\uc190\ud558\ub3c4\ub85d, \uadf8\ub7fc \ub110 \uacf5\uaca9\ud560 \uc77c\uc740 \uc5c6\uc744\ud14c\ub2c8."}, {"op": "replace", "path": "/dialog/converse/avian/default/2", "value": "\ub108\uc5d0\uac8c \uc788\ub294 \ubaa8\ub4e0 \uc88b\uc740\uc77c\uc740 \ud074\ub8e8\uc5d1\uc2a4\uc758 \ucd95\ubcf5\uc774\uc694, \ubaa8\ub4e0 \ub098\uc05c\uc77c\uc740 \ub108\uc758 \uc545\ub355\uc774\ub2c8."}, {"op": "replace", "path": "/dialog/converse/avian/default/1", "value": "\ub09c \ubcc4 \uad00\uce21\uc790, \ud074\ub8e8\uc5d1\uc2a4\uc758 \uc804\ub839! \ub0b4\uac00 \ub9d0\ud560\ub550 \uc6b0\ub9ac\uc758 \ub0a0\uac1c \ub2ec\ub9b0 \uc2e0\uc758 \ub9d0\uc744 \ub9d0\ud55c\ub2e4!"}, {"op": "replace", "path": "/dialog/converse/avian/default/0", "value": "\ud074\ub8e8\uc5d1\uc2a4\uc758 \ub9d0\uc744 \ub4e4\uc5b4\ub77c \uc774\ubc29\uc778\uc774\uc5ec! \uadf8\uc758 \ub0a0\uac1c\ub2ec\ub9b0 \ud798\uc5d0 \ub5a8\uc5b4\ub77c! \uadf8\uc758 \ub0a0\uce74\ub85c\uc6b4 \uc790\ube44\uc5d0 \ub180\ub77c\ub77c!"}, {"op": "replace", "path": "/dialog/converse/avian/avian/4", "value": "\uc6b0\ub9ac\uac00 \ud074\ub8e8\uc5d1\uc2a4\uc5d0\uac8c \uc798 \ud55c\ub2e4\uba74, \uc6b0\ub9ac\ub294 \ubaa8\ub450 \ucc9c\uad6d\uc5d0 \uc790\ub9ac\ub97c \uc5bb\uc744\uac83\uc774\ub2e4. \ub2e4\ub978 \uc885\uc871\ub4e4\uc5d0\uac90 \uc548\ub42c\uc9c0\ub9cc."}, {"op": "replace", "path": "/dialog/converse/avian/avian/3", "value": "\ubcc4\ub4e4 \ub118\uc5b4\uc5d0\uc11c \uc628 \uc544\ube44\uc548\uc778\uac00! \ub0a0\uac1c\ub97c \uc5bb\uae30 \uc704\ud574 \ud074\ub8e8\uc5d1\uc2a4\uc758 \uc704\ub300\ud568\uc744 \uc124\ud30c\ud558\uace0 \ub2e4\ub154\ub294\uac00? \ub9d0\ud574\ub2e4\uc624."}, {"op": "replace", "path": "/dialog/converse/avian/avian/2", "value": "\ub108\uc5d0\uac8c \uc788\ub294 \ubaa8\ub4e0 \uc88b\uc740\uc77c\uc740 \ud074\ub8e8\uc5d1\uc2a4\uc758 \ucd95\ubcf5\uc774\uc694... \ub294 \uc774\ubbf8 \uc544\ube44\uc548\uc774\ub2c8 \uc54c\uace0 \uc788\uaca0\uc9c0."}, {"op": "replace", "path": "/dialog/converse/avian/avian/1", "value": "\ub09c \ubcc4 \uad00\uce21\uc790, \ud074\ub8e8\uc5d1\uc2a4\uc758 \uc804\ub839!... \uc774\ub77c\uace4 \ud588\uc9c0\ub9cc \uc774\ubbf8 \uadf8 \ub208\uc744 \ubd10\uc120 \ub0b4\uac00 \ub204\uad70\uc9c0 \uc54c\uace0\uc788\uc5c8\ub098\ubcf4\uad70!"}, {"op": "replace", "path": "/dialog/converse/avian/avian/0", "value": "\ud074\ub8e8\uc5d1\uc2a4\uc758 \ub9d0\uc744 \ub4e4\uc5b4\ub77c \uc544\ube44\uc548\uc774\uc5ec! \uadf8\uc758 \ub0a0\uac1c\ub2ec\ub9b0 \ud798\uc5d0 \ub5a8\uc5b4\ub77c! \uadf8\uc758 \ub0a0\uce74\ub85c\uc6b4 \uc790\ube44\uc5d0 \ub180\ub77c\ub77c!"}]