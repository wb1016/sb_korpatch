[{"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/default/default/0", "value": "\uc228\uc5b4\uc788\uc5b4\uc57c\ud574! \uc81c\ubc1c \ub0a0 \ub118\uae30\uc9c0 \ub9d0\uc544\uc918, \ub300\uc2e0 \ub108\uc758 \uc2b9\ubb34\uc6d0\uc774 \ub420 \uc218 \uc788\uc744\uae4c?"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/apex/default/6", "value": "\uc228\uc5b4\uc788\uc744 \uacf3\uc774 \uc815\ub9d0 \ud544\uc694\ud55c\ub370... \uadf8\ub7ec\uace0 \ubcf4\ub2c8 \ub2f9\uc2e0, \ud568\uc120\uc774 \uc788\ub098\uc694? \uac70\ub798\ub97c \uc81c\uc2dc\ud558\uace0 \uc2f6\uc740\ub370..."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/floran/default/2", "value": "\ud50c\ub85c\ub780\uc740 \ud53c\ud558\uace0 \uc788\ub294 \uc0e4\ub78c\ub4e4\uc774 \uc788\uc5b4! \ud50c\ub85c\ub780\uc744 \ub3c4\uc640\uc904 \uc288 \uc788\uc5b4? \uc608\ub97c \ub4e4\uc5b4\uc11c \ud50c\ub85c\ub780\uc744 \ub108\uc758 \ud568\uc158\uc5d0 \ucc59\uaca8 \ub123\ub294\ub2e4\ub358\uac00."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/apex/default/5", "value": "\uac70\ub798\ud558\uc790, \ub108\uc758 \ud568\uc120\uc5d0 \ub0a0 \uc228\uaca8\uc8fc\uba74 \ub110 \uc704\ud574 \uc2f8\uc6cc\uc8fc\uc9c0. \ub0b4\uac00 \ud53c\ud574\uc57c \ud558\ub294 \uc0ac\ub78c\ub4e4\uc774 \uc788\uc5b4\uc11c..."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/floran/default/0", "value": "\uc774 \ud568\uc158\uc740 \ud50c\ub85c\ub780\uc774 \uc298\uae30 \ub531 \uc88b\uc544! \ud50c\ub85c\ub780\uc740 \uc5ec\uae30 \uc788\uc744 \ud14c\ub2c8 \ud544\uc694\ud558\uba74 \uc5b8\uc81c\ub4e0 \ubd88\ub7ec\uc918!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/floran/default/1", "value": "\ud50c\ub85c\ub780\uc744 \ub3c4\uc640\uc918\uc154 \uace0\ub9c8\uc6cc. \ud50c\ub85c\ub780\uc740 \ub108\uc758 \uc801\uacfc \uc5b8\uc81c\ub4e0 \uc330\uc6cc\uc904\uac8c!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/floran/default/2", "value": "\ud50c\ub85c\ub780\uc740 \uc9c4\uc9dc \ub098\uc058\uc9c0 \uc54a\ub2e4\ub2c8\uae4c. \uc774 \ud568\uc158\uc740 \uc298\uae30 \ub531 \uc88b\uc740\uac78. \uc330\uc6c0\uc5d0 \ub3c4\uc6c0\uc774 \ud544\uc694\ud558\uba74 \ud50c\ub85c\ub780\uc744 \ubd88\ub7ec\uc918!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/floran/default/3", "value": "\ud50c\ub85c\ub780\uc740 \uc0e4\ub78c\ub4e4\uc774 \ud50c\ub85c\ub780\uc774 \ud55c \uc77c\uc744 \uae4c\uba39\uc744 \ub54c \uae4c\uc9c0 \uc5ec\uae30\uc5d0 \uc298\uc744\uc288 \uc788\uc744 \uac83 \uac19\uc544. \uace0\ub9c8\uc6cc! \ub3c4\uc6c0\uc774 \ud544\uc694\ud558\ub2e4\uba74 \ud50c\ub85c\ub780\uc744 \ubd88\ub7ec\uc918."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/hylotl/default/0", "value": "\uc228\uac8c \ub3c4\uc640\uc8fc\uc154\uc11c \uac10\uc0ac\ud569\ub2c8\ub2e4! \ubb54\uac00 \ud798\uc4f8 \uacf3\uc774 \uc0dd\uae30\uba74 \uc81c\uac00 \uc5ec\uae30 \uc788\ub2e4\ub294\uac78 \uc78a\uc9c0 \ub9c8\uc138\uc694!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/hylotl/default/1", "value": "\uc774\uacf3\uc774\ub77c\uba74 \uc870\uc6a9\ud788 \uc788\uc744 \uc218 \uc788\uaca0\uad70\uc694. \uace0\ub9d8\uad70\uc694. \uc9c0\uc6d0\uc774 \ud544\uc694\ud558\uc2dc\ub2e4\uba74 \uc5b8\uc81c\ub098 \ub3c4\uc640\ub4dc\ub9ac\uc8e0."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/hylotl/default/2", "value": "\uadf8\ub807\uac8c \ub098\uc058\uc9c4 \uc54a\uc744\uac81\ub2c8\ub2e4. \uc774 \ud568\uc120\uc740 \uc870\uc6a9\ud788 \uc788\uae30 \ub531 \uc88b\uad70\uc694. \ub2f9\uc2e0\uc774 \ub4a4\ub97c \ubd10\uc904 \uc0ac\ub78c\uc774 \ud544\uc694\ud558\ub2e4\uba74 \uc800\ub97c \ubd88\ub7ec\uc8fc\uc138\uc694!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/hylotl/default/3", "value": "\uac00\ub77c\uc549\uc744 \ub54c \uae4c\uc9c0 \uc5ec\uae30\uc11c \uc228\uc744 \uc218 \uc788\uaca0\uad70\uc694! \ubaa8\ub450 \ub2f9\uc2e0 \ub355\ubd84\uc785\ub2c8\ub2e4, \ub3c4\uc6c0\uc774\ub098 \uce7c\uc774 \ud55c\uc790\ub8e8 \ub354 \ud544\uc694\ud558\ub2e4\uba74 \uc778\uc0ac\ud558\ub7ec \uc640\uc8fc\uc138\uc694!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/glitch/default/0", "value": "\uac10\uc0ac. \uc5ec\uae30\uc5d0 \ub0a8\uc544\uc788\ub3c4\ub85d \ud5c8\ub77d\ud574\uc918\uc11c \uace0\ub9d9\uad70. \ud544\uc694\ud558\ub2e4\uba74 \uc5b8\uc81c\ub4e0 \ucc38\uc804\ud574\uc8fc\uc9c0, \uac70\ub798\ub294 \uae4c\uba39\uc9c0 \uc54a\uc558\ub124."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/glitch/default/1", "value": "\uace0\ub9c8\uc6c0. \uc774 \ud568\uc120\uc740 \uc870\uc6a9\ud788 \uc788\uae30 \uc815\ub9d0 \uc88b\uad70. \uc9c0\uc6d0\uc774 \ud544\uc694\ud558\ub2e4\uba74 \ub09c \uc5ec\uae30\uc788\uc744\uaec4\uc138."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/glitch/default/2", "value": "\uac10\uc0ac. \uc228\uac8c \ub3c4\uc640\uc918\uc11c \uace0\ub9d9\uc18c! \uc790\ub124\uac00 \ud544\uc694\ud560\ub54c \ub9c8\ub2e4 \ub098\uac00\uc11c \uc2f8\uc6cc\uc8fc\ub3c4\ub85d \ud558\uc9c0."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/glitch/default/3", "value": "\uace0\ub9c8\uc6c0. \uc0c1\ud669 \uac00\ub77c\uc549\uc744 \ub54c \uae4c\uc9c0 \uc5ec\uae30\uc11c \uc228\uc744 \uc218 \uc788\uaca0\uad70. \ubaa8\ub450 \uc790\ub124 \ub355\ubd84\uc774\ub124, \uc9c0\uc6d0\uc774 \ud544\uc694\ud558\ub2e4\uba74 \ub0a0 \ucc3e\uc544\uc8fc\uac8c."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/default/default/0", "value": "\uc228\uac8c \ub3c4\uc640\uc8fc\uc154\uc11c \uac10\uc0ac\ud569\ub2c8\ub2e4! \ubb54\uac00 \ud798\uc4f8 \uacf3\uc774 \uc0dd\uae30\uba74 \uc81c\uac00 \uc5ec\uae30 \uc788\ub2e4\ub294\uac78 \uc78a\uc9c0 \ub9c8\uc138\uc694!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/default/default/1", "value": "\uc774\uacf3\uc774\ub77c\uba74 \uc870\uc6a9\ud788 \uc788\uc744 \uc218 \uc788\uaca0\uad70\uc694. \uace0\ub9d9\uad70\uc694! \uc9c0\uc6d0\uc774 \ud544\uc694\ud558\uc2dc\ub2e4\uba74 \uc5b8\uc81c\ub098 \ub3c4\uc640\ub4dc\ub9ac\uc8e0."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/default/default/2", "value": "\uc81c\uac00 \uadf8\ub807\uac8c \ub098\uc058\uc9c4 \uc54a\uc544\uc694. \uc774 \ud568\uc120\uc740 \uc870\uc6a9\ud788 \uc788\uae30 \ub531 \uc88b\uad70\uc694. \ub2f9\uc2e0\uc774 \ub3c4\uc640\uc904 \uc0ac\ub78c\uc774 \ud544\uc694\ud558\ub2e4\uba74 \uc800\ub97c \ubd88\ub7ec\uc8fc\uc138\uc694!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/default/default/3", "value": "\uac00\ub77c\uc549\uc744 \ub54c \uae4c\uc9c0 \uc5ec\uae30\uc11c \uc228\uc744 \uc218 \uc788\uaca0\uad70\uc694. \ubaa8\ub450 \ub2f9\uc2e0 \ub355\ubd84\uc785\ub2c8\ub2e4, \ub3c4\uc640\uc904 \uc0ac\ub78c\uc774\ub098, \uce7c\uc774 \ud55c\uc790\ub8e8 \ub354 \ud544\uc694\ud558\ub2e4\uba74 \uc778\uc0ac\ud558\ub7ec \uc640\uc8fc\uc138\uc694!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/apex/default/0", "value": "\uc228\uac8c \ub3c4\uc640\uc8fc\uc154\uc11c \uac10\uc0ac\ud569\ub2c8\ub2e4! \ubb54\uac00 \ub354 \ud798\uc744 \uc4f8\uacf3\uc774 \uc0dd\uae30\uba74 \uc81c\uac00 \uc5ec\uae30 \uc788\ub2e4\ub294\uac78 \uc78a\uc9c0 \ub9c8\uc138\uc694!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/apex/default/1", "value": "\uc774\uacf3\uc774\ub77c\uba74 \uc870\uc6a9\ud788 \uc788\uc744 \uc218 \uc788\uaca0\uad70\uc694. \uace0\ub9d9\uc2b5\ub2c8\ub2e4! \uc6d0\uad70\uc774 \ud544\uc694\ud558\uc2dc\ub2e4\uba74 \uc5b8\uc81c\ub098 \ub3c4\uc640\ub4dc\ub9ac\uc8e0."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/apex/default/2", "value": "\uc81c\uac00 \uadf8\ub807\uac8c \ub098\uc058\uc9c4 \uc54a\uc544\uc694. \uc774 \ud568\uc120\uc740 \uc870\uc6a9\ud788 \uc788\uae30 \ub531 \uc88b\uad70\uc694. \ub2f9\uc2e0\uc774 \ub3c4\uc6c0\uc774 \ud544\uc694\ud558\ub2e4\uba74 \uc800\ub97c \ubd88\ub7ec\uc8fc\uc138\uc694!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/apex/default/3", "value": "\uac00\ub77c\uc549\uc744 \ub54c \uae4c\uc9c0 \uc5ec\uae30\uc11c \uc228\uc744 \uc218 \uc788\uaca0\uad70\uc694. \ubaa8\ub450 \ub2f9\uc2e0 \ub355\ubd84\uc785\ub2c8\ub2e4, \ucd94\uac00 \ud654\ub825\uc774 \ud544\uc694\ud558\ub2e4\uba74 \uc808 \ucc3e\uc544\uc8fc\uc138\uc694."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/avian/default/0", "value": "\uc228\uac8c \ub3c4\uc640\uc8fc\uc154\uc11c \uac10\uc0ac\ud569\ub2c8\ub2e4! \ubb54\uac00 \ud798\uc4f8 \uacf3\uc774 \uc0dd\uae30\uba74 \uc81c\uac00 \uc5ec\uae30 \uc788\ub2e4\ub294\uac78 \uc78a\uc9c0 \ub9c8\uc138\uc694!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/avian/default/1", "value": "\uc774\uacf3\uc774\ub77c\uba74 \uc870\uc6a9\ud788 \uc788\uc744 \uc218 \uc788\uaca0\uad70\uc694. \uace0\ub9d9\uc2b5\ub2c8\ub2e4! \uc9c0\uc6d0\uc774 \ud544\uc694\ud558\uc2dc\ub2e4\uba74 \uc5b8\uc81c\ub098 \ub3c4\uc640\ub4dc\ub9ac\uc8e0."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/roleDescription/avian/default/2", "value": "\uc81c\uac00 \uadf8\ub807\uac8c \ub098\uc058\uc9c4 \uc54a\uc544\uc694. \uc774 \ud568\uc120\uc740 \uc870\uc6a9\ud788 \uc788\uae30 \ub531 \uc88b\uad70\uc694. \ub2f9\uc2e0\uc774 \ub3c4\uc6c0\uc774 \ud544\uc694\ud558\ub2e4\uba74 \uc800\ub97c \ubd88\ub7ec\uc8fc\uc138\uc694!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/apex/default/4", "value": "\uc870\uc6a9\ud788 \uc228\uace0 \uc2f6\uc740\ub370... \ub2f9\uc2e0\uc758 \ud568\uc120\uc5d0 \uc2b9\uc120\ud558\ub294 \uac78 \ud5c8\ub77d\ud574\uc900\ub2e4\uba74 \ub2f9\uc2e0\uc744 \uc704\ud574 \uc2f8\uc6cc\ub4dc\ub9ac\uc8e0."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/floran/default/0", "value": "\ud50c\ub85c\ub780 \uc88b\uc740 \uc0e4\ub78c\uc774\ub77c\uad6c! \uadf8\ub0e5 \ud50c\ub85c\ub780\uc740 \uc870\uc6a9\ud788\ub9cc \uc788\uc73c\uba74 \ub3fc. \ub3c4\uc640\uc8fc\uba74 \uc548\ub420\uae4c?"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/floran/default/1", "value": "\ud568\uc158\uc774 \uc788\uc5b4? \ud50c\ub85c\ub780\uc774 \uc298\uc5b4\uc57c\ud560 \uc0e4\ub78c\ub4e4\uc774 \uc788\uc5b4. \ud50c\ub85c\ub780\uc744 \ub3c4\uc640\uc8fc\uaca0\uc5b4?"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/floran/default/3", "value": "\ud50c\ub85c\ub780\uc740 \uc298\uc5b4\uc57c\ud574. \ub108\uc758 \ud568\uc158\uc5d0 \uc790\ub9ac\ub97c \ub9c8\ub828\ud574\uc904\uc288 \uc788\uc73c\uba74 \ud50c\ub85c\ub780\uc774 \ub110 \uc704\ud574 \uc2f8\uc6b8\uac8c!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/floran/default/4", "value": "\ud50c\ub85c\ub780\uc774 \uc81c\uc2dc\ud560 \uac8c \uc788\uc5b4. \ud50c\ub85c\ub780\uc744 \ub108\uc758 \ud568\uc120\uc5d0 \uc298\uaca8\uc8fc\uba74, \ud50c\ub85c\ub780\uc758 \ud798\uc744 \ub108\uc5d0\uac8c \uc81c\uacf5\ud560\uac8c. \ud50c\ub85c\ub780\uc740 \uc5ec\uae30\uc5d0 \uc788\uc73c\uba74 \uc548 \ub3fc..."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/floran/default/5", "value": "\ud50c\ub85c\ub780\uc740 \uc5b4\ub518\uac00\uc5d0 \uc298\uc5b4\uc57c\ud574. \ud568\uc158\uc774 \uc788\uc5b4? \ud50c\ub85c\ub780\uc774 \uc81c\uc758\uac00 \uc788\uc5b4..."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/floran/default/6", "value": "\ud50c\ub85c\ub780\uc740 \uc870\uc6a9\ud788 \uc788\uc5b4\uc57c\ud574! \ud50c\ub85c\ub780\uc744 \ub118\uae30\uc9c0 \ub9d0\uc544\uc918... \ud50c\ub85c\ub780\uc774 \ub110 \uc704\ud574 \uc77c\ud574\uc904\uac8c."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/hylotl/default/0", "value": "\uadf8\ub0e5 \uc870\uc6a9\ud788 \uc788\uace0 \uc2f6\uc2b5\ub2c8\ub2e4. \uccb4\ud3ec\ud558\uc9c0 \ub9d0\uc544\uc8fc\uc138\uc694! \ub300\uc2e0 \ub2f9\uc2e0\uacfc \ud568\uaed8 \uac00\uba74 \uc548 \ub420\uae4c\uc694?"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/hylotl/default/1", "value": "\ub09c \uc88b\uc740 \uc0ac\ub78c\uc774\ub77c\ub2c8\uae4c! \uc81c\ubc1c \uc2b9\uc120\uc744 \ud5c8\uc6a9\ud574\uc918..."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/hylotl/default/2", "value": "\ub098\ub97c\u2026 \ucc3e\ub294 \uc0ac\ub78c\ub4e4\uc774 \uc788\ub294\ub370... \ub2f9\uc2e0\uc758 \ud568\uc120\uc5d0 \uc228\ub294 \uac78 \ud5c8\ub77d\ud574\uc8fc\uba74 \ub2f9\uc2e0\uc744 \uc704\ud574 \uc77c\ud574\ub4dc\ub9ac\uc8e0!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/hylotl/default/3", "value": "\ub2f9\uc2e0\uc744 \ub2e4\uce58\uac8c \ud558\ub824\ub294\uac8c \uc544\ub2d9\ub2c8\ub2e4, \uadf8\ub0e5 \uc81c\uac00 \uc228\ub294\uac78 \ub3c4\uc640\uc8fc\uc2e4 \uc218 \uc5c6\ub098\uc694?"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/hylotl/default/4", "value": "\uc228\uc5b4\uc57c\ud558\ub294\ub370... \ub2f9\uc2e0\uc758 \ud568\uc120\uc5d0 \uc788\ub3c4\ub85d \ud574\uc8fc\uc2e0\ub2e4\uba74 \ub2f9\uc2e0\uc744 \uc704\ud574 \uc2f8\uc6cc\ub4dc\ub9ac\uc9c0\uc694!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/hylotl/default/5", "value": "\ub2f9\uc2e0\uc744 \uc704\ud574 \uc2f8\uc6b0\ub294 \uc870\uac74\uc73c\ub85c \ub2f9\uc2e0\uc758 \ud568\uc120\uc5d0 \uc228\uaca8\uc8fc\uc2e4 \uc218 \uc788\uc744\uae4c\uc694? \uc5b4\ub5a4 \uc77c\uc774 \uc788\uc5b4\ub3c4 \ud53c\ud558\uace0 \uc2f6\uc740 \uc0ac\ub78c\ub4e4\uc774 \uc788\uc2b5\ub2c8\ub2e4..."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/hylotl/default/6", "value": "\uc5b4\ub518\uac00\uc5d0 \uae09\ud788 \uc228\uc5b4\uc57c \ud558\ub294\ub370. \uadf8\ub798, \ub2f9\uc2e0\uc740 \ud568\uc120\uc774 \uc788\ub098\uc694? \uadf8\ub807\ub2e4\uba74 \uc774\ub7f0 \uac74 \uc5b4\ub5bb\uc2b5\ub2c8\uae4c..."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/glitch/default/0", "value": "\ud544\uc0ac\uc801. \uadf8\ub0e5 \uc870\uc6a9\ud788 \uc788\