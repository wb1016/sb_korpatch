ialog/crewmember/offer/glitch/default/1", "value": "\uc808\ub9dd. \ubcc4\uc5d0\uc11c \uc0dd\ud65c\uc740 \ub098\ub97c \uc704\ud558\ub294\uac70\uc694. \uc81c\ubc1c, \ub0b4\uac00 \uc120\uc6d0\uc774 \ub420 \uc218 \uc788\uaca0\uc18c?"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/glitch/default/2", "value": "\uc5f4\ub9dd. \ub09c \ub9e4\uc6b0 \uc4f8\ubaa8\uc788\ub294 \uc0ac\ub78c\uc774\uc624! \ub0b4\uac00 \ub2f9\uc2e0\uacfc \uac19\uc774 \ub2e4\ub140\ub3c4 \ub418\uaca0\uc18c?"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/default/default/0", "value": "\uc548\ub155 \ubaa8\ud5d8\uac00,  \ub10c \ub098\uc640 \ud568\uaed8 \ub2e4\ub2d0 \ub9c8\uc74c\uc774 \uc788\uc5b4?"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/apex/default/0", "value": "\ub2f9\uc2e0\ub124 \uc120\uc6d0\uc774 \ub418\uba74 \ub0a0 \ube45 \uc5d0\uc774\ud504\uc5d0\uac8c\uc11c \ubcf4\ud638\ud574\uc904 \uc218 \uc788\uc5b4?"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/avian/default/0", "value": "\ubcc4\uacfc \ubcc4 \uc0ac\uc774\ub97c \ud56d\ud574\ud55c\ub2e4... \ud639\uc2dc \ub108 \uc120\uc6d0\uc744 \ucc3e\uace0\uc788\ub2c8?"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/avian/default/1", "value": "\ub108 \ud568\uc120\uc774 \uc788\ub2c8? \ud639\uc2dc \ub108 \uc120\uc6d0\uc774 \ub418\uae30 \uc704\ud55c \uc0ac\ub78c\uc744 \ucc3e\uace0 \uc788\ub2c8?"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/offer/avian/default/2", "value": "\ub9d0\ud574\uc918, \uc120\uc6d0\uc744 \ucc3e\uace0\uc788\ub2c8? \uc65c\ub0d0\ud558\uba74 \ubcc4\uacfc \ubcc4 \uc0ac\uc774\ub97c \uc5ec\ud589\ud558\ub294 \uac83\uc740 \uba4b\uc9c0\uae30 \ub54c\ubb38\uc774\uc57c..."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/cantreach/default/default/0", "value": "\ub098 \ub108\ud55c\ud14c\uae4c\uc9c0 \uc548 \ub2ff\uc544!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/cantreach/default/default/1", "value": "\ub