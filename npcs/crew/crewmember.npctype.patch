itch/default/4", "value": "\ud53c\uace4. \ubd80\ud65c\uc740 \uc18c\ubaa8\uc801\uc774\uc624."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/follow/default/default/0", "value": "\uc54c\uaca0\uc2b5\ub2c8\ub2e4. \uc120\uc7a5!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/follow/default/default/1", "value": "\uc120\uc7a5!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/follow/default/default/2", "value": "\uac00\uc790!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/follow/default/default/3", "value": "\uac11\uc2dc\ub2e4. \uc120\uc7a5!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/follow/default/default/4", "value": "\uc6b0\ub9ac \uc5b4\ub514 \uac08\uac70\uc57c?"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/follow/floran/default/0", "value": "\uc54c\uaca0\uc5b4, \uc158\uc7a5!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/follow/floran/default/1", "value": "\uac00\uc790!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/follow/floran/default/3", "value": "\ud50c\ub85c\ub780 \ub2e4\uc2dc \uac08 \uc900\ube44\uac00 \ub410\uc5b4!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/follow/floran/default/4", "value": "\uc6b0\ub9ac \uc5b4\ub514 \uac08\uac70\uc57c?"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/follow/penguin/default/1", "value": "\uadf8\ub7fc \uc774\uc81c \uac00\uc790."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/follow/penguin/default/2", "value": "\ub124\uac00 \ub450\ubaa9\uc774\uc57c!"}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/follow/penguin/default/3", "value": "\ub09c \ub3cc\uc544\uc628\ub2e4."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/follow/glitch/default/0", "value": "\uacb8\uc190. \ub098\ub294 \ub530\ub974\uaca0\uc18c. \uc120\uc7a5."}, {"op": "replace", "path": "/scriptConfig/dialog/crewmember/follow/glitch/default/1", "value": "\uacbd\uacc4. \uc6b0\ub9ac \u