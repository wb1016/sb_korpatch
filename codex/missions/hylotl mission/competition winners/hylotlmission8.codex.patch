 {
  "op": "replace", 
  "path": "/description", 
  "value": "\uBBF8\uCE74\uC5D8 \"KDraconis\" \uD0B4\uC758 \uAE30\uC774\uD55C \uC774\uC57C\uAE30\uC774\uB2E4."
 }, 
 {
  "op": "replace", 
  "path": "/contentPages/0", 
  "value": "\uBAA8\uB4E0 \uBAA8\uD5D8\uC5D4 \uAD34\uBB3C\uC774 \uC788\uC5B4\uC57C \uD55C\uB2E4\uB294 \uBD88\uBB38\uC728\uC774 \uC788\uC2B5\uB2C8\uB2E4.

\uADF8\uAC8C \uC6A9\uC774\uB4E0, \uC810\uC561 \uAD34\uBB3C \uAC19\uC740 \uAC83\uC774\uB4E0, \uC544\uB2C8\uBA74 \uD0AC\uB7EC \uC6B0\uC8FC \uACE0\uB798\uC774\uB4E0, \uADF8\uB7F0 \uC0DD\uBA85\uCCB4\uB4E4\uC740 \uBAA8\uB4E0 \uC57C\uC2EC \uAC00\uB4DD\uD55C \uC601\uC6C5\uC5D0\uAC90 \uAF2D \uD544\uC694\uD55C \uC0AC\uB0E5\uAC10\uC785\uB2C8\uB2E4. 

\uADF8\uB7F0 \uB9CC\uD07C, \uB2E4\uC591\uD55C \uC0AC\uB78C\uB4E4\uC774 \uBCC4\uB4E4\uC758 \uBC14\uB2E4\uB97C \uB5A0\uB3CC\uC544\uB2E4\uB2C8\uB294 \uB180\uB77C\uC6B4 \uB3D9\uBB3C\uB4E4\uC758 \uC774\uC57C\uAE30\uB97C \uC8FC\uACE0\uBC1B\uC544 \uC628 \uAC83\uC740 \uC804\uD600 \uB180\uB78D\uC9C0 \uC54A\uC2B5\uB2C8\uB2E4. \uBC14\uB85C \uC0AC\uB78C\uC744 \uB2F9\uD669\uD558\uAC8C \uB9CC\uB4DC\uB294 \uC6B0\uC8FC \uBA3C\uC9C0\uD565\uAE30\uC785\uB2C8\uB2E4."
 },
 {
  "op": "replace", 
  "path": "/contentPages/1", 
  "value": "\uD750\uBB3C\uAC70\uB9AC\uACE0 \uC9C8\uCC99\uAC70\uB9AC\uB294 \uBAB8\uACFC \uD53C\uBD80\uC5D0\uC11C \uB9C8\uAD6C\uC7A1\uC774\uB85C \uB3CB\uC544\uB098\uB294 \uB2E4\uC591\uD55C \uD600\uB97C \uAC00\uC9C4\uB2E4\uACE0 \uC77C\uCEEC\uC5B4\uC9C0\uBA70, \uCD5C\uADFC \uBA87\uBA87 \uBCD1\uB825 \uC218\uC1A1\uC120\uC758 \uC2E4\uC885 \uC6D0\uC778\uC785\uB2C8\uB2E4. \uC544\uB9C8\uB3C4 \uC774 \uC0DD\uBB3C\uC740 \uD568\uC120\uC5D0 \uB2EC\uB77C\uBD99\uC5B4\uC11C, \uBCBD\uC744 \uBD80\uC218\uC5B4 \uC5F0 \uB2E4\uC74C, \uADF8\uAC83\uC774 \uBB34\uC5C7\uC744 \uC758\uBBF8\uD558\uB4E0\uC9C0, \uC548\uCABD\uC5D0 \uC788\uB294 \uC0AC\uB78C\uB4E4\uC744 \"\uD565\uC544\"\uBC84\uB9BD\uB2C8\uB2E4.

\uC774\uB7F0 \uC774\uC57C\uAE30\uB4E4\uC744 \uC804\uD588\uB358 \uC0AC\uB78C\uB4E4\uC740 \uC6B0\uC8FC \uBA3C\uC9C0\uD565\uAE30\uC758 \uD600\uB294 \uC0AC\uC2E4 \uD76C\uC0DD\uC790\uC758 \uD600\uC774\uBA70, \uB4A4\uD2C0\uB9AC\uACE0 \uC6B0\uC2A4\uAF5D\uC2A4\uB7EC\uC6B4 \uC804\uB9AC\uD488\uC73C\uB85C\uC368 \uAC00\uC838\uAC14\uB2E4\uACE0 \uC8FC\uC7A5\uD569\uB2C8\uB2E4."
 },
 {
  "op": "replace", 
  "path": "/contentPages/2", 
  "value": "\uD55C \uC6B0\uC8FC \uC5EC\uD589\uC790\uB294 \uC790\uC2E0\uC774 \uC6B0\uC8FC \uBA3C\uC9C0\uD565\uAE30\uB97C \uBCF4\uC558\uB2E4\uACE0 \uB9D0\uD588\uC2B5\uB2C8\uB2E4. \uD558\uC9C0\uB9CC, \uC77C\uBC18\uC801\uC778 \uC0DD\uAC01\uACFC\uB294 \uB2E4\uB974\uAC8C, \uC6B0\uC8FC \uBA3C\uC9C0\uD565\uAE30\uB294 \uC804\uD600 \uC801\uB300\uC801\uC774\uC9C0 \uC54A\uC558\uB2E4\uACE0 \uD569\uB2C8\uB2E4.

\uC6B0\uC8FC \uBA3C\uC9C0\uD565\uAE30\uB294 \uB354\uB7EC\uC6B4 \uC6B0\uC8FC\uC120 \uC704\uB85C \uB465\uB465 \uB5A0\uC654\uACE0, \uC6B0\uC8FC\uC120\uC744 \uAE68\uB057\uD558\uAC8C \uD565\uC740 \uB2E4\uC74C, \uD754\uC801 \uD558\uB098 \uC5C6\uC774 \uB5A0\uB0AC\uB2E4\uACE0 \uD569\uB2C8\uB2E4.

\uADF8 \uC6B0\uC8FC \uC5EC\uD589\uC790\uB294 \uC6B0\uC8FC \uBA3C\uC9C0\uD565\uAE30\uAC00 \"\uC785\uC73C\uB85C \uB0B4\uB294 \uBC29\uADC0 \uC18C\uB9AC\"\uC640 \uBE44\uC2B7\uD55C \uC18C\uB9AC\uB97C \uB0C8\uB2E4\uACE0 \uC8FC\uC7A5\uD588\uC9C0\uB9CC, \uBAA8\uB450\uAC00 \uC54C \uB4EF \uC6B0\uC8FC\uC5D0\uC11C\uB294, \uB204\uAD6C\uB3C4 \uB2F9\uC2E0\uC774 \"\uD515\u3142\u3142\uBE0C\uBE1F\" \uC18C\uB9AC\uB97C \uB0B4\uB294 \uAC78 \uB4E4\uC744 \uC218\uAC00 \uC5C6\uC2B5\uB2C8\uB2E4."
 }
